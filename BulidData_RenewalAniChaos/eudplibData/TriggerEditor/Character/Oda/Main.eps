import variable as v;
import func.trig as trg;

import Character.Oda.Skill_A as A;
import Character.Oda.Skill_S as S;
import Character.Oda.Skill_C as C;
import Character.Oda.Skill_O as O;

import Character.Oda.Skill_SSS as SSS;
import Character.Oda.Skill_SSSCC as SSSCC;
import Character.Oda.Skill_SSSCCAA as SSSCCAA;

import Character.Oda.Skill_CC as CC;
import Character.Oda.Skill_CCA as CCA;
import Character.Oda.Skill_CCAAA as CCAAA;
import Character.Oda.Skill_CCAAAAA as CCAAAAA;

import Character.Oda.Skill_AASS as AASS;

import Character.Oda.Skill_AAAA as AAAA;
import Character.Oda.Skill_AAAAAA as AAAAAA;
import Character.Oda.Skill_AO as AO;

import Character.Oda.Text as text;
import Character.Oda.Commend as commend;

var loop  = 0;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 193;
	v.P_UnitID[playerID] = 99;
	
	v.P_Ultimate1[playerID] = 650;
	v.P_Ultimate2[playerID] = 300;

	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 120:
			SSSCC.main(playerID);
			break;
		case 130:
			SSSCCAA.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 210:
			CC.main(playerID);
			break;
		case 220:
			CCA.main(playerID);
			break;
		case 230:
			CCAAA.main(playerID);
			break;
		case 240:
			CCAAAAA.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 310:
			AAAA.main(playerID);
			break;
		case 320:
			AAAAAA.main(playerID);
			break;
		case 330:
			AASS.main(playerID);
			break;
		case 340:
			AO.main(playerID);
			break;
			
		}
	}
	
	loop++;
	if (loop > 12)
	{
		O.savePosRoutine(playerID);
		loop = 0;
	}
}

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      v.stb.printAt(0, "\n");
	      v.stb.printAt(1, "\x1F　＃\x1B- 오다 사쿠노스케　\x04[ 문호 스트레이독스 ]");
	      v.stb.printAt(2, "　　\x1FA\x04ction List");
	      v.stb.printAt(3, "　　　\x18O \x04천의무봉 \x19[ 5초 전의 장소로 이동 ] \x053분");
	      v.stb.printAt(4, "　　　\x18AO \x04킬러 \x04 \x19[ 영구 각성 / 자리고정 / 30초 ]");
	      v.stb.printAt(5, "　　　\x04\x1FSSS + CC + AA \x04꿈 \x19[ 공성 / CC - 자리고정 ]");
	      v.stb.printAt(6, "　　　\x04\x1FCC + A + CC + AA \x04실의 \x19[ 공성 / 자리고정 / 각성시 사용 가능 ]");
	      v.stb.printAt(7, "　　　\x04\x1FAA \x04전투술 \x19[ 대인 / 순간딜 / 각성시 사용 가능 ]");
	      v.stb.printAt(8, "　　　\x08ACCC \x04절규 \x19[ 공성 / 자리고정 ] \x05", v.P_Ultimate1[playerID]);
	      v.stb.printAt(9, "　　　\x08 + AA \x04절망 \x19[ 공성 / 선딜 10초 / 선딜 후 자리고정 ] \x05", v.P_Ultimate2[playerID]);
	      v.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}