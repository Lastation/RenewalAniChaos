import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   s.HoldPosition(playerID);
   s.BanReturn(playerID);

   MoveUnit(All, "40 + 1n Goliath", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");

   ModifyUnitShields(All, v.P_CharacterID[playerID], playerID, "Anywhere", 1);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] > 0)
         {
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");

            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 3, 75);
         }
         if (v.P_LoopMain[playerID] > 1)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 5, 150);
         }
         if (v.P_LoopMain[playerID] > 2)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 7, 225);
         }
         if (v.P_LoopMain[playerID] > 3)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 9, 300);
         }
         if (v.P_LoopMain[playerID] > 4)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 11, 375);
         }
         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 32)
         {
            s.Voice_Routine(playerID, 2);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 1)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 3, 75);
         }
         if (v.P_LoopMain[playerID] < 2)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 5, 150);
         }
         if (v.P_LoopMain[playerID] < 3)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 7, 225);
         }
         if (v.P_LoopMain[playerID] < 4)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 9, 300);
         }
         if (v.P_LoopMain[playerID] < 5)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 11, 375);
         }

         if (v.P_LoopMain[playerID] == 1)
         {
            s.EdgeShapeWithProperty(playerID, 1, "40 + 1n Lurker", 45, 3, 75, 0);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            s.EdgeShapeWithProperty(playerID, 1, "40 + 1n Lurker", 45, 5, 150, 0);
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            s.EdgeShapeWithProperty(playerID, 1, "40 + 1n Lurker", 45, 7, 225, 0);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.EdgeShapeWithProperty(playerID, 1, "40 + 1n Lurker", 45, 9, 300, 0);
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            s.EdgeShapeWithProperty(playerID, 1, "40 + 1n Lurker", 45, 11, 375, 0);
         }

         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         RemoveUnitAt(All, "40 + 1n Lurker", "[Skill]Unit_Wait_ALL", playerID);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 14)
         {
            s.Voice_Routine(playerID, 3);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 10)
         {
            if (v.P_LoopMain[playerID] < 4)
               s.LineShape(playerID, 1, "60 + 1n High Templar", 90, 11, 75, 75 * 4 - 75 * v.P_LoopMain[playerID]);
            else
               s.LineShape(playerID, 1, "60 + 1n High Templar", 270, 11, 75, -75 * 4 + 75 * v.P_LoopMain[playerID]);
         }

         if (v.P_LoopMain[playerID] < 11)
         {
            if (v.P_LoopMain[playerID] < 5)
               s.LineShape(playerID, 1, "60 + 1n Siege", 90, 11, 75, 75 * 5 - 75 * v.P_LoopMain[playerID]);
            else
               s.LineShape(playerID, 1, "60 + 1n Siege", 270, 11, 75, -75 * 5 + 75 * v.P_LoopMain[playerID]);
         }

         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 14)
         {
            KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);

            s.Voice_Routine(playerID, 4);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 5)
         {
            s.EdgeShape(playerID, 1, "60 + 1n High Templar", 45, 3 + 2 * v.P_LoopMain[playerID], 75 * (v.P_LoopMain[playerID] + 1));
         }
         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         if (v.P_LoopMain[playerID] < 6 && v.P_LoopMain[playerID] > 0)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Tank", 45, 1 + 2 * v.P_LoopMain[playerID], 75 * v.P_LoopMain[playerID]);
         }

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         RemoveUnitAt(All, "50 + 1n Tank", "[Skill]Unit_Wait_ALL", playerID);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 22)
         {
            s.Voice_Routine(playerID, 5);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 3, 75);
            s.EdgeShape(playerID, 1, "80 + 1n Tank", 45, 3, 75);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 5, 150);
            s.EdgeShape(playerID, 1, "80 + 1n Tank", 45, 5, 150);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 7, 225);
            s.EdgeShape(playerID, 1, "80 + 1n Tank", 45, 7, 225);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 9, 300);
            s.EdgeShape(playerID, 1, "80 + 1n Tank", 45, 9, 300);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);            
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 11, 375);
            s.EdgeShape(playerID, 1, "80 + 1n Tank", 45, 11, 375);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);            
         }
         KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 24)
         {
            s.Voice_Routine(playerID, 6);
         }

         if (v.P_LoopMain[playerID] == 48)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
            SetSwitch("UiltimateSwitch", Clear);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         SetDeaths(playerID, SetTo, 0, " `ShieldRecharge");

         s.SkillEnd();
      }
   }
}