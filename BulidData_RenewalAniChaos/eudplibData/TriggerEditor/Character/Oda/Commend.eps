import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
	if (s.wait[playerID] == 0 && s.step[playerID] == 0)
	{
		if (Bring(playerID, AtLeast, 1, "Protoss Corsair", "[Skill]UseSkill")
		&& Deaths(CurrentPlayer, AtLeast, 1, " `O Skill Condition")
		&& Deaths(CurrentPlayer, Exactly, 0, " `UniqueCoolTime"))
		{
			s.Voice_Routine(playerID, 27);
		
			s.step[playerID] = 1;
			KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Corsair", "[Skill]UseSkill"))
		{
			SetResources(CurrentPlayer, Add, 60, Gas);
			KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Carrier", "[Skill]UseSkill"))
		{
			s.step[playerID] = 200;
			KillUnitAt(1, "Protoss Carrier", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill"))
		{
			s.step[playerID] = 300;
			KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		
		else if (Bring(playerID, AtLeast, 1, "Protoss Scout", "[Skill]UseSkill"))
		{
			s.step[playerID] = 100;
			KillUnitAt(1, "Protoss Scout", "[Skill]UseSkill", playerID);
		}

	}
	
	// Combo
	if (s.wait[playerID] >= 2 && v.P_CountMain[playerID] == 0)
	{
		if (Bring(playerID, AtLeast, 2, "Protoss Scout", "[Skill]UseSkill") && s.step[playerID] == 100)
		{
			s.Voice_Routine(playerID, 13);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 110;
			KillUnitAt(2, "Protoss Scout", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 2, "Protoss Carrier", "[Skill]UseSkill") 
		&& s.step[playerID] == 110)
		{
			s.Voice_Routine(playerID, 14);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 120;
			KillUnitAt(2, "Protoss Carrier", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 2, "Protoss Arbiter", "[Skill]UseSkill")
		&& s.step[playerID] == 120)
		{
			s.Voice_Routine(playerID, 15);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 130;
			KillUnitAt(2, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Carrier", "[Skill]UseSkill") && s.step[playerID] == 200
		&& Switch("Reinforce - Oda", Set))
		{
			s.Voice_Routine(playerID, 18);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 210;
			KillUnitAt(1, "Protoss Carrier", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill") && s.step[playerID] == 210)
		{
			s.Voice_Routine(playerID, 19);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 220;
			KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 2, "Protoss Carrier", "[Skill]UseSkill") && s.step[playerID] == 220)
		{
			s.Voice_Routine(playerID, 20);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 230;
			KillUnitAt(2, "Protoss Carrier", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 2, "Protoss Arbiter", "[Skill]UseSkill") && s.step[playerID] == 230)
		{
			s.Voice_Routine(playerID, 22);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 240;
			KillUnitAt(2, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill") 
		&& s.step[playerID] == 300
		&& Switch("Reinforce - Oda", Set))
		{
			s.Voice_Routine(playerID, 25);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 330;
			KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		
		// Ultimate
		else if (Bring(playerID, AtLeast, 3, "Protoss Carrier", "[Skill]UseSkill") 
		&& s.step[playerID] == 300
		&& Deaths(playerID, AtLeast, s.UltimateA[playerID], " `UltimateCoolTime"))
		{
			if (Switch("UiltimateSwitch", Cleared))
			{
				s.Voice_Routine(playerID, 1);
				SetSwitch("UiltimateSwitch", Set);
               		CreateUnit(1, " Item. Flag", "[Uiltimate]Flag", CurrentPlayer);
				s.wait[playerID] = 0;
				v.P_CountMain[playerID] = 0;
				v.P_LoopMain[playerID] = 0;
				s.step[playerID] = 310;
				SetDeaths(playerID, Subtract, s.UltimateA[playerID], " `UltimateCoolTime");
				KillUnitAt(3, "Protoss Carrier", "[Skill]UseSkill", playerID);
			}
			else
			{
				SetResources(CurrentPlayer, Add, 900, Gas);
				KillUnitAt(3, "Protoss Arbiter", "[Skill]UseSkill", playerID);
				SetDeaths(CurrentPlayer, SetTo, 999, " `SYSTEMTEXT");
			}
		}
		else if (Bring(playerID, AtLeast, 2, "Protoss Arbiter", "[Skill]UseSkill") 
		&& s.step[playerID] == 310
		&& Deaths(playerID, AtLeast, s.UltimateB[playerID], " `UltimateCoolTime")
		)
		{
			SetSwitch("UiltimateSwitch", Set);
			SetSwitch("Ult - Oda", Set);
			s.Voice_Routine(playerID, 7);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 320;
			SetDeaths(playerID, Subtract, s.UltimateB[playerID], " `UltimateCoolTime");
			KillUnitAt(2, "Protoss Arbiter", "[Skill]UseSkill", playerID);
		}
		else if (Bring(playerID, AtLeast, 1, "Protoss Corsair", "[Skill]UseSkill") 
		&& s.step[playerID] == 300
		&& Deaths(CurrentPlayer, AtLeast, 1, " `O Skill Condition")
		&& Switch("Reinforce - Oda", Cleared)
		)
		{
			s.Voice_Routine(playerID, 28);
			s.wait[playerID] = 0;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
			s.step[playerID] = 340;
			KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
		}
		

	}
}