import variable as v;
import func.trig as trg;

function main(playerID)
{
   MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "40 + 1n Goliath", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "60 + 1n Dragoon", playerID, "Anywhere", "[Skill]HoldPosition");

   trg.Debuff_BanReturn();
   trg.Debuff_Stop();

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         var i = 50;
         var d = 25 + 25 * v.P_LoopMain[playerID];

         trg.Table_Sin(playerID, 45, 25);
         trg.Table_Cos(playerID, 45, 25);

         var x_i = v.P_AngleCos[playerID];
         var y_i = v.P_AngleSin[playerID];

         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", d - i / 2 + x_i, d + i / 2 + y_i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", d - i, d + i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", d + 2 * x_i, d + 2 * y_i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", d + i, d - i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", d + i / 2 + x_i, d - i / 2 + y_i);

         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", d - i, d + i);
         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", d, d);
         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", d - i, d + i);

         KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
         KillUnitAt(All, "Vulture Spider Mine", "Anywhere", playerID);

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 6)
         {
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", d - i / 2 + x_i, d + i / 2 + y_i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", d - i, d + i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", d + 2 * x_i, d + 2 * y_i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", d + i, d - i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", d + i / 2 + x_i, d - i / 2 + y_i);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {

         var i = 50;
         var d = 25 + 25 * v.P_LoopMain[playerID];

         trg.Table_Sin(playerID, 45, 25);
         trg.Table_Cos(playerID, 45, 25);

         var x_i = v.P_AngleCos[playerID];
         var y_i = v.P_AngleSin[playerID];

         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", -d + i / 2 - x_i, d + i / 2 + y_i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", -d + i, d + i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", -d - 2 * x_i, d + 2 * y_i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", -d - i, d - i);
         trg.Shape_Double(playerID, 1, "Vulture Spider Mine", -d - i / 2 - x_i, d - i / 2 + y_i);

         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", -d + i, d + i);
         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", -d, d);
         trg.Shape_Double(playerID, 1, " Unit. Hoffnung 25000", -d + i, d + i);

         KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
         KillUnitAt(All, "Vulture Spider Mine", "Anywhere", playerID);

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 6)
         {
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", -d + i / 2 - x_i, d + i / 2 + y_i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", -d + i, d + i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", -d - 2 * x_i, d + 2 * y_i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", -d - i, d - i);
            trg.Shape_Double(playerID, 1, "60 + 1n Siege", -d - i / 2 - x_i, d - i / 2 + y_i);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 7)
         {
            var i = 50;
            var d = 150 - 25 * v.P_LoopMain[playerID];

            trg.Table_Sin(playerID, 45, 15);
            trg.Table_Cos(playerID, 45, 15);

            var x_i = v.P_AngleCos[playerID];
            var y_i = v.P_AngleSin[playerID];

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", d - i + x_i, d + i + y_i);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", d + 2 * x_i, d + 2 * y_i);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", d + i + x_i, d - i + y_i);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] == 1)
            {
               trg.Shape_Square(playerID, 1, "40 + 1n Drone", d - i + x_i, d + i + y_i);
               trg.Shape_Square(playerID, 1, "40 + 1n Drone", d + 2 * x_i, d + 2 * y_i);
               trg.Shape_Square(playerID, 1, "40 + 1n Drone", d + i + x_i, d - i + y_i);

               MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
               MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
               Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            }
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 9)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         var i = 0;
         var x = 0;
         var y = 0;

         if (v.P_LoopMain[playerID] < 4)
         {
            i = v.P_LoopMain[playerID];

            if (i < 2)
            {
               x = 50 - 50 * i;
               y = 50 + 50 * i;
            }
            else
            {
               x = -50 - 50 * (i - 2);
               y = 50 - 50 * (i - 2); 
            }
         }
         else if (v.P_LoopMain[playerID] < 12)
         {
            i = v.P_LoopMain[playerID] - 4;

            if (i < 4)
            {
               x = 150 - 50 * i;
               y = 50 + 50 * i;
            }
            else
            {
               x = -50 - 50 * (i - 4);
               y = 150 - 50 * (i - 4); 
            }
         }

         if (i % 2 == 0 && v.P_LoopMain[playerID] < 12)
         {
            abv.Shape_EdgeAt(playerID, 1, "Kakaru (Twilight)", 0, 2, 25, x, y);
            abv.Shape_EdgeAt(playerID, 1, "Rhynadon (Badlands)", 0, 2, 25, x, y);
            abv.Shape_EdgeAt(playerID, 1, "Kakaru (Twilight)", 0, 2, 25, -x, -y);
            abv.Shape_EdgeAt(playerID, 1, "Rhynadon (Badlands)", 0, 2, 25, -x, -y);

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);
         }
         else 
         {
            abv.Shape_EdgeAt(playerID, 1, "40 + 1n Guardian", 0, 2, 25, x, y);
            abv.Shape_EdgeAt(playerID, 1, "60 + 1n Archon", 0, 2, 25, x, y);
            trg.Shape_Dot(playerID, 1, "50 + 1n Tank", x, y);
            abv.Shape_EdgeAt(playerID, 1, "40 + 1n Guardian", 0, 2, 25, -x, -y);
            abv.Shape_EdgeAt(playerID, 1, "60 + 1n Archon", 0, 2, 25, -x, -y);
            trg.Shape_Dot(playerID, 1, "50 + 1n Tank", -x, -y);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Gantrithor", 50, 50);
            trg.Shape_Square(playerID, 1, "60 + 1n Dragoon", 50, 50);

         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Gantrithor", 50, 150);
            trg.Shape_Square(playerID, 1, "60 + 1n Dragoon", 50, 150);

         }           
         else if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Gantrithor", 150, 50);
            trg.Shape_Square(playerID, 1, "60 + 1n Dragoon", 150, 50);
         }

         KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 9)
         {
            s.CharacterVoice(21);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         if (v.P_LoopMain[playerID] < 6)
         {
            trg.Shape_Double(playerID, 1, "Protoss Dark Archon", 150 - 50 * v.P_LoopMain[playerID], 150);
            trg.Shape_Double(playerID, 1, "80 + 1n Tank", -150, 150 - 50 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Edge(playerID, 1, "40 + 1n Mojo", 45, 3, 75);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_Edge(playerID, 1, "40 + 1n Mojo", 45, 5, 150);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Edge(playerID, 1, "Protoss Dark Archon", 22, 3, 75);
            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 22, 3, 75);
            trg.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 45, 3, 100);
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 2, 100);
            trg.Shape_Dot(playerID, 1, "50 + 1n Battlecruiser", 0, 0);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            trg.Shape_Edge(playerID, 1, "Protoss Dark Archon", 45, 3, 50);
            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 45, 3, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 9)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 22, 3, 75);
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 22, 3, 75);
            trg.Shape_Dot(playerID, 1, "50 + 1n Battlecruiser", 0, 0);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 11)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 12)
         {
            trg.Shape_Edge(playerID, 1, "Protoss Dark Archon", 22, 3, 75);
            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 22, 3, 75);
            trg.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 13)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 45, 3, 100);
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 3, 100);
            trg.Shape_Dot(playerID, 1, "50 + 1n Battlecruiser", 0, 0);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 15)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 26)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }

      else if (v.P_CountMain[playerID] == 6)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);

         trg.SkillEnd();
      }
   }
}