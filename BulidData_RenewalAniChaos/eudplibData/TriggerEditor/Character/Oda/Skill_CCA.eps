import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   s.BanReturn(playerID);
   s.HoldPosition(playerID);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

         var d = 105 - 10 * v.P_LoopMain[playerID];

         s.EdgeShape(playerID, 1, "40 + 1n Mutalisk", 45, 3, d);
         s.EdgeShape(playerID, 1, "Protoss Dark Archon", 45, 3, d);
         KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         s.EdgeShape(playerID, 1, "Bengalaas (Jungle)", 45, 3, 50);
         s.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);
         KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] < 2)
         {
            s.EdgeShape(playerID, 1, "Protoss Dark Archon", 45, 3 + 2 * v.P_LoopMain[playerID], 50 + 50 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] < 6)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            var i = v.P_LoopMain[playerID] - 2;

            var x = 50 - 50 * i;
            var y = 100;

            s.Shape_Square(playerID, 1, "40 + 1n Mutalisk", x, y);
            s.Shape_Square(playerID, 1, "Rhynadon (Badlands)", x, y);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            x = 100 - 50 * i;
            y = 100;

            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", x, y);
            s.Shape_Square(playerID, 1, "Bengalaas (Jungle)", x, y);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] == 5)
            {
               s.Shape_Square(playerID, 1, "Target", 150, 150);
               KillUnitAt(All, "Target", "Anywhere", playerID);
            }
         }
         else if (v.P_LoopMain[playerID] == 6)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "Target", 50, 50);
            KillUnitAt(All, "Target", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            s.Shape_Dot(playerID, 1, "Target", 0, 0);
            KillUnitAt(All, "Target", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            s.Shape_NxNSquare(playerID, 1, "40 + 1n Mutalisk", 3, 50);
            s.Shape_NxNSquare(playerID, 1, "Rhynadon (Badlands)", 3, 50);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);

            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 10)
         {
            s.EdgeShape(playerID, 1, "Rhynadon (Badlands)", 45, 5, 100);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.Table_Sin(playerID, 30 * v.P_LoopMain[playerID], 100);
         s.Table_Cos(playerID, 30 * v.P_LoopMain[playerID], 100);

         s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
         s.Shape_Square(playerID, 1, "40 + 1n Goliath", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}