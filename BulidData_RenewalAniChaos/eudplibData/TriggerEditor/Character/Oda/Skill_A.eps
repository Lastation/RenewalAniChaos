import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

         s.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);
         s.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);

         s.Shape_Square(playerID, 1, "Rhynadon (Badlands)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
         KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);

         s.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, 0);
         Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "50 + 1n Tank", 45, 5, 100);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            s.EdgeShape(playerID, 1, " Unit. Hoffnung 25000", 45, 5, 100);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 3, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.LineShapeAt(playerID, 1, "40 + 1n Wraith", 135, 4, 50, 100, 50);
            s.LineShapeAt(playerID, 1, "40 + 1n Wraith", 135, 4, 50, -100, -50);
            s.LineShapeAt(playerID, 1, "50 + 1n Tank", 135, 4, 50, 100, 50);
            s.LineShapeAt(playerID, 1, "50 + 1n Tank", 135, 4, 50, -100, -50);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShapeAt(playerID, 1, "40 + 1n Wraith", 45, 4, 50, -50, 100);
            s.LineShapeAt(playerID, 1, "40 + 1n Wraith", 45, 4, 50, 50, -100);
            s.LineShapeAt(playerID, 1, "50 + 1n Tank", 45, 4, 50, -50, 100);
            s.LineShapeAt(playerID, 1, "50 + 1n Tank", 45, 4, 50, 50, -100);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 6)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 7)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}