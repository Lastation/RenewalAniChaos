function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 170;
	v.P_CharacterID[playerID] = 77;
	
	s.UltimateA[playerID] = 700;
	s.UltimateB[playerID] = 700;
	
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			//O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			//A.main(playerID);
			break;
		}
	}
	
}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Kiana_Voice[playerID] != 0) { text.main(playerID); }
}

