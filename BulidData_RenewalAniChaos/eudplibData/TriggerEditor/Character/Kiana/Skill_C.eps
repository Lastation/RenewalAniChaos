import PluginVariables as msqcvar;
import func.shape as s;

function Shape(playerID, UnitA : TrgUnit, UnitB : TrgUnit, x, y);

function main(playerID)
{
	if (v.P_Delay[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			s.Shape_Dot(playerID, 1, "60 + 1n Danimoth", 0, 0);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			s.Shape_NxNSquare(playerID, 1, "60 + 1n Danimoth", 3, 64);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				s.Shape_Square(playerID, 1, "50 + 1n Tank", 128, 64);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				s.Shape_Square(playerID, 1, "50 + 1n Tank", 64, 128);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				s.Shape_Square(playerID, 1, "40 + 1n Goliath", 64, 64);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(1000);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				v.P_CountMain[playerID] += 1;
				v.P_LoopMain[playerID] = 0;
			}
		}
		else if (v.P_CountMain[playerID] == 3)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				Shape(playerID, "40 + 1n Mojo", "Protoss Dark Archon", 96, 32);
				s.SkillWait(50);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				Shape(playerID, "40 + 1n Mojo", "Protoss Dark Archon", 32, 32);
				s.SkillWait(50);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				Shape(playerID, "40 + 1n Mojo", "Protoss Dark Archon", -32, 32);
				s.SkillWait(50);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				Shape(playerID, "40 + 1n Mojo", "Protoss Dark Archon", -96, 32);
				s.SkillWait(50);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
				s.SkillEnd();
			}
		}
	}
	
	if (v.P_DelayB[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 2)
		{
			if (v.P_LoopMainB[playerID] == 0)
			{
				s.Table_Cos(playerID, 45, 128);
				s.Table_Sin(playerID, 45, 128);
				
				s.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 128);
				s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID] + v.P_AngleSin[playerID], -v.P_AngleSin[playerID] + v.P_AngleCos[playerID]);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				s.SkillWaitB(playerID, 0);
				v.P_LoopMainB[playerID] += 1;
			}
			else if (v.P_LoopMainB[playerID] == 1)
			{
				s.Table_Cos(playerID, 45, 128);
				s.Table_Sin(playerID, 45, 64);
				
				s.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 64);
				s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID] + v.P_AngleSin[playerID], -v.P_AngleSin[playerID] + v.P_AngleCos[playerID]);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				s.SkillWaitB(playerID, 0);
				v.P_LoopMainB[playerID] += 1;
			}
			else if (v.P_LoopMainB[playerID] == 2)
			{
				s.Table_Cos(playerID, 45, 128);
				s.Table_Sin(playerID, 45, 0);
				
				s.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 0);
				s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID] + v.P_AngleSin[playerID], -v.P_AngleSin[playerID] + v.P_AngleCos[playerID]);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				s.SkillWaitB(playerID, 0);
				v.P_LoopMainB[playerID] += 1;
			}
			else if (v.P_LoopMainB[playerID] == 3)
			{
				s.Table_Cos(playerID, 45, 64);
				s.Table_Sin(playerID, 45, 128);
				
				s.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, -64);
				s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID] + v.P_AngleSin[playerID], -v.P_AngleSin[playerID] + v.P_AngleCos[playerID]);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				s.SkillWaitB(playerID, 0);
				v.P_LoopMainB[playerID] = 0;
			}
		}
	}
}

function Shape(playerID, UnitA : TrgUnit, UnitB : TrgUnit, x, y)
{
	s.MoveLoc(v.P_CharacterID[playerID], playerID, x, y);
	s.SkillUnit(playerID, 1, UnitA);
	s.SkillUnit(playerID, 1, UnitB);
	s.MoveLoc(v.P_CharacterID[playerID], playerID, -x, -y);
	s.SkillUnit(playerID, 1, UnitA);
	s.SkillUnit(playerID, 1, UnitB);
	KillUnitAt(All, UnitB, "Anywhere", playerID);
	MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
	Order(UnitA, playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
}