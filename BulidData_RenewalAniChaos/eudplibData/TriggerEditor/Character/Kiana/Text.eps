import PluginVariables as msqcvar;
import func.shape as s;

const looker = PVariable();

function Text(num);
function Looker()
{
	if (looker[getuserplayerid() - 128] > 0)
	{
		Text(looker[getuserplayerid() - 128]);
		looker[getuserplayerid() - 128] = 0;
	}
}

function main(playerID)
{
	if (s.Kiana_Voice[playerID] > 0)
	{
		Text(s.Kiana_Voice[playerID]);
		looker[playerID] = s.Kiana_Voice[playerID];
		s.Kiana_Voice[playerID] = 0;
	}
}

function Text(num)
{
	switch (num) 
	{
	case 1:
		PlayWAV("Kiana_01.ogg");
		s.stb.print("\x13\x19Kiana Kaslana\n\n\x13\x04네코 참!");
		break;
	}
}