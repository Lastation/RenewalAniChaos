import PluginVariables as msqcvar;
import Function as f;

const looker = PVariable();

function Text(num);
function Looker()
{
	if (looker[getuserplayerid() - 128] > 0)
	{
		Text(looker[getuserplayerid() - 128]);
		looker[getuserplayerid() - 128] = 0;
	}
}

function main(cp)
{
	if (f.Kiana_Voice[cp] > 0)
	{
		Text(f.Kiana_Voice[cp]);
		looker[cp] = f.Kiana_Voice[cp];
		f.Kiana_Voice[cp] = 0;
	}
}

function Text(num)
{
	switch (num) 
	{
	case 1:
		PlayWAV("Kiana_01.ogg");
		f.stb.print("\x13\x19Kiana Kaslana\n\n\x13\x04네코 참!");
		break;
	}
}