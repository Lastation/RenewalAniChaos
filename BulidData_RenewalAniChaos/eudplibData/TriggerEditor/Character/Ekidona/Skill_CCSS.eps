import variable as v;
import func.trig as trg;

function main(playerID)
{
   trg.Debuff_BanReturn();
   trg.Debuff_Stop();

   MoveUnit(All, "60 + 1n High Templar", playerID, "Anywhere", "[Skill]HoldPosition");

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 50, 0);
            epic.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 0, 1);

            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "60 + 1n High Templar", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("60 + 1n High Templar", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 100, 100);
            epic.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 100, 1);

            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "60 + 1n High Templar", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("60 + 1n High Templar", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 150, 0);
            epic.Shape_Square(playerID, 1, "40 + 1n Mojo", 150, 0, 1);

            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "60 + 1n High Templar", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("60 + 1n High Templar", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 8)
         {
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 50, 0);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 100, 100);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 150, 0);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 16)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         RemoveUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

         if (v.P_LoopMain[playerID] < 12)
         {
            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 150 - 8 * v.P_LoopMain[playerID], 0);
            }
            if (v.P_LoopMain[playerID] % 2 == 1)
            {
               epic.Shape_Square(playerID, 1, "40 + 1n Mojo", 150 - 8 * v.P_LoopMain[playerID], 0, 1);
            }
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150 - 8 * v.P_LoopMain[playerID], 0);
            trg.Shape_Square(playerID, 1, "Rhynadon (Badlands)", 50, 50);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);

            trg.MoveLoc("60 + 1n High Templar", playerID, 0, 0);
            RemoveUnitAt(1, "60 + 1n High Templar", "Anywhere", playerID);
            trg.SkillUnit(playerID, 1, "40 + 1n Guardian");
            trg.SkillUnit(playerID, 1, " Unit. Schnee");
            epic.SkillUnit(playerID, 1, "40 + 1n Lurker", 0);
            epic.SkillUnit(playerID, 1, "40 + 1n Drone", 0);

            KillUnitAt(All, " Unit. Schnee", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 17)
         {
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 42)
         {
            KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            trg.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            trg.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);

         trg.SkillEnd();
      }
   }
}