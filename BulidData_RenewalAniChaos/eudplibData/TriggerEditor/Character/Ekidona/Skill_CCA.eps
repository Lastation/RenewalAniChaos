import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   s.BanReturn(playerID);
   s.HoldPosition(playerID);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.Shape_Dot(playerID, 1, "40 + 1n Gantrithor", 0, 0);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 15)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 3)
         {
            s.Shape_Square(playerID, 8, "Bengalaas (Jungle)", 75, 75);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] < 5)
         {
            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", 75, 75);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 3)
         {
            s.Shape_Square(playerID, 1, "60 + 1n Dragoon", 75, 75);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Dragoon", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 11)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         var x = 225 - 75 * v.P_LoopMain[playerID];
         var y = 75 + 75 * v.P_LoopMain[playerID];

         if (v.P_LoopMain[playerID] < 4)
         {
            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
            s.Shape_Square(playerID, 1, "60 + 1n Siege", x, y);

            s.LineShape(playerID, 1, "Kakaru (Twilight)", 45 * v.P_LoopMain[playerID], 7, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Zergling", 45 * v.P_LoopMain[playerID], 7, 50, 0);

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Zergling", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            s.EdgeShape(playerID, 1, "50 + 1n Tank", 0, 3, 100);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 8)
         {
            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 0, 3, 100);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 0, 5, 200);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 0, 5, 200);
            s.EdgeShape(playerID, 1, "Protoss Reaver", 0, 5, 200);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            ModifyUnitHangarCount(1, All, "Protoss Reaver", CurrentPlayer, "Anywhere");

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "Protoss Reaver", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);

         }
         if (v.P_LoopMain[playerID] == 13)
         {
            KillUnitAt(All, "Protoss Reaver", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 15)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.Shape_Dot(playerID, 1, "50 + 1n Battlecruiser", 0, 0);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] < 6)
         {
            s.Table_Sin(playerID, 0, 300 - 50 * v.P_LoopMain[playerID]);
            s.Table_Cos(playerID, 0, 300 - 50 * v.P_LoopMain[playerID]);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "Bengalaas (Jungle)", x, y);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);

            s.Table_Sin(playerID, 45, 300 - 50 * v.P_LoopMain[playerID]);
            s.Table_Cos(playerID, 45, 300 - 50 * v.P_LoopMain[playerID]);

            x = v.P_AngleCos[playerID];
            y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "Bengalaas (Jungle)", x, y);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            s.Table_Sin(playerID, 0, 100);
            s.Table_Cos(playerID, 0, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
            s.Shape_Square(playerID, 4, "50 + 1n Tank", x, y);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 7)
         {
            s.Table_Sin(playerID, 0, 100);
            s.Table_Cos(playerID, 0, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 11)
         {
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            s.Table_Sin(playerID, 30, 100);
            s.Table_Cos(playerID, 30, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
            s.Shape_Square(playerID, 4, "50 + 1n Tank", x, y);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 12)
         {
            s.Table_Sin(playerID, 30, 100);
            s.Table_Cos(playerID, 30, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 16)
         {
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            s.Table_Sin(playerID, 60, 100);
            s.Table_Cos(playerID, 60, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
            s.Shape_Square(playerID, 4, "50 + 1n Tank", x, y);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 17)
         {
            s.Table_Sin(playerID, 60, 100);
            s.Table_Cos(playerID, 60, 100);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 19)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }   


      else if (v.P_CountMain[playerID] == 4)
      {
         KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}