import variable as v;
import func.trig as trg;

import Character.Ekidona.Skill_O as O;
import Character.Ekidona.Skill_S as S;
import Character.Ekidona.Skill_C as C;
import Character.Ekidona.Skill_A as A;

import Character.Ekidona.Skill_SSS as SSS;
import Character.Ekidona.Skill_CCA as CCA;
import Character.Ekidona.Skill_CCAAAA as CCAAAA;
import Character.Ekidona.Skill_CCSS as CCSS;
import Character.Ekidona.Skill_CCSSA as CCSSA;
import Character.Ekidona.Skill_AAAAA as AAAAA;
import Character.Ekidona.Skill_AAAAAAAA as AAAAAAAA;
import Character.Ekidona.Text as text;
import Character.Ekidona.Commend as commend;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 195;
	v.P_UnitID[playerID] = 34;
	
	v.P_Ultimate1[playerID] = 750;
	v.P_Ultimate2[playerID] = 200;
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCA.main(playerID);
			break;
		case 220:
			CCAAAA.main(playerID);
			break;
		case 230:
			CCSS.main(playerID);
			break;
		case 240:
			CCSSA.main(playerID);
			break;
		case 310:
			AAAAA.main(playerID);
			break;
		case 320:
			AAAAAAAA.main(playerID);
			break;
		}
	}
	
	if (Switch("Unique - Ekidona", Set))
	{
		O.contract(playerID);
	}
}

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 에키드나　\x04[ Re : 제로부터 시작하는 이세계 생활 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04계약 \x19[ 아군 한 명과 계약 / 계약 대상 5분(개인쿨)마다 부활 시간 10초 단축 ] \x0530초");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04즐거움 \x19[ 테러 ]");
		v.stb.printAt(5, "　　　\x04\x1FCCA + AAC \x04당황 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(6, "　　　\x04\x1FCCC + C \x04흥미 \x19[ 만능 / 자리고정 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAAAA + AAA \x04탐욕의 마녀 \x19[ 공성 ] \x05", v.P_Ultimate1[playerID], " + ",  v.P_Ultimate2[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}