import Function as f;

import Character.Ekidona.Skill_O as O;
import Character.Ekidona.Skill_S as S;
import Character.Ekidona.Skill_C as C;
import Character.Ekidona.Skill_A as A;

import Character.Ekidona.Skill_SSS as SSS;
import Character.Ekidona.Skill_CCA as CCA;
import Character.Ekidona.Skill_CCAAAA as CCAAAA;
import Character.Ekidona.Skill_CCSS as CCSS;
import Character.Ekidona.Skill_CCSSA as CCSSA;
import Character.Ekidona.Skill_AAAAA as AAAAA;
import Character.Ekidona.Skill_AAAAAAAA as AAAAAAAA;
import Character.Ekidona.Text as text;
import Character.Ekidona.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 195;
	f.heroID[cp] = 34;
	
	f.UltimateA[cp] = 750;
	f.UltimateB[cp] = 200;
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCA.main(cp);
			break;
		case 220:
			CCAAAA.main(cp);
			break;
		case 230:
			CCSS.main(cp);
			break;
		case 240:
			CCSSA.main(cp);
			break;
		case 310:
			AAAAA.main(cp);
			break;
		case 320:
			AAAAAAAA.main(cp);
			break;
		}
	}
	
	if (Switch("Unique - Ekidona", Set))
	{
		O.contract(cp);
	}
}

// Character Voice
function SkillVoice(cp)
{
	if (f.Ekidona_Voice[cp] != 0) { text.main(cp); }
}

function SkillList(cp)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		f.stb.printAt(0, "\n");
		f.stb.printAt(1, "\x1F　＃\x1B- 에키드나　\x04[ Re : 제로부터 시작하는 이세계 생활 ]");
		f.stb.printAt(2, "　　\x1FA\x04ction List");
		f.stb.printAt(3, "　　　\x18O \x04계약 \x19[ 아군 한 명과 계약 / 계약 대상 5분(개인쿨)마다 부활 시간 10초 단축 ] \x0530초");
		f.stb.printAt(4, "　　　\x04\x1FSSS \x04즐거움 \x19[ 테러 ]");
		f.stb.printAt(5, "　　　\x04\x1FCCA + AAC \x04당황 \x19[ 공성 / 자리고정 ]");
		f.stb.printAt(6, "　　　\x04\x1FCCC + C \x04흥미 \x19[ 만능 / 자리고정 ]");
		f.stb.printAt(7, "\n");
		f.stb.printAt(8, "　　　\x08AAAAA + AAA \x04탐욕의 마녀 \x19[ 공성 ] \x05", f.UltimateA[cp], " + ",  f.UltimateB[cp]);
		f.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}