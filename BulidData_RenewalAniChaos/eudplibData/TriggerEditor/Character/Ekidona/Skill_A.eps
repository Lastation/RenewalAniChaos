import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.Shape_NxNSquare(playerID, 1, "40 + 1n Mojo", 3, 50);
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, "Anywhere");

            s.EdgeShapeWithProperty(playerID, 1, "60 + 1n Archon", 45, 5, 100, 1);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 45, 3, 50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         }

         var i = v.P_LoopMain[playerID] / 2;
         var x = 50 - 50 * i;
         var y = 100;

         if (v.P_LoopMain[playerID] < 8 && v.P_LoopMain[playerID] % 2 == 0)
         {
            RemoveUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);

            if (i % 2 == 0)
            {
               s.Shape_SquareWithProperty(playerID, 1, "80 + 1n Artanis", x, y, 1);
               s.Shape_Square(playerID, 1, "Protoss Dark Templar", x, y);
               KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);
            }
            else
            {
               s.Shape_Square(playerID, 1, "80 + 1n Artanis", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n High Templar", x, y);
               KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            }

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("80 + 1n Artanis", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 8)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            RemoveUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Mojo", 45, 5, 100);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 2)
         {
            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 45, 5, 100);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "40 + 1n Mojo", 3, 50);
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, "Anywhere");

            s.EdgeShape(playerID, 1, "60 + 1n Archon", 45, 3, 50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 6)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}