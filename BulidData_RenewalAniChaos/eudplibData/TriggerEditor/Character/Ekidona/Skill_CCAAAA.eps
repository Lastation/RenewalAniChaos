import PluginVariables as msqcvar;
import func.shape as s;

function Shape_NxNSquareAt(cp : TrgPlayer, count, Unit : TrgUnit, n, interval, x, y);
function Shape_NxNSquareAtDouble(cp : TrgPlayer, count, Unit : TrgUnit, n, interval, x, y);

var x = 0;
var y = 0;

function main(playerID)
{
   s.BanReturn(playerID);
   s.HoldPosition(playerID);

   MoveUnit(All, "40 + 1n Gantrithor", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "60 + 1n Dragoon", playerID, "Anywhere", "[Skill]HoldPosition");

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)      //0.00
      {
         RemoveUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

         if (v.P_LoopMain[playerID] % 2 == 0 && v.P_LoopMain[playerID] < 64)
         {
            RemoveUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            s.Table_Sin(playerID, 45 * (v.P_LoopMain[playerID] / 2), 64);
            s.Table_Cos(playerID, 45 * (v.P_LoopMain[playerID] / 2), 64);

            x = v.P_AngleCos[playerID];
            y = v.P_AngleSin[playerID];

            s.DoubleShape(playerID, 1, "60 + 1n Danimoth", x, y);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x, y);
            s.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            //Order("40 + 1n Mojo", playerID, "Anywhere", Attack, "Anywhere");
         }

         if (v.P_LoopMain[playerID] >= 8 && v.P_LoopMain[playerID] < 14)    //0.64 - 1.12
         {
            var i = v.P_LoopMain[playerID] - 8;

            if (i < 4)
            {
               s.Table_Sin(playerID, 0, 50 + 50 * i);
               s.Table_Cos(playerID, 0, 50 + 50 * i);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               s.Shape_Square(playerID, 1, "40 + 1n Mojo", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", x, y);

               KillUnitAt(All,  "60 + 1n Archon", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               //Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }

            if (i == 4)
            {
               s.Table_Sin(playerID, 0, 200);
               s.Table_Cos(playerID, 0, 200);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n Siege", x, y);

               KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
            }
         }
         if (v.P_LoopMain[playerID] >= 14 && v.P_LoopMain[playerID] < 20)   //1.12 - 1.60
         {
            var i = v.P_LoopMain[playerID] - 14;

            if (i < 4)
            {
               s.Table_Sin(playerID, 45, 50 + 50 * i);
               s.Table_Cos(playerID, 45, 50 + 50 * i);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               s.Shape_Square(playerID, 1, "40 + 1n Mojo", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", x, y);

               KillUnitAt(All,  "60 + 1n Archon", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               //Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }

            if (i == 4)
            {
               s.Table_Sin(playerID, 45, 200);
               s.Table_Cos(playerID, 45, 200);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n Siege", x, y);

               KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
            }
         }

         if (v.P_LoopMain[playerID] >= 30 && v.P_LoopMain[playerID] < 64)    //1.92
         {
            var i = v.P_LoopMain[playerID] - 30;

            if (i == 0) 
            {
               s.Table_Sin(playerID, 22, 100);
               s.Table_Cos(playerID, 22, 100);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }
            if (i == 6) 
            {
               s.Table_Sin(playerID, 67, 150);
               s.Table_Cos(playerID, 67, 150);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }            
            if (i == 12) 
            {
               s.Table_Sin(playerID, 67, 100);
               s.Table_Cos(playerID, 67, 100);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }            
            if (i == 18) 
            {
               s.Table_Sin(playerID, 22, 150);
               s.Table_Cos(playerID, 22, 150);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }
            if (i == 24) 
            {
               s.Table_Sin(playerID, 22, 100);
               s.Table_Cos(playerID, 22, 100);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }
            if (i == 30) 
            {
               s.Table_Sin(playerID, 67, 150);
               s.Table_Cos(playerID, 67, 150);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];
            }

            if (i % 6 == 0)
            {
               KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

               s.Shape_Square(playerID, 1, "50 + 1n Tank", x, y);
               s.Shape_Square(playerID, 1, "60 + 1n Danimoth", x, y);

               KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
               Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            }
         }
         if (v.P_LoopMain[playerID] == 64)      //5.12
         {
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            s.Voice_Routine(playerID, 17);            
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 72)      
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)    //5.80
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            Shape_NxNSquareAt(playerID, 1, "60 + 1n Danimoth", 2, 75, 100, 0);
            Shape_NxNSquareAt(playerID, 1, "60 + 1n High Templar", 2, 75, 100, 0);

            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 4)    //6.12
         {
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            Shape_NxNSquareAt(playerID, 1, "60 + 1n Danimoth", 2, 75, 100, 100);
            Shape_NxNSquareAt(playerID, 1, "60 + 1n High Templar", 2, 75, 100, 100);

            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         if (v.P_LoopMain[playerID] == 8)    //6.44
         {
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)    //6.80
      {
         if (v.P_LoopMain[playerID] < 4)
         {
            RemoveUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 50 - 50 * v.P_LoopMain[playerID], 100);
            s.Shape_Square(playerID, 1, "Protoss Dark Archon", 50 - 50 * v.P_LoopMain[playerID], 100);

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "60 + 1n Danimoth", 5, 75);
            s.Shape_NxNSquare(playerID, 1, "60 + 1n High Templar", 5, 75);

            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "60 + 1n Danimoth", 3, 75);
            s.Shape_NxNSquare(playerID, 1, "60 + 1n High Templar", 3, 75);

            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 12)
         {
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 16)
         {
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
            s.Voice_Routine(playerID, 18);            

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3) //8.10
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            for (var k = 0; k < 8; k++)
            {
               CreateUnit(4, "60 + 1n Siege", k + 33, playerID);
               SetInvincibility(Enable, "60 + 1n Siege", playerID, "[Skill]Unit_Wait_ALL");
            }
         }

         var i = v.P_LoopMain[playerID] % 4;

         if (v.P_LoopMain[playerID] % 8 == 0)
         {
            KillUnitAt(All, "Protoss Reaver", "Anywhere", playerID);
         }

         var r = ((v.P_LoopMain[playerID] / 4) % 2) * 45 + 22;

         if (i % 2 == 0) 
         {
            s.Table_Sin(playerID, r, 50 + 50 * i);
            s.Table_Cos(playerID, r, 50 + 50 * i);

            x = v.P_AngleCos[playerID];
            y = v.P_AngleSin[playerID];
         }
         if (i % 2 == 1) 
         {
            s.Table_Sin(playerID, r + 22, 50 + 50 * i);
            s.Table_Cos(playerID, r + 22, 50 + 50 * i);

            x = v.P_AngleCos[playerID];
            y = v.P_AngleSin[playerID];
         }

         RemoveUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

         s.Shape_Square(playerID, 1, "Protoss Reaver", x, y);
         s.Shape_Square(playerID, 1, "60 + 1n Danimoth", x, y);
         s.Shape_Square(playerID, 1, "Protoss Dark Archon", x, y);

         KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "Protoss Reaver", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 20)
         {
            KillUnitAt(All, "Protoss Reaver", "Anywhere", playerID);
            RemoveUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

            s.Voice_Routine(playerID, 19);            

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4) //9.90
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.Shape_NxNSquareWithProperty(playerID, 1, "50 + 1n Battlecruiser", 3, 75, 1);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            s.Shape_NxNSquareWithProperty(playerID, 1, "50 + 1n Battlecruiser", 3, 75, 1);

            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.Voice_Routine(playerID, 20);            

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5) //10.70
      {
         if (v.P_LoopMain[playerID] < 8)
         {
            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               s.Table_Sin(playerID, (dwrand() % 36 * 10), 100 + dwrand() % 100);
               s.Table_Cos(playerID, (dwrand() % 36 * 10), 100 + dwrand() % 100);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               Shape_NxNSquareAtDouble(playerID, 1, "40 + 1n Gantrithor", 3, 50, x, y);
               Shape_NxNSquareAtDouble(playerID, 1, "50 + 1n Tank", 2, 75, x, y);

               for (var i = 0; i < 8; i++)
               {
                  s.MoveLoc("50 + 1n Tank", playerID, 0, 0);
                  RemoveUnitAt(1, "50 + 1n Tank", "Anywhere", playerID);
                  MoveUnit(1, "60 + 1n Siege", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
               }
            }

            if (v.P_LoopMain[playerID] % 2 == 1)
            {
               s.Table_Sin(playerID, (dwrand() % 36 * 10), 100 + dwrand() % 100);
               s.Table_Cos(playerID, (dwrand() % 36 * 10), 100 + dwrand() % 100);

               x = v.P_AngleCos[playerID];
               y = v.P_AngleSin[playerID];

               Shape_NxNSquareAtDouble(playerID, 1, "50 + 1n Battlecruiser", 3, 50, x, y);
               Shape_NxNSquareAtDouble(playerID, 1, "Protoss Reaver", 2, 75, x, y);

               KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
               KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

               ModifyUnitHangarCount(1, All, "Protoss Reaver", CurrentPlayer, "Anywhere");

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               MoveUnit(All, "Protoss Reaver", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            }
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 18)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 6)    //9.90
      {
         KillUnitAt(All, "Protoss Reaver", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}

function Shape_NxNSquareAt(cp : TrgPlayer, count, Unit : TrgUnit, n, interval, x, y)
{
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, x, y);
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, -x, -y);
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, -y, x);
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, y, -x);
}

function Shape_NxNSquareAtDouble(cp : TrgPlayer, count, Unit : TrgUnit, n, interval, x, y)
{
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, x, y);
   s.Shape_NxNSquareAt(playerID, count, Unit, n, interval, -x, -y);
}


