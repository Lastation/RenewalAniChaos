import variable as v;
import func.trig as trg;

var x = 0;
var y = 0;

function main(playerID)
{
   MoveUnit(All, "40 + 3n Zeratul", playerID, "Anywhere", "[Skill]HoldPosition");

   trg.Debuff_BanReturn();
   trg.Debuff_Stop();

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Edge(playerID, 1, "Unclean One (Defiler)", 45, 3, 32);

            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 3, 50);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 50);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 50);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         }
         /*
         if (v.P_LoopMain[playerID] < 8)
         {
            if (v.P_LoopMain[playerID] == 0) { x = 32; y = 0; }
            if (v.P_LoopMain[playerID] == 1) { x = 32; y = 32; }
            if (v.P_LoopMain[playerID] == 2) { x = 0; y = 32; }
            if (v.P_LoopMain[playerID] == 3) { x = -32; y = 32; }
            if (v.P_LoopMain[playerID] == 4) { x = -32; y = 0; }
            if (v.P_LoopMain[playerID] == 5) { x = -32; y = -32; }
            if (v.P_LoopMain[playerID] == 6) { x = 0; y = -32; }
            if (v.P_LoopMain[playerID] == 7) { x = 32; y = -32; }

            epic.Shape_Dot(playerID, 1, "40 + 1n Lurker", x, y, 0);
         }
         */
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 8)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n Hydralisk", 100, 100);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 100);
            epic.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 100, 1);

            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 100, 0);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "60 + 1n Hydralisk", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("60 + 1n Hydralisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n Hydralisk", 100, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 100, 0);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 0);
            epic.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0, 1);

            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 100, 100);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "60 + 1n Hydralisk", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("60 + 1n Hydralisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 19)
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 9, 50);
            trg.Shape_Edge(playerID, 1, "Protoss Dark Templar", 45, 3, 150);
            trg.Shape_Edge(playerID, 1, "Protoss Dark Templar", 45, 5, 200);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("Protoss Dark Templar", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 3n Zeratul", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

         }
         if (v.P_LoopMain[playerID] == 9)
         {
            s.CharacterVoice(8);
         }


         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 32)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mutalisk", 3, 50);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 50);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 3)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mutalisk", 3, 50);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 50);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 7)
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (Bring(playerID, AtLeast, 2, "Protoss Arbiter", "[Skill]UseSkill")
            )
         {
            s.CharacterVoice(9);
            v.P_SkillDelay[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            v.P_Step[playerID] = 230;
            KillUnitAt(2, "Protoss Arbiter", "[Skill]UseSkill", playerID);
         }
         else
         {
            KillUnitAt(All, "40 + 3n Zeratul", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Hydralisk", "Anywhere", playerID);
            KillUnitAt(All, "Unclean One (Defiler)", "Anywhere", playerID);

            trg.SkillEnd();
         }
      }
   }
}