function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 197;
	v.P_UnitID[playerID] = 53;
	
	v.P_Ultimate1[playerID] = 550;
	v.P_Ultimate2[playerID] = 200;
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCAA.main(playerID);
			break;
		case 220:
			CCC.main(playerID);
			break;
		case 230:
			CCCAA.main(playerID);
			break;
		case 310:
			AACS.main(playerID);
			break;
		case 320:
			AACSAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

