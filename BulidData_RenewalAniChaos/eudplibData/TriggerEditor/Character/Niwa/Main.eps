import PluginVariables as msqcvar;
import Function as f;

import Character.Niwa.Skill_O as O;
import Character.Niwa.Skill_S as S;
import Character.Niwa.Skill_C as C;
import Character.Niwa.Skill_A as A;

import Character.Niwa.Skill_SSS as SSS;
import Character.Niwa.Skill_CCC as CCC;
import Character.Niwa.Skill_CCCAA as CCCAA;
import Character.Niwa.Skill_CCAA as CCAA;
import Character.Niwa.Skill_AACS as AACS;
import Character.Niwa.Skill_AACSAA as AACSAA;
import Character.Niwa.Text as text;
import Character.Niwa.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 197;
	f.heroID[cp] = 53;
	
	f.UltimateA[cp] = 550;
	f.UltimateB[cp] = 200;
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCAA.main(cp);
			break;
		case 220:
			CCC.main(cp);
			break;
		case 230:
			CCCAA.main(cp);
			break;
		case 310:
			AACS.main(cp);
			break;
		case 320:
			AACSAA.main(cp);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Niwa_Voice[cp] != 0) { text.main(cp); }
}

