import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 8)
         {
            s.Shape_Dot(playerID, 1, "40 + 1n Gantrithor", 0, 0);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 8)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {
            var x = 150;
            var y = 50 * v.P_LoopMain[playerID];

            for (var i = 0; i < 4; i++)
            {
               s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", x - 50 * i, y);
            }
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 75, 75);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 75, 150);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 150, 75);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 150, 150);

            s.Shape_Square(playerID, 1, "60 + 1n Dragoon", 75, 75);
            s.Shape_Square(playerID, 1, "60 + 1n Dragoon", 75, 150);
            s.Shape_Square(playerID, 1, "60 + 1n Dragoon", 150, 75);
            s.Shape_Square(playerID, 1, "60 + 1n Dragoon", 150, 150);

            s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 0, 75);
            s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 0, 150);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 8)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 5)
         {
            KillUnitAt(4, "40 + 1n Mojo", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] == 2) KillUnitAt(4, "60 + 1n Danimoth", "Anywhere", playerID);
            else
            {
               KillUnitAt(1, "60 + 1n Danimoth", "Anywhere", playerID);
               KillUnitAt(4, "60 + 1n Dragoon", "Anywhere", playerID);
            }
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 5)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {

         s.SkillEnd();
      }
   }
}