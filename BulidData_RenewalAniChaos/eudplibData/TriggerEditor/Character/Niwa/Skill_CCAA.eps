import Function as f;

var x = 0;
var y = 0;

var d = 75;

function main(cp)
{
   MoveUnit(All, "60 + 1n High Templar", cp, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "60 + 1n Dragoon", cp, "Anywhere", "[Skill]HoldPosition");

   f.BanReturn(cp);
   f.HoldPosition(cp);

   if (f.delay[cp] == 0)
   {
      if (f.count[cp] == 0)
      {
         if (f.loop[cp] == 0)
         {
            f.Table_Sin(cp, 22, d);
            f.Table_Cos(cp, 22, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);

            f.Table_Sin(cp, 67, d);
            f.Table_Cos(cp, 67, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         }
         if (f.loop[cp] == 2)
         {
            f.Table_Sin(cp, 22, 3 * d);
            f.Table_Cos(cp, 22, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "60 + 1n Siege", x, y);
            f.SquareShape(cp, 1, "40 + 1n Gantrithor", x, y);

            f.Table_Sin(cp, 67, 3 * d);
            f.Table_Cos(cp, 67, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "60 + 1n Siege", x, y);
            f.SquareShape(cp, 1, "40 + 1n Gantrithor", x, y);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", cp);
         
         }
         if (f.loop[cp] == 4)
         {
            f.Table_Sin(cp, 22, 2 * d);
            f.Table_Cos(cp, 22, 2 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);

            f.Table_Sin(cp, 67, 2 * d);
            f.Table_Cos(cp, 67, 2 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         }
         if (f.loop[cp] < 8)
         {
            f.Table_Sin(cp, 45 * f.loop[cp], d);
            f.Table_Cos(cp, 45 * f.loop[cp], d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.DotShape(cp, 1, "60 + 1n Dragoon", x, y);
         }

         f.Table_Sin(cp, 45 * f.loop[cp], 2 * d);
         f.Table_Cos(cp, 45 * f.loop[cp], 2 * d);

         x = f.CosAngle[cp];
         y = f.SinAngle[cp];

         f.DoubleShape(cp, 1, "50 + 1n Battlecruiser", x, y);
         f.DoubleShape(cp, 1, "60 + 1n Archon", x, y);
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);

         f.Table_Sin(cp, 45 * f.loop[cp], d);
         f.Table_Cos(cp, 45 * f.loop[cp], d);

         x = f.CosAngle[cp];
         y = f.SinAngle[cp];

         f.DoubleShape(cp, 1, "Kakaru (Twilight)", x, y);
         f.DoubleShape(cp, 1, "Protoss Dark Archon", x, y);
         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);

         if (f.loop[cp] % 2 == 0)
         {
            f.DotShape(cp, 1, "40 + 1n Guardian", 0, 0);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 60)
         {
            KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);

            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 1)
      {
         if (f.loop[cp] == 0)
         {
            f.Table_Sin(cp, 22, d);
            f.Table_Cos(cp, 22, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n High Templar", x, y);

            f.Table_Sin(cp, 67, d);
            f.Table_Cos(cp, 67, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n High Templar", x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         }
         if (f.loop[cp] == 2)
         {
            f.Table_Sin(cp, 22, 3 * d);
            f.Table_Cos(cp, 22, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n Archon", x, y);

            f.Table_Sin(cp, 67, 3 * d);
            f.Table_Cos(cp, 67, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n Archon", x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);
         }
         if (f.loop[cp] == 4)
         {
            f.Table_Sin(cp, 22, d);
            f.Table_Cos(cp, 22, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);

            f.Table_Sin(cp, 67, d);
            f.Table_Cos(cp, 67, d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            f.Table_Sin(cp, 0, 3 * d);
            f.Table_Cos(cp, 0, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "60 + 1n Siege", x, y);
            f.SquareShape(cp, 1, "50 + 1n Tank", x, y);
            f.SquareShape(cp, 1, "50 + 1n Battlecruiser", x, y);

            f.Table_Sin(cp, 45, 3 * d);
            f.Table_Cos(cp, 45, 3 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "60 + 1n Siege", x, y);
            f.SquareShape(cp, 1, "50 + 1n Tank", x, y);
            f.SquareShape(cp, 1, "50 + 1n Battlecruiser", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);
         }
         if (f.loop[cp] < 8)
         {
            f.Table_Sin(cp, 45 * f.loop[cp], 2 * d);
            f.Table_Cos(cp, 45 * f.loop[cp], 2 * d);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.DotShape(cp, 1, "60 + 1n Dragoon", x, y);
         }

         f.Table_Sin(cp, 45 * f.loop[cp], d);
         f.Table_Cos(cp, 45 * f.loop[cp], d);

         x = f.CosAngle[cp];
         y = f.SinAngle[cp];

         f.DoubleShape(cp, 1, "40 + 1n Zealot", x, y);
         KillUnitAt(All, "40 + 1n Zealot", "Anywhere", cp);

         f.Table_Sin(cp, 45 * f.loop[cp], 2 * d);
         f.Table_Cos(cp, 45 * f.loop[cp], 2 * d);

         x = f.CosAngle[cp];
         y = f.SinAngle[cp];

         f.DoubleShape(cp, 1, "40 + 1n Gantrithor", x, y);
         f.DoubleShape(cp, 1, "Protoss Dark Archon", x, y);
         KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", cp);
         KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);

         if (f.loop[cp] % 2 == 0)
         {
            f.DotShape(cp, 1, "40 + 1n Guardian", 0, 0);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 47)
         {
            f.Voice_Routine(cp, 10);
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }

      else if (f.count[cp] == 2)
      {
         if (f.loop[cp] < 8)
         {
            f.MoveLoc("60 + 1n Siege", cp, 0, 0);
            RemoveUnitAt(1, "60 + 1n Siege", "Anywhere", cp);
            f.SkillUnit(cp, 1, "60 + 1n Archon");
            f.SkillUnit(cp, 1, "40 + 1n Guardian");
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Drone", 1);
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Lurker", 0);

            f.MoveLoc("60 + 1n Siege", cp, 0, 0);
            RemoveUnitAt(1, "60 + 1n Siege", "Anywhere", cp);
            f.SkillUnit(cp, 1, "60 + 1n Archon");
            f.SkillUnit(cp, 1, "40 + 1n Guardian");
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Drone", 1);
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Lurker", 0);

            f.MoveLoc("60 + 1n Dragoon", cp, 0, 0);
            RemoveUnitAt(1, "60 + 1n Dragoon", "Anywhere", cp);
            f.SkillUnit(cp, 1, "60 + 1n Archon");
            f.SkillUnit(cp, 1, "40 + 1n Guardian");
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Drone", 1);
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Lurker", 0);

            f.MoveLoc("60 + 1n High Templar", cp, 0, 0);
            RemoveUnitAt(1, "60 + 1n High Templar", "Anywhere", cp);
            f.SkillUnit(cp, 1, "60 + 1n Archon");
            f.SkillUnit(cp, 1, "40 + 1n Guardian");
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Drone", 1);
            f.SkillUnitWithProperty(cp, 1, "40 + 1n Lurker", 0);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            Order("40 + 1n Drone", cp, "Anywhere", Attack, f.location[cp]);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 36)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 3)
      {
         if (f.loop[cp] == 0)
         {
            for (var i = 0; i < 32; i++)
            {
               f.MoveLoc("40 + 1n Lurker", cp, 0, 0);
               RemoveUnitAt(1, "40 + 1n Lurker", "Anywhere", cp);
               f.SkillUnit(cp, 1, "60 + 1n Danimoth");
               if (i % 2 == 0)
                  f.SkillUnit(cp, 1, "Protoss Dark Archon");
               else
                  f.SkillUnit(cp, 1, "60 + 1n Archon");
            }

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("60 + 1n Danimoth", cp, "Anywhere", Attack, f.location[cp]);
         }
         if (f.loop[cp] == 1)
         {
            for (var i = 0; i < 32; i++)
            {
               f.MoveLoc("60 + 1n Danimoth", cp, 0, 0);
               RemoveUnitAt(1, "60 + 1n Danimoth", "Anywhere", cp);
               f.SkillUnit(cp, 1, "Kakaru (Twilight)");
            }

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         }

         if (f.loop[cp] == 2)
         {
            for (var i = 0; i < 32; i++)
            {
               f.MoveLoc("40 + 1n Drone", cp, 0, 0);
               RemoveUnitAt(1, "40 + 1n Drone", "Anywhere", cp);
               if (i % 2 == 0)
                  f.SkillUnit(cp, 1, "40 + 1n Mojo");
               f.SkillUnit(cp, 1, "60 + 1n Hydralisk");
            }

            KillUnitAt(All, "60 + 1n Hydralisk", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
         }
         if (f.loop[cp] == 3)
         {
            for (var i = 0; i < 16; i++)
            {
               f.MoveLoc("40 + 1n Mojo", cp, 0, 0);
               RemoveUnitAt(1, "40 + 1n Mojo", "Anywhere", cp);
               f.SkillUnit(cp, 1, "Kakaru (Twilight)");
            }

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         }


         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 4)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 4)
      {
         f.SkillEnd(cp);
      }
   }
}