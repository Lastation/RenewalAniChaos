               EdgeShapeAt(playerID, 1, "60 + 1n Archon", 0, 2, 75, x, y);
            }
            GiveUnits(All, "40 + 1n Mojo", P7, "Anywhere", playerID);
            GiveUnits(All, "40 + 1n Mojo", P8, "Anywhere", playerID);
