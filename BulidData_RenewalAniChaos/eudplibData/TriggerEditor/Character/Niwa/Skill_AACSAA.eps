         if (v.P_LoopMain[playerID] == 29)
         {
            for (var i = 0; i < 4; i++)
            {
               s.Table_Sin(playerID, 22 + 22 * i, 400);
               s.Table_Cos(playerID, 22 + 22 * i, 400);

               var x = v.P_AngleCos[playerID];
               var y = v.P_AngleSin[playerID];

               EdgeShapeAt(playerID, 1, "40 + 1n Gantrithor", 0, 2, 75, x, y);
