         if (v.P_LoopMain[playerID] == 25)
         {
            for (var i = 0; i < 2; i++)
            {
               s.Table_Sin(playerID, 22 + 45 * i, 200);
               s.Table_Cos(playerID, 22 + 45 * i, 200);

