         }
         if (Bring(playerID, AtLeast, 1, "60 + 1n Hydralisk", "Anywhere"))
         {
            for (var i = 0; i < 3; i++)
            {
               s.MoveLoc("60 + 1n Hydralisk", playerID, 0, 0);
               RemoveUnitAt(1, "60 + 1n Hydralisk", "Anywhere", playerID);
               if (i % 3 == 0) 
               {
                  s.SkillUnit(playerID, 1, "40 + 1n Wraith");
                  s.SkillUnit(playerID, 1, "50 + 1n Tank");
               }
               else if (i % 3 == 1)
               {
                  s.SkillUnit(playerID, 1, "40 + 1n Mutalisk");
