            s.Shape_Square(playerID, 1, "40 + 1n Mojo", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            s.EdgeShape(playerID, 1, "100 + 1n Dragoon", 0, 5, 150);
            KillUnitAt(All, "100 + 1n Dragoon", "Anywhere", playerID);

            s.Table_Sin(playerID, 22, 275);
            s.Table_Cos(playerID, 22, 275);

            s.Shape_Square(playerID, 1, "60 + 1n Danimoth", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(1120);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 14)
         {       
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            if ((v.P_LoopMain[playerID] / 4) % 2 == 0)
            { 
               s.Shape_Dot(playerID, 1, "40 + 1n Wraith", 160 - 40 * (v.P_LoopMain[playerID] % 4) , 40 * (v.P_LoopMain[playerID] % 4));
