            Order("60 + 1n Dragoon", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {
            s.EdgeShape(playerID, 1, "100 + 1n Dragoon", 45, 5, 150);
