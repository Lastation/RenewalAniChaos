function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 185;
	v.P_CharacterID[playerID] = 64;
	
	s.UltimateA[playerID] = 700;
	s.UltimateB[playerID] = 500;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 230:
			CCA.main(playerID);
			break;
		case 240:
			CCAAA.main(playerID);
			break;
		case 250:
			CCAAAAA.main(playerID);
			break;
		case 260:
			UltA.main(playerID);
			break;
		case 310:
			UltB.main(playerID);
			break;
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Nanami_Voice[playerID] != 0) { text.main(playerID); }
}

