import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            s.Shape_Square(playerID, 1, "60 + 1n High Templar", 50, 50);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 50);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "60 + 1n High Templar", 50, 50);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 50);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 100);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 150, 150);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 50);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Goliath", 50, 50);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "Protoss Dark Templar", 120 - 30 * v.P_LoopMain[playerID], 30 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            s.Shape_Square(playerID, 1, "40 + 1n Zealot", 120, 0);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Zealot", 80, 40);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Zealot", 40, 80);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {         
            s.Shape_Square(playerID, 1, "60 + 1n High Templar", 120, 0);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 120, 0);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {         
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "60 + 1n High Templar", 80, 40);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 80, 40);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {         
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "60 + 1n High Templar", 40, 80);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", 40, 80);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}