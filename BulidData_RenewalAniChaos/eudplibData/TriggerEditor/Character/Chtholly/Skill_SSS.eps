import PluginVariables as msqcvar;
import Function as f;

function main(cp)
{
	if (f.delay[cp] == 0)
	{
		if (f.count[cp] < 4)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
		
			f.SquareShape(cp, 1, "40 + 1n Mojo", 16 * f.count[cp] + 32, 16 * f.count[cp] + 32);
			f.SquareShape(cp, 1, "40 + 1n Zealot", 16 * f.count[cp] + 32, 16 * f.count[cp] + 32);
			MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
			KillUnitAt(All, "40 + 1n Zealot", "Anywhere", cp);
			Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 150);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 4)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 5 || f.count[cp] == 6)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", cp);
			
			f.SquareShape(cp, 1, "40 + 1n Mojo", 116 - 32 * (f.count[cp] - 5), 0);
			f.SquareShape(cp, 1, "40 + 1n Marine", 116 - 32 * (f.count[cp] - 5), 0);
			MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			Order("40 + 1n Marine", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 500);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 7)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", cp);
			
			f.SquareShape(cp, 1, "50 + 1n Tank", 90, 90);
			f.SquareShape(cp, 1, "50 + 1n Tank", 128, 0);
			MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
			Order("50 + 1n Tank", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 740);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 8)
		{
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);
			f.SkillEnd(cp);
		}
	}
}