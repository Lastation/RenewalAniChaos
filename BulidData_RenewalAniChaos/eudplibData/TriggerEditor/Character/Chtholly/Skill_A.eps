import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
	if (v.P_Delay[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			s.CrossShape(playerID, 1, "40 + 1n Wraith", 45, 3, 64);
			s.CrossShape(playerID, 1,"60 + 1n Dragoon", 45, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
		
			s.LineShape(playerID, 1, "40 + 1n Mojo", 45, 3, 64, 64);
			s.LineShape(playerID, 1, "40 + 1n Mojo", 225, 3, 64, 64);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 45, 3, 64, 64);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 225, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
		
			s.LineShape(playerID, 1, "40 + 1n Mojo", 45, 1, 64, 128);
			s.LineShape(playerID, 1, "40 + 1n Mojo", 225, 1, 64, 128);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 45, 1, 64, 128);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 225, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
		
			s.LineShape(playerID, 1, "40 + 1n Mutalisk", 135, 3, 64, 64);
			s.LineShape(playerID, 1, "40 + 1n Mutalisk", 315, 3, 64, 64);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 135, 3, 64, 64);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 315, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
		
			s.LineShape(playerID, 1, "40 + 1n Mutalisk", 135, 1, 64, 128);
			s.LineShape(playerID, 1, "40 + 1n Mutalisk", 315, 1, 64, 128);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 135, 1, 64, 128);
			s.LineShape(playerID, 1, "60 + 1n Dragoon", 315, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		if (v.P_CountMain[playerID] == 5)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
			
			s.CrossShape(playerID, 1, "40 + 1n Wraith", 0, 3, 64);
			s.CrossShape(playerID, 1,"60 + 1n Dragoon", 0, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 6)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			s.SkillEnd();
		}
	}
}