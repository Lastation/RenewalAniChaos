import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
	s.BanReturn(playerID);
	
	if (v.P_CountMain[playerID] > 2)
	{
		s.Voice_Routine(playerID, 14);
	}

	if (v.P_Delay[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 0)
		{
			s.SkillWait(1000);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 1)
		{
			s.Voice_Routine(playerID, 12);
			s.SkillWait(3500);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 2)
		{
			s.Voice_Routine(playerID, 13);
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] <= 4)
		{
			s.Shape_NxNSquare(playerID, 1, "40 + 1n Gantrithor", (v.P_LoopMain[playerID] + 1), 64);
			KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
			s.SkillWait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] == 5)
		{
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			s.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 0);
			s.Shape_Square(playerID, 6, "50 + 1n Tank", 96, 192);
			s.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 96);
			s.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 192);
			s.Shape_Square(playerID, 4, "40 + 1n Goliath", 192, 0);
			s.Shape_Square(playerID, 4, "40 + 1n Goliath", 192, 192);
			s.Shape_Square(playerID, 12, "40 + 1n Marine", 0, 0);
			s.Shape_Square(playerID, 4, "40 + 1n Marine", 96, 192);
			s.Shape_Square(playerID, 4, "40 + 1n Marine", 192, 96);
			s.Shape_Square(playerID, 4, "40 + 1n Marine", 192, 192);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			MoveUnit(All, "(any unit)", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
			Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] <= 27)
		{
			s.Table_Cos(playerID, 15 * v.P_LoopMain[playerID], 256);
			s.Table_Sin(playerID, 15 * v.P_LoopMain[playerID], 256);
			s.Shape_Square(playerID, 1, "80 + 1n Artanis", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			s.Shape_Square(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("80 + 1n Artanis", playerID, "Anywhere", Move, v.P_LocationID[playerID]);
			KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
			s.SkillWait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] == 28)
		{
			s.SkillWait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			s.SkillWait(1500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 5)
		{
			if (cp < 3)	{ GiveUnits(All, "80 + 1n Artanis", playerID, "Anywhere", 6); }
			else 		{ GiveUnits(All, "80 + 1n Artanis", playerID, "Anywhere", 7); }
			SetSwitch("JunkYardDog", Set);
			s.SkillWait(250);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 6)
		{
			SetSwitch("JunkYardDog", Clear);
			s.SkillWait(1500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 7)
		{
			SetSwitch("JunkYardDog", Set);
			s.SkillWait(250);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 7)
		{
			SetSwitch("JunkYardDog", Clear);
			s.SkillWait(1000);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 8)
		{
			if (cp < 3)	{ GiveUnits(All, "80 + 1n Artanis", 6, "Anywhere", playerID); }
			else 		{ GiveUnits(All, "80 + 1n Artanis", 7, "Anywhere", playerID); }
			
			if (v.P_LoopMainB == 1)
			{ 
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 6);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 7);
			}
			
			s.MoveLoc(v.P_CharacterID[playerID], playerID, 0, 0);
			Order("80 + 1n Artanis", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			s.SkillWait(3500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 9)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			s.SkillWait(800);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 10)
		{
			KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
			v.P_LoopMainB[playerID] = 0;
			s.SkillEnd();
		}
	}
}