import Function as f;

function main(cp)
{
	f.BanReturn(cp);
	
	if (f.count[cp] > 2)
	{
		f.Voice_Routine(cp, 14);
	}

	if (f.delay[cp] == 0)
	{
		if (f.count[cp] == 0 && f.loop[cp] == 0)
		{
			f.SkillWait(cp, 1000);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 0 && f.loop[cp] == 1)
		{
			f.Voice_Routine(cp, 12);
			f.SkillWait(cp, 3500);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 0 && f.loop[cp] == 2)
		{
			f.Voice_Routine(cp, 13);
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 1 && f.loop[cp] <= 4)
		{
			f.NxNSquareShape(cp, 1, "40 + 1n Gantrithor", (f.loop[cp] + 1), 64);
			KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 1 && f.loop[cp] == 5)
		{
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 2)
		{
			f.SquareShape(cp, 6, "50 + 1n Tank", 192, 0);
			f.SquareShape(cp, 6, "50 + 1n Tank", 96, 192);
			f.SquareShape(cp, 6, "50 + 1n Tank", 192, 96);
			f.SquareShape(cp, 6, "50 + 1n Tank", 192, 192);
			f.SquareShape(cp, 4, "40 + 1n Goliath", 192, 0);
			f.SquareShape(cp, 4, "40 + 1n Goliath", 192, 192);
			f.SquareShape(cp, 12, "40 + 1n Marine", 0, 0);
			f.SquareShape(cp, 4, "40 + 1n Marine", 96, 192);
			f.SquareShape(cp, 4, "40 + 1n Marine", 192, 96);
			f.SquareShape(cp, 4, "40 + 1n Marine", 192, 192);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			MoveUnit(All, "(any unit)", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
			Order("50 + 1n Tank", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 3 && f.loop[cp] <= 27)
		{
			f.Table_Cos(cp, 15 * f.loop[cp], 256);
			f.Table_Sin(cp, 15 * f.loop[cp], 256);
			f.SquareShape(cp, 1, "80 + 1n Artanis", f.CosAngle[cp], f.SinAngle[cp]);
			f.SquareShape(cp, 1, "Protoss Dark Archon", f.CosAngle[cp], f.SinAngle[cp]);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("80 + 1n Artanis", cp, "Anywhere", Move, f.location[cp]);
			KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 3 && f.loop[cp] == 28)
		{
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 4)
		{
			f.SkillWait(cp, 1500);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 5)
		{
			if (cp < 3)	{ GiveUnits(All, "80 + 1n Artanis", cp, "Anywhere", 6); }
			else 		{ GiveUnits(All, "80 + 1n Artanis", cp, "Anywhere", 7); }
			SetSwitch("JunkYardDog", Set);
			f.SkillWait(cp, 250);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 6)
		{
			SetSwitch("JunkYardDog", Clear);
			f.SkillWait(cp, 1500);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 7)
		{
			SetSwitch("JunkYardDog", Set);
			f.SkillWait(cp, 250);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 7)
		{
			SetSwitch("JunkYardDog", Clear);
			f.SkillWait(cp, 1000);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 8)
		{
			if (cp < 3)	{ GiveUnits(All, "80 + 1n Artanis", 6, "Anywhere", cp); }
			else 		{ GiveUnits(All, "80 + 1n Artanis", 7, "Anywhere", cp); }
			
			if (f.loopB == 1)
			{ 
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", cp);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 6);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 7);
			}
			
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("80 + 1n Artanis", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 3500);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 9)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", cp);
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", cp);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);
			f.SkillWait(cp, 800);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 10)
		{
			KillUnitAt(All, "80 + 1n Artanis", "Anywhere", cp);
			f.loopB[cp] = 0;
			f.SkillEnd(cp);
		}
	}
}