import Character.Chtholly.Text as text;
import Character.Chtholly.Commend as commend;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 117;
	v.P_CharacterID[playerID] = 77;
	
	s.UltimateA[playerID] = 700;
	s.UltimateB[playerID] = 700;
	
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			//O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 210:
			CCA.main(playerID);
			break;
		case 220:
			CCACC.main(playerID);
			break;
		case 230:
			CCACCAAA.main(playerID);
			break;
		case 240:
			Uiltimate.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		}
	}
	
}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Chtholly_Voice[playerID] != 0) { text.main(playerID); }
}

