import PluginVariables as msqcvar;
import func.shape as s;

const s = StringBuffer();

function main(playerID)
{
   if (Switch("Unique - Milim", Cleared))
   {
      s.HoldPosition(playerID);
   }
   s.BanReturn(playerID);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            var x = 75;
            var y = 125;

            s.Shape_Square(playerID, 1, "40 + 1n Mojo", x, y);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", y, x);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", x, y);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", y, x);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "Target", 45, 5, 100);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            var x = 125;
            var y = 175;

            s.Shape_Square(playerID, 1, "40 + 1n Mojo", x, y);
            s.Shape_Square(playerID, 1, "40 + 1n Mojo", y, x);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", x, y);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", y, x);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "Target", 0, 5, 100);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            var x = 75;
            var y = 125;

            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", x, y);
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", y, x);

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            x = 125;
            y = 175;

            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", x, y);
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", y, x);

            s.EdgeShape(playerID, 1, "40 + 1n Wraith", 0, 5, 100);
            s.EdgeShape(playerID, 1, "40 + 1n Firebat", 0, 3, 100);

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Firebat", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Firebat", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 7)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Mojo", 45 * (v.P_LoopMain[playerID] % 4), 5, 64, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 45 * (v.P_LoopMain[playerID] % 4), 5, 64, 0);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(240);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            s.SkillWait(320);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            s.Table_Sin(playerID, 22, 75);
            s.Table_Cos(playerID, 22, 75);

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            s.Table_Sin(playerID, 67, 125);
            s.Table_Cos(playerID, 67, 125);

            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            s.Table_Sin(playerID, 22, 175);
            s.Table_Cos(playerID, 22, 175);

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            s.Table_Sin(playerID, 67, 225);
            s.Table_Cos(playerID, 67, 225);

            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);


            s.SkillWait(160);
            
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            s.SkillWait(480);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.Table_Sin(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 75);
            s.Table_Cos(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 75);

            s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 7, "40 + 1n Zealot", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 8)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.Table_Sin(playerID, 22 + 22 * (v.P_LoopMain[playerID] % 4), 225);
            s.Table_Cos(playerID, 22 + 22 * (v.P_LoopMain[playerID] % 4), 225);

            s.Shape_Square(playerID, 1, "80 + 1n Tank", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 12)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.Table_Sin(playerID, 22 + 22 * (v.P_LoopMain[playerID] % 4), 225);
            s.Table_Cos(playerID, 22 + 22 * (v.P_LoopMain[playerID] % 4), 225);

            s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 12)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.SkillWait(640);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Firebat", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}