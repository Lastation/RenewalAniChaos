function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 188;
	v.P_CharacterID[playerID] = 54;
	
	s.UltimateA[playerID] = 650;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCAA.main(playerID);
			break;
		case 220:
			CCC.main(playerID);
			break;
		case 310:
			AAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

