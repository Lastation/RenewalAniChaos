function MargeSound(playerID)
{
	
	if (getuserplayerid() >= 128)
	{
		//s.stb.printAt(0, "\x04cp = ", playerID, "  getuserplayerid() = ", getuserplayerid());
		//s.stb.printAt(1, "\x04v.Sound_Text1[", playerID, "] = ", v.Sound_Text1[playerID], "  v.Sound_Text2[", playerID, "] = ", v.Sound_Text2[playerID]);
		//s.stb.printAt(2, "\x04v.Sound_Text3[", playerID, "] = ", v.Sound_Text3[playerID], "  v.Sound_Text4[", playerID, "] = ", v.Sound_Text4[playerID]);
		//s.stb.printAt(3, "\x04v.Sound_Text_Uilti[", playerID, "] = ", v.Sound_Text_Uilti[playerID], "  v.Sound_Text_Uniq[", playerID, "] = ", v.Sound_Text_Uniq[playerID]);
	}
	
	
	// 1 ~ 6
	if (v.Sound_Text1[playerID] > 0)
	{
		maihime.NormalText(playerID);
		tenshi.NormalText(playerID);
		miyuki.NormalText(playerID);
		negev.NormalText(playerID);
	}
	// 7 ~ 12
	if (v.Sound_Text2[playerID] > 0)
	{
		cecillia.NormalText(playerID);
		anzu.NormalText(playerID);
		hotomoka.NormalText(playerID);
		alterego.NormalText(playerID);
		//kurumi.NormalText(playerID);
		ryuzu.NormalText(playerID);
	}
	
	// 13 ~ 18
	if (v.Sound_Text3[playerID] > 0)
	{
		sakura.NormalText(playerID);
		yashiro.NormalText(playerID);
		rin.NormalText(playerID);
		seraphim.NormalText(playerID);
		tenkei.NormalText(playerID);
	}
		
	// 19 ~ 24
	if (v.Sound_Text4[playerID] > 0)
	{
		mn.NormalText(playerID);
		emet.NormalText(playerID);
		magellan.NormalText(playerID);
	}
	
	if (v.Sound_Text_Uilti[playerID] > 0)
	{
		// 1 ~ 6
		maihime.UiltimateText(playerID);
		tenshi.UiltimateText(playerID);
		miyuki.UiltimateText(playerID);
		negev.UiltimateText(playerID);
		
		// 7 ~ 12
