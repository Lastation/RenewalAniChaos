		anzu.UiltimateText(playerID);
		hotomoka.UiltimateText(playerID);
		alterego.UiltimateText(playerID);
		//kurumi.UiltimateText(playerID);
		ryuzu.UiltimateText(playerID);
		
		// 13 ~ 18
		sakura.UiltimateText(playerID);
		yashiro.UiltimateText(playerID);
		rin.UiltimateText(playerID);
		seraphim.UiltimateText(playerID);
		tenkei.UiltimateText(playerID);
		
		// 19 ~ 24
		mn.UiltimateText(playerID);
		emet.UiltimateText(playerID);
		magellan.UiltimateText(playerID);
	}
	
	if (v.Sound_Text_Uniq[playerID] > 0)
	{
		// 1 ~ 6
		maihime.UniqueText(playerID);
		tenshi.UniqueText(playerID);
		miyuki.UniqueText(playerID);
		negev.UniqueText(playerID);
		
		// 7 ~ 12// 7 ~ 12
		cecillia.UniqueText(playerID);
		anzu.UniqueText(playerID);
		hotomoka.UniqueText(playerID);
		alterego.UniqueText(playerID);
		//kurumi.UniqueText(playerID);
		ryuzu.UniqueText(playerID);
		
		// 13 ~ 18// 13 ~ 18
		sakura.UniqueText(playerID);
		yashiro.UniqueText(playerID);
		rin.UniqueText(playerID);
		seraphim.UniqueText(playerID);
		tenkei.UniqueText(playerID);
		
		// 19 ~ 24// 13 ~ 18
		mn.UniqueText(playerID);
		emet.UniqueText(playerID);
		magellan.UniqueText(playerID);
	}
}

