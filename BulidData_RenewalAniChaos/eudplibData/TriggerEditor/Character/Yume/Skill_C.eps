import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {                        
            s.EdgeShape(playerID, 1, "40 + 1n Mutalisk", 0, 3, 50);
            s.EdgeShape(playerID, 1, "Protoss Dark Archon", 0, 3, 50);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {                        
            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 0, 5, 100);
            s.EdgeShape(playerID, 1, "40 + 1n Marine", 0, 3, 100);
            s.EdgeShape(playerID, 1, "50 + 1n Tank", 0, 5, 100);
            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 0, 3, 50);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 2)
         {         
            var x = 0;
            var y = 0;

            if (v.P_LoopMain[playerID] == 0) { x = 150; y = 100; }
            else if (v.P_LoopMain[playerID] == 1) { x = 100; y = 50; }

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", x, y);
            s.Shape_Square(playerID, 1, "40 + 1n Guardian", y, x);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;

         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 100);
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 100, 50);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 50, 100);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 100, 50);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;

         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 100);
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 50);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 50, 100);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 100, 50);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;

         }

      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}