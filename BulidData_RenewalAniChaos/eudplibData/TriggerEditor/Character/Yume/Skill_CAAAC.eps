import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   ModifyUnitShields(All, v.P_CharacterID[playerID], playerID, "Anywhere", 1);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 3)
         {             
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");

            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID], 9, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID], 9, 50, 75);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID] + 180, 9, 50, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID], 7, 75, 0);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID], 7, 75, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID] + 180, 7, 75, 75);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID], 9, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID], 9, 50, 75);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 45 * v.P_LoopMain[playerID] + 180, 9, 50, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID], 7, 75, 0);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID], 7, 75, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 45 * v.P_LoopMain[playerID] + 180, 7, 75, 75);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 8)
         {             
            if (v.P_LoopMain[playerID] == 0)
            {
               s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 45, 7, 75, 0);
               s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 45, 7, 75, 75);
               s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 225, 7, 75, 75);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 45, 7, 75, 0);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 45, 7, 75, 75);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 225, 7, 75, 75);

               KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
            }
            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

               s.Shape_Square(playerID, 1, "40 + 1n Wraith", 160 - 40 * v.P_LoopMain[playerID], 160);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

               s.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 50 + 25 * v.P_LoopMain[playerID]);
               s.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 50 + 25 * v.P_LoopMain[playerID]);

               s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }
            else if (v.P_LoopMain[playerID] % 2 == 1)
            {
               s.Shape_Square(playerID, 1, "40 + 1n Mojo", 160 - 40 * v.P_LoopMain[playerID], 160);
               KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

               s.Table_Sin(playerID, 45 * v.P_LoopMain[playerID], 50 + 25 * v.P_LoopMain[playerID]);
               s.Table_Cos(playerID, 45 * v.P_LoopMain[playerID], 50 + 25 * v.P_LoopMain[playerID]);

               s.Shape_Square(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 160 - 40 * v.P_LoopMain[playerID], 160);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Mutalisk", 45, 7, 160);
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 9)
         {
            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 45, 7, 160);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Ghost", 45, 5, 200);

            s.EdgeShape(playerID, 1, "40 + 1n Wraith", 45, 7, 160);
            s.EdgeShape(playerID, 1, "40 + 1n Drone", 45, 7, 160);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(400);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            s.LineShape(playerID, 1, "40 + 1n Wraith", 45, 5, 75, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 45, 7, 50, 75);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 225, 7, 50, 75);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Wraith", 45, 5, 75, 75);
            s.LineShape(playerID, 1, "40 + 1n Wraith", 225, 5, 75, 75);
            s.LineShape(playerID, 1, "50 + 1n Tank", 45, 7, 50, 150);
            s.LineShape(playerID, 1, "50 + 1n Tank", 225, 7, 50, 150);

            s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", 50, 50);
            s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", 125, 125);

            s.DoubleShape(playerID, 1, "60 + 1n High Templar", 50, 50);
            s.DoubleShape(playerID, 1, "60 + 1n High Templar", 125, 125);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Wraith", 135, 5, 75, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 135, 7, 50, 75);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 315, 7, 50, 75);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Wraith", 135, 5, 75, 75);
            s.LineShape(playerID, 1, "40 + 1n Wraith", 315, 5, 75, 75);
            s.LineShape(playerID, 1, "50 + 1n Tank", 135, 7, 50, 150);
            s.LineShape(playerID, 1, "50 + 1n Tank", 315, 7, 50, 150);

            s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", -50, 50);
            s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", -125, 125);

            s.DoubleShape(playerID, 1, "60 + 1n High Templar", -50, 50);
            s.DoubleShape(playerID, 1, "60 + 1n High Templar", -125, 125);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 4)
         {             
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 180 - 45 * v.P_LoopMain[playerID], 9, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 180 - 45 * v.P_LoopMain[playerID], 9, 50, 75);
            s.LineShape(playerID, 1, "40 + 1n Mutalisk", 180 - 45 * v.P_LoopMain[playerID] + 180, 9, 50, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 180 - 45 * v.P_LoopMain[playerID], 7, 75, 0);
            s.LineShape(playerID, 1, "60 + 1n Archon", 180 - 45 * v.P_LoopMain[playerID], 7, 75, 75);
            s.LineShape(playerID, 1, "60 + 1n Archon", 180 - 45 * v.P_LoopMain[playerID] + 180, 7, 75, 75);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 135, 7, 75, 0);
            s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 135, 7, 75, 75);
            s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 315, 7, 75, 75);
            s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 135, 7, 75, 0);
            s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 135, 7, 75, 75);
            s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 315, 7, 75, 75);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(400);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
         SetDeaths(playerID, SetTo, 0, " `ShieldRecharge");

         s.SkillEnd();
      }
   }
}