import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   ModifyUnitShields(All, v.P_CharacterID[playerID], playerID, "Anywhere", 1);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 8)
         {             
            if (v.P_LoopMain[playerID] == 0)
            {
               s.Table_Sin(playerID, 22, 50);
               s.Table_Cos(playerID, 22, 50);

               s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "40 + 1n Zealot", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 67, 100);
               s.Table_Cos(playerID, 67, 100);

               s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "40 + 1n Zealot", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            }
            else if (v.P_LoopMain[playerID] == 2)
            {
               RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

               s.Table_Sin(playerID, 22, 100);
               s.Table_Cos(playerID, 22, 100);

               s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "40 + 1n Zealot", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 67, 150);
               s.Table_Cos(playerID, 67, 150);

               s.Shape_Square(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "40 + 1n Zealot", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }
            else if (v.P_LoopMain[playerID] == 4)
            {
               RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

               s.Table_Sin(playerID, 22, 50);
               s.Table_Cos(playerID, 22, 50);

               s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 67, 100);
               s.Table_Cos(playerID, 67, 100);

               s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 22, 100);
               s.Table_Cos(playerID, 22, 100);

               s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 67, 150);
               s.Table_Cos(playerID, 67, 150);

               s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               s.Table_Sin(playerID, 22, 150);
               s.Table_Cos(playerID, 22, 150);

               s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

               KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
               KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            }

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", 160 - 40 * v.P_LoopMain[playerID], 160);
            s.Shape_Square(playerID, 1, "60 + 1n Archon", 160 - 40 * v.P_LoopMain[playerID], 160);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);


            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            s.EdgeShape(playerID, 1, "40 + 1n Ghost", 0, 3, 160);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 45, 7, 160);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 45, 7, 160);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 3)
         {             
            RemoveUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

            s.Shape_Dot(playerID, 8, "Bengalaas (Jungle)", 0, 0);
            s.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);

            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 45, 7, 160);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 45, 7, 160);

            s.Shape_Square(playerID, 1, " Creep. Dunkelheit", 80, 80);
            s.Shape_Square(playerID, 7, "60 + 1n High Templar", 80, 80);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order(" Creep. Dunkelheit", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            RemoveUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "40 + 1n Wraith", 45, 7, 160);
            s.EdgeShape(playerID, 1, " Creep. Dunkelheit", 45, 7, 160);
            KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }

         else if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 45, 7, 160);
            s.EdgeShape(playerID, 1, " Unit. Hoffnung 25000", 45, 7, 160);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}