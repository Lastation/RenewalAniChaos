import Character.Yume.Text as text;
import Character.Yume.Commend as commend;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 190;
	v.P_CharacterID[playerID] = 74;
	
	s.UltimateA[playerID] = 500;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 230:
			CC.main(playerID);
			break;
		case 240:
			CCA.main(playerID);
			break;
		case 210:
			CAA.main(playerID);
			break;
		case 220:
			CAAAC.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Yume_Voice[playerID] != 0) { text.main(playerID); }
}

