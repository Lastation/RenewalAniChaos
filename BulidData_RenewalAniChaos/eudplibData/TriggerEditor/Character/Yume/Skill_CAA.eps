import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   ModifyUnitShields(All, v.P_CharacterID[playerID], playerID, "Anywhere", 1);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 8)
         {             
            if (v.P_LoopMain[playerID] == 0)
            {
               s.LineShape(playerID, 1, "40 + 1n Mojo", 45, 3, 50, 0);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 45, 3, 50, 0);
            }
            else if (v.P_LoopMain[playerID] == 2)
            {
               RemoveUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

               s.LineShape(playerID, 1, "40 + 1n Mojo", 45, 5, 50, 0);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 45, 5, 50, 0);
            }
            else if (v.P_LoopMain[playerID] == 4)
            {
               RemoveUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

               s.LineShape(playerID, 1, "40 + 1n Wraith", 45, 7, 50, 0);
               s.LineShape(playerID, 1, " Unit. Hoffnung 25000", 45, 7, 50, 0);
            }
            else if (v.P_LoopMain[playerID] == 6)
            {
               RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

               s.LineShape(playerID, 1, "50 + 1n Battlecruiser", 45, 5, 75, 0);
               s.Shape_NxNSquareAt(playerID, 1, "Protoss Dark Archon", 3, 25, 75, 75);
               s.Shape_NxNSquareAt(playerID, 1, "Protoss Dark Archon", 3, 25, 150, 150);
               s.Shape_NxNSquareAt(playerID, 1, "Protoss Dark Archon", 3, 25, 0, 0);
               s.Shape_NxNSquareAt(playerID, 1, "Protoss Dark Archon", 3, 25, -75, -75);
               s.Shape_NxNSquareAt(playerID, 1, "Protoss Dark Archon", 3, 25, -150, -150);
            }

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, "Anywhere");

            s.Shape_Square(playerID, 8, "Vulture Spider Mine", 160 - 40 * v.P_LoopMain[playerID], 160);
            s.Shape_Square(playerID, 1, "40 + 1n Guardian", 160 - 40 * v.P_LoopMain[playerID], 160);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "Vulture Spider Mine", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 8)
         {             

            if (v.P_LoopMain[playerID] == 0)
            {
               s.Table_Sin(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 50);
               s.Table_Cos(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 50);

               s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }
            else if (v.P_LoopMain[playerID] == 2)
            {
               s.Table_Sin(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 100);
               s.Table_Cos(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 100);

               s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }
            else if (v.P_LoopMain[playerID] == 4)
            {
               s.Table_Sin(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 100);
               s.Table_Cos(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 100);

               s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }
            else if (v.P_LoopMain[playerID] == 6)
            {
               s.Table_Sin(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 150);
               s.Table_Cos(playerID, 22 + 45 * (v.P_LoopMain[playerID] % 4), 150);

               s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
               s.Shape_Square(playerID, 1, "60 + 1n Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            }

            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

               s.LineShape(playerID, 1, " Creep. Dunkelheit", 45, 5, 75, v.P_LoopMain[playerID] * 25);
               s.LineShape(playerID, 1, " Creep. Dunkelheit", 225, 5, 75, v.P_LoopMain[playerID] * 25);
               s.LineShape(playerID, 8, "Vulture Spider Mine", 45, 5, 75, v.P_LoopMain[playerID] * 25);
               s.LineShape(playerID, 8, "Vulture Spider Mine", 225, 5, 75, v.P_LoopMain[playerID] * 25);
               s.LineShape(playerID, 1, "40 + 1n Wraith", 45, 7, 50, v.P_LoopMain[playerID] * 25);
               s.LineShape(playerID, 1, "40 + 1n Wraith", 225, 7, 50, v.P_LoopMain[playerID] * 25);
            }

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order(" Creep. Dunkelheit", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 160 - 40 * v.P_LoopMain[playerID], 160);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "Vulture Spider Mine", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

            s.Table_Sin(playerID, 22, 50);
            s.Table_Cos(playerID, 22, 50);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            s.Table_Sin(playerID, 67, 100);
            s.Table_Cos(playerID, 67, 100);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "40 + 1n Drone", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            s.Table_Sin(playerID, 22, 100);
            s.Table_Cos(playerID, 22, 100);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            s.Table_Sin(playerID, 67, 150);
            s.Table_Cos(playerID, 67, 150);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, "40 + 1n Drone", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(560);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 3)
         {         
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");

            var x = 50 + 50 * v.P_LoopMain[playerID];
            var y = 50 + 50 * v.P_LoopMain[playerID];
            var interval = 50 + 10 * v.P_LoopMain[playerID];

            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x, y);
            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x - interval, y);
            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x, y - interval);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            var x = 150;
            var y = 150;
            var interval = 70;

            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x - interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x, y - interval);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x, y);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x - interval, y);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x, y - interval);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            var x = 150;
            var y = 150;
            var interval = 70;

            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x - interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x, y - interval);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x - interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x, y - interval);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }

         else if (v.P_LoopMain[playerID] < 8)
         {         
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            var x = -50 - 50 * (v.P_LoopMain[playerID] - 5);
            var y = 50 + 50 * (v.P_LoopMain[playerID] - 5);
            var interval = 50 + 10 * (v.P_LoopMain[playerID] - 5);

            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x, y);
            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x + interval, y);
            s.DoubleShape(playerID, 1, " Unit. Hoffnung 25000", x, y - interval);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {         
            var x = -150;
            var y = 150;
            var interval = 70;

            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x + interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Guardian", x, y - interval);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x, y);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x + interval, y);
            s.DoubleShape(playerID, 1, "Protoss Dark Archon", x, y - interval);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 9)
         {         
            var x = -150;
            var y = 150;
            var interval = 70;

            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x + interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Wraith", x, y - interval);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x, y);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x + interval, y);
            s.DoubleShape(playerID, 1, "40 + 1n Ghost", x, y - interval);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 10)
         {
            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Wraith", 45 + 45 * v.P_LoopMain[playerID], 7, 50, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 45 + 45 * v.P_LoopMain[playerID], 7, 50, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 7)
         {       
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.LineShape(playerID, 1, "40 + 1n Wraith", 45, 7, 50, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 45, 7, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Wraith", 90, 7, 50, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 90, 7, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Wraith", 135, 7, 50, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 135, 7, 50, 0);
            s.LineShape(playerID, 1, "40 + 1n Wraith", 0, 7, 50, 0);
            s.LineShape(playerID, 1, "Protoss Dark Archon", 0, 7, 50, 0);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 0, 3, 50);
            s.EdgeShape(playerID, 1, "Protoss Dark Archon", 0, 3, 50);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            s.EdgeShape(playerID, 1, "40 + 1n Guardian", 0, 5, 100);
            s.EdgeShape(playerID, 1, "Protoss Dark Archon", 0, 5, 100);

            s.EdgeShape(playerID, 1, "40 + 1n Ghost", 0, 5, 100);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 0, 5, 100);
            s.EdgeShape(playerID, 1, "50 + 1n Tank", 0, 5, 100);

            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            s.EdgeShape(playerID, 1, "Kakaru (Twilight)", 0, 3, 50);

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         SetDeaths(playerID, SetTo, 0, " `ShieldRecharge");

         if (Bring(playerID, AtLeast, 1, "Protoss Carrier", "[Skill]UseSkill") &&
         Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill") && s.step[playerID] == 210)
         {
            s.Voice_Routine(playerID, 4);
            s.wait[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            s.step[playerID] = 220;
            KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
            KillUnitAt(1, "Protoss Carrier", "[Skill]UseSkill", playerID);
         }
         else {
            s.SkillEnd();
         }

      }
   }
}