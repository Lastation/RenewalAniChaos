import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0 && Deaths(CurrentPlayer, AtLeast, 1, " `UniqueSkill"))
         {       
         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveLocation("31.Park_Bozo", v.P_CharacterID[playerID], playerID, "Anywhere");
            if (cp < 3)
            {
               if (Bring(3, AtLeast, 1, v.P_CharacterID[3], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[3], 3, "Anywhere");
               }
               else if (Bring(4, AtLeast, 1, v.P_CharacterID[4], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[4], 4, "Anywhere");
               }
               else if (Bring(5, AtLeast, 1, v.P_CharacterID[5], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[5], 5, "Anywhere");
               }

            }
            else if (cp >= 3)
            {
               if (Bring(0, AtLeast, 1, v.P_CharacterID[0], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[0], 0, "Anywhere");
               }
               else if (Bring(1, AtLeast, 1, v.P_CharacterID[1], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID],v.P_CharacterID[1], 1, "Anywhere");
               }
               else if (Bring(2, AtLeast, 1, v.P_CharacterID[2], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[2], 2, "Anywhere");
               }
            }
         MoveUnit(All, v.P_CharacterID[playerID], playerID, "Anywhere", v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 0)
         {
            SetDeaths(playerID, SetTo, 0, " `UniqueSkill");
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 0);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 2)
         {
         KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 50);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 0);
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 0);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 50);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 100, 0);
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 0);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 50);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 100, 0);
            s.Shape_Square(playerID, 1, "50 + 1n Tank", 100, 100);
            s.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         }
         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
         v.P_CountMain[playerID] += 1;
         v.P_LoopMain[playerID] = 0;
         }

      }
         
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
         KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 8 )
         {
            s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 0, 150);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
         }

       s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            s.SkillEnd();
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
   }
}
