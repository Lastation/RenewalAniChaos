import PluginVariables as msqcvar;
import Function as f;

import Character.Park.Skill_S as S;
import Character.Park.Skill_C as C;
import Character.Park.Skill_A as A;
import Character.Park.Skill_O as O;

import Character.Park.Skill_SSS as SSS;
import Character.Park.Skill_ACC as ACC;

import Character.Park.Skill_CCC as CCC;
import Character.Park.Skill_CCCAC as CCCAC;
import Character.Park.Skill_CCCACCC as CCCACCC;

import Character.Park.Skill_AAA as AAA;
import Character.Park.Skill_AAAAA as AAAAA;

import Character.Park.Text as text;
import Character.Park.Commend as commend;

var loop  = 0;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 199;
	f.heroID[cp] = 10;
	
	f.UltimateA[cp] = 500;
	f.UltimateB[cp] = 250;

	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAC.main(cp);
			break;
		case 230:
			CCCACCC.main(cp);
			break;
		case 310:
			ACC.main(cp);
			break;
		case 320:
			AAA.main(cp);
			break;
		case 330:
			AAAAA.main(cp);
			break;
		}
	}
	

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Park_Voice[cp] != 0) { text.main(cp); }
}

function SkillList(cp)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      f.stb.printAt(0, "\n");
	      f.stb.printAt(1, "\x1F　＃\x1B- 박 일표　\x04[ 갓 오브 하이스쿨 ]");
	      f.stb.printAt(2, "　　\x1FA\x04ction List");
	      f.stb.printAt(3, "　　　\x18O \x04택견 - 무음신속 \x19[ 10초 간 첫 스킬에 한해 스킬 사용시 적에게 돌진 ] \x052분");
	      f.stb.printAt(4, "　　　\x04\x1FSSS\x04 소용 \x19[ 대인 / 순간딜 ]");
	      f.stb.printAt(5, "　　　\x04\x1FCCA + AC + CC\x04 약점 \x19[ 대인 / 순간딜 ]");
	      f.stb.printAt(6, "　　　\x04\x1FAAC \x04도발 \x19[ 대인 / 순간딜 ]");
	      f.stb.printAt(7, "　　　\x08AAA \x04택견 오의 - 화축 \x19[ 대인 ] \x05", f.UltimateA[cp]);
	      f.stb.printAt(8, "　　　\x08 + AA \x04 최종 오의 - 삼염멸도각 \x19[ 공성 ] \x05", f.UltimateB[cp]);
	      f.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}