import PluginVariables as msqcvar;
import func.shape as s;

import Character.Park.Skill_S as S;
import Character.Park.Skill_C as C;
import Character.Park.Skill_A as A;
import Character.Park.Skill_O as O;

import Character.Park.Skill_SSS as SSS;
import Character.Park.Skill_ACC as ACC;

import Character.Park.Skill_CCC as CCC;
import Character.Park.Skill_CCCAC as CCCAC;
import Character.Park.Skill_CCCACCC as CCCACCC;

import Character.Park.Skill_AAA as AAA;
import Character.Park.Skill_AAAAA as AAAAA;

import Character.Park.Text as text;
import Character.Park.Commend as commend;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 199;
	v.P_CharacterID[playerID] = 10;
	
	s.UltimateA[playerID] = 500;
	s.UltimateB[playerID] = 250;

	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAC.main(playerID);
			break;
		case 230:
			CCCACCC.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;
		}
	}
	

}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Park_Voice[playerID] != 0) { text.main(playerID); }
}

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      s.stb.printAt(0, "\n");
	      s.stb.printAt(1, "\x1F　＃\x1B- 박 일표　\x04[ 갓 오브 하이스쿨 ]");
	      s.stb.printAt(2, "　　\x1FA\x04ction List");
	      s.stb.printAt(3, "　　　\x18O \x04택견 - 무음신속 \x19[ 10초 간 첫 스킬에 한해 스킬 사용시 적에게 돌진 ] \x052분");
	      s.stb.printAt(4, "　　　\x04\x1FSSS\x04 소용 \x19[ 대인 / 순간딜 ]");
	      s.stb.printAt(5, "　　　\x04\x1FCCA + AC + CC\x04 약점 \x19[ 대인 / 순간딜 ]");
	      s.stb.printAt(6, "　　　\x04\x1FAAC \x04도발 \x19[ 대인 / 순간딜 ]");
	      s.stb.printAt(7, "　　　\x08AAA \x04택견 오의 - 화축 \x19[ 대인 ] \x05", s.UltimateA[playerID]);
	      s.stb.printAt(8, "　　　\x08 + AA \x04 최종 오의 - 삼염멸도각 \x19[ 공성 ] \x05", s.UltimateB[playerID]);
	      s.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}