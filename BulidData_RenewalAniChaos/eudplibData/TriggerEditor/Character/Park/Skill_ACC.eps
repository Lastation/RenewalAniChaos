import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0 && Deaths(CurrentPlayer, AtLeast, 1, " `UniqueSkill"))
         {       
         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveLocation("31.Park_Bozo", v.P_UnitID[playerID], playerID, "Anywhere");
            if (playerID < 3)
            {
               if (Bring(3, AtLeast, 1, v.P_UnitID[3], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[3], 3, "Anywhere");
               }
               else if (Bring(4, AtLeast, 1, v.P_UnitID[4], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[4], 4, "Anywhere");
               }
               else if (Bring(5, AtLeast, 1, v.P_UnitID[5], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[5], 5, "Anywhere");
               }

            }
            else if (playerID >= 3)
            {
               if (Bring(0, AtLeast, 1, v.P_UnitID[0], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[0], 0, "Anywhere");
               }
               else if (Bring(1, AtLeast, 1, v.P_UnitID[1], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID],v.P_UnitID[1], 1, "Anywhere");
               }
               else if (Bring(2, AtLeast, 1, v.P_UnitID[2], "31.Park_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[2], 2, "Anywhere");
               }
            }
         MoveUnit(All, v.P_UnitID[playerID], playerID, "Anywhere", v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 0)
         {
            SetDeaths(playerID, SetTo, 0, " `UniqueSkill");
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 25);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 25, 75);
            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 75, 25);
            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 25, 75);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 2)
         {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 50);
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 6)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 100);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 8)
         {
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 125, 0);
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
         v.P_CountMain[playerID] += 1;
         v.P_LoopMain[playerID] = 0;
         }

      }
         
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Buff_ShieldFix(1);
         }
         if (v.P_LoopMain[playerID] == 6)
         {
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         trg.Shape_Double(playerID, 1, "40 + 1n Guardian", 125, 0);
         trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 75, 0);
         trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 75, 75);
         trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 75, 0);
         trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 0, 75);
         KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);      
         }
         if (v.P_LoopMain[playerID] == 9 )
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            trg.Shape_Double(playerID, 1, "40 + 1n Guardian", 0, 125);
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mutalisk", 3, 75);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 75);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);   
         }
         if (v.P_LoopMain[playerID] == 12 )
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            trg.Shape_Double(playerID, 1, "40 + 1n Guardian", 125, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 75, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 75, 75);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 75, 0);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 0, 75);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);    
         }
         if (v.P_LoopMain[playerID] == 15 )
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            trg.Shape_Double(playerID, 1, "40 + 1n Guardian", 0, 125);
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mutalisk", 3, 75);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 75);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);   
         }
       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 18)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0 )
         {
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            trg.Shape_Edge(playerID, 1, "40 + 1n Goliath", 45,3, 32);
            trg.Shape_Edge(playerID, 1, "50 + 1n Tank", 45,3, 32);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
         }
         if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 6 )
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            trg.Shape_Edge(playerID, 1, "40 + 1n Goliath", 45,3, 32);
            trg.Shape_Edge(playerID, 1, "50 + 1n Tank", 45,3, 32);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
         }

       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            
            trg.SkillEnd();
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
   }
}
