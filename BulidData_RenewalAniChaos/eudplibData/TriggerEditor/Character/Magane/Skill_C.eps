import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {             
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.Shape_Dot(playerID, 1, "40 + 1n Mutalisk", 0, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");

            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID], 150);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID], 150);

            s.Shape_Dot(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 8)
         {             
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.Shape_Dot(playerID, 1, "40 + 1n Mutalisk", 0, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");

            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID] + 45, 150);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID] + 45, 150);

            s.Shape_Dot(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 2)
         {       
            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID] + 45, v.P_LoopMain[playerID] * 75 + 75);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID] + 45, v.P_LoopMain[playerID] * 75 + 75);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID], v.P_LoopMain[playerID] * 75 + 75);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID], v.P_LoopMain[playerID] * 75 + 75);

            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID] + 45, 150);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID] + 45, 150);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID], 150);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID], 150);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(240);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID] + 45, 75);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID] + 45, 75);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            s.Table_Sin(playerID, 90 * v.P_LoopMain[playerID], 75);
            s.Table_Cos(playerID, 90 * v.P_LoopMain[playerID], 75);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(240);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}