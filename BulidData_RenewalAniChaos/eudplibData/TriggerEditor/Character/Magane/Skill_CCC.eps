import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

         s.Table_Sin(playerID, 90 - 22 * v.P_LoopMain[playerID], 100);
         s.Table_Cos(playerID, 90 - 22 * v.P_LoopMain[playerID], 100);

         var x = v.P_AngleCos[playerID];
         var y = v.P_AngleSin[playerID];

         s.Shape_Square(playerID, 1, "40 + 1n Mutalisk", x, y);
         s.Shape_Square(playerID, 1, "Scantid (Desert)", x, y);
         KillUnitAt(All, "Scantid (Desert)", "Anywhere", playerID);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

         if (v.P_LoopMain[playerID] < 4)
         {
            var x = 300 - 75 * v.P_LoopMain[playerID];
            var y = 75 * v.P_LoopMain[playerID];

            s.Shape_Square(playerID, 1, "60 + 1n Siege", x, y);
            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Table_Sin(playerID, 90 - 22 * v.P_LoopMain[playerID], 100);
            s.Table_Cos(playerID, 90 - 22 * v.P_LoopMain[playerID], 100);

            x = v.P_AngleCos[playerID];
            y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "40 + 1n Mutalisk", x, y);
            s.Shape_Square(playerID, 1, "Scantid (Desert)", x, y);
            KillUnitAt(All, "Scantid (Desert)", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 5)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 4)
         {
            s.Table_Sin(playerID, 22 * v.P_LoopMain[playerID], 200);
            s.Table_Cos(playerID, 22 * v.P_LoopMain[playerID], 200);

            var x = v.P_AngleCos[playerID];
            var y = v.P_AngleSin[playerID];

            s.Shape_Square(playerID, 1, "Scantid (Desert)", x, y);
            KillUnitAt(All, "Scantid (Desert)", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] == 0)
         {
            s.EdgeShape(playerID, 1, "Target", 0, 3, 75);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 0, 3, 75);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            s.EdgeShape(playerID, 1, "Target", 45, 3, 100);
            s.EdgeShape(playerID, 1, "60 + 1n Archon", 45, 3, 100);
            s.EdgeShape(playerID, 1, "40 + 1n Drone", 45, 2, 100);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.EdgeShape(playerID, 1, "40 + 1n Drone", 45, 2, 150);
            s.EdgeShape(playerID, 1, "50 + 1n Battlecruiser", 45, 2, 150);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 5)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 4)
         {
            s.Shape_Square(playerID, 1, "40 + 1n Gantrithor", 75 * v.P_LoopMain[playerID], 225 - 75 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            if ((v.P_LoopMain[playerID] % 2) == 0)
            {
               s.DoubleShape(playerID, 1, "Protoss Dark Archon", 100 - 100 * (v.P_LoopMain[playerID] % 2), 100 * (v.P_LoopMain[playerID] % 2));
               s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", 100 - 100 * (v.P_LoopMain[playerID] % 2), 100 * (v.P_LoopMain[playerID] % 2));
               KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }
            else if ((v.P_LoopMain[playerID] % 2) == 1)
            {
               s.DoubleShape(playerID, 1, "Protoss Dark Archon", 0 - 100 * (v.P_LoopMain[playerID] % 2), 100 * (v.P_LoopMain[playerID] % 2));
               s.DoubleShape(playerID, 1, "40 + 1n Mutalisk", 0 - 100 * (v.P_LoopMain[playerID] % 2), 100 * (v.P_LoopMain[playerID] % 2));
               KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 5)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         var x = 0;
         var y = 0;

         if (v.P_LoopMain[playerID] == 0)
         {
            x = 150;
            y = 0;

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", -y, x);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", x, y);
            s.Shape_Square(playerID, 1, "60 + 1n Archon", x, y);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            x = 150;
            y = 150;

            s.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", -y, x);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", x, y);
            s.Shape_Square(playerID, 1, "60 + 1n Archon", x, y);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }

         s.SkillWait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 24)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.SkillEnd();
      }
   }
}
