import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {             
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.Shape_Dot(playerID, 1, "40 + 1n Mutalisk", 0, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 2)
         {       
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);
            s.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 50);
            s.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 50);

            s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(320);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.Shape_NxNSquareAt(playerID, 1, "Kakaru (Twilight)", 2, 50, 100, 100);
            s.Shape_NxNSquareAt(playerID, 1, "Kakaru (Twilight)", 2, 50, -100, 100);
            s.Shape_NxNSquareAt(playerID, 1, "Kakaru (Twilight)", 2, 50, 100, -100);
            s.Shape_NxNSquareAt(playerID, 1, "Kakaru (Twilight)", 2, 50, -100, -100);
            s.Shape_NxNSquareAt(playerID, 1, " Unit. Hoffnung 25000", 2, 50, 100, 100);
            s.Shape_NxNSquareAt(playerID, 1, " Unit. Hoffnung 25000", 2, 50, -100, 100);
            s.Shape_NxNSquareAt(playerID, 1, " Unit. Hoffnung 25000", 2, 50, 100, -100);
            s.Shape_NxNSquareAt(playerID, 1, " Unit. Hoffnung 25000", 2, 50, -100, -100);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}