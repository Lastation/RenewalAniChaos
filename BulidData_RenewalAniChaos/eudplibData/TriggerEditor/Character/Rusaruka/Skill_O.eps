import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
	if (v.P_Delay[playerID] == 0)
	{
		if (cp < 3)
		{ MoveUnit(All, v.P_CharacterID[playerID], playerID, "Anywhere", "[Unique]Position_Team1"); }
		else
		{ MoveUnit(All, v.P_CharacterID[playerID], playerID, "Anywhere", "[Unique]Position_Team2"); }
	
		if (v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				SetInvincibility(Enable, v.P_CharacterID[playerID], playerID, "Anywhere");
				s.Voice_Routine(playerID, 7);
				s.SkillWait(5800);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				s.Voice_Routine(playerID, 8);
				s.SkillWait(5600);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				s.Voice_Routine(playerID, 9);
				s.SkillWait(6100);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				s.Voice_Routine(playerID, 10);
				s.SkillWait(6200);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				s.Voice_Routine(playerID, 11);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				v.P_LoopMain[playerID] = 0;
				v.P_CountMain[playerID] += 1;
			}
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				s.SkillUnit(playerID, 1, "50 + 1n Battlecruiser");
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 1, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 5, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 7, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				if (cp < 3)
				{ 
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P7);
					MoveUnit(1, v.P_CharacterID[playerID], playerID, "Anywhere", "[Potal]Base7");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P7, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team1");
					MoveUnit(1, v.P_CharacterID[playerID], playerID, "Anywhere", "[Unique]Position_Team1");
				}
				else
				{
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P8);
					MoveUnit(1, v.P_CharacterID[playerID], playerID, "Anywhere", "[Potal]Base8");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P8, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team2");
					MoveUnit(1, v.P_CharacterID[playerID], playerID, "Anywhere", "[Unique]Position_Team2");
				}
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 6)
			{
				SetInvincibility(Disable, v.P_CharacterID[playerID], playerID, "Anywhere");
				s.SkillEnd();
			}
		}
	}
}