import PluginVariables as msqcvar;
import func.shape as s;

import Character.Rusaruka.Skill_O as O;
import Character.Rusaruka.Skill_S as S;
import Character.Rusaruka.Skill_C as C;
import Character.Rusaruka.Skill_A as A;

import Character.Rusaruka.Skill_SSS as SSS;
import Character.Rusaruka.Skill_CCC as CCC;
import Character.Rusaruka.Skill_CCCAA as CCCAA;
import Character.Rusaruka.Skill_ACC as ACC;
import Character.Rusaruka.Skill_AAAC as AAAC;

import Character.Rusaruka.Text as text;
import Character.Rusaruka.Commend as commend;

function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 32;
	v.P_CharacterID[playerID] = 51;
	
	s.UltimateA[playerID] = 650;
	
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 320:
			AAAC.main(playerID);
			break;
		}
	}
	
}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Rusaruka_Voice[playerID] != 0) { text.main(playerID); }
}

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		s.stb.printAt(0, "\n");
		s.stb.printAt(1, "\x1F　＃\x04- 루살카 \x1B슈베게린　\x04[ 디에스 이래 ]");
		s.stb.printAt(2, "　　\x1FA\x04ction List");
		s.stb.printAt(3, "　　　\x18O \x04창조 \x19[ 15x15 / 적 5초마다 경직 / 파괴될시 5분후 재생 / 설치형 ]");
		s.stb.printAt(4, "　　　\x04\x1FSSS \x04위협 \x19[ 대인 / 공성 ]");
		s.stb.printAt(5, "　　　\x04\x1FCCC + AA \x04고문바퀴 \x19[ 공성 / 후속 스킬 귀환봉인, 자리고정 ]");
		s.stb.printAt(6, "　　　\x04\x1FACC \x04고문용 쇠사슬 \x19[ 대인 ]");
		s.stb.printAt(7, "\n");
		s.stb.printAt(8, "　　　\x08AAAC \x04갈망 \x19[ 공성 / 선딜없음 /  장판형 스킬 ] \x05", s.UltimateA[playerID]);
		s.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}