import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{	
	if (v.P_Delay[playerID] == 0)
	{
		if(v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] < 11)
			{
				s.Table_Cos(playerID, 9 * v.P_LoopMain[playerID], 160);
				s.Table_Sin(playerID, 9 * v.P_LoopMain[playerID], 160);
				
				s.Shape_Square(playerID, 1, "Zerg Devourer", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
				s.Shape_Square(playerID, 1,"40 + 1n Zergling", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
				KillUnitAt(All, "40 + 1n Zergling", "Anywhere", playerID);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("Zerg Devourer", playerID, "Anywhere", Move, v.P_LocationID[playerID]);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 11)
			{
				KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
				v.P_LoopMain[playerID] = 0;
				v.P_CountMain[playerID] += 1;
			}
		}
		else if(v.P_CountMain[playerID] == 1)
		{
			if (v.P_LoopMain[playerID] < 6)
			{
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				
				s.Shape_Square(playerID, 1, "40 + 1n Wraith", 192 - 32 * v.P_LoopMain[playerID],  32 * v.P_LoopMain[playerID]);
				s.Shape_Square(playerID, 1, "Torrasque", 96 - 16 * v.P_LoopMain[playerID], 16 * v.P_LoopMain[playerID]);
				KillUnitAt(All, "Torrasque", "Anywhere", playerID);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] >= 6 && v.P_LoopMain[playerID] < 12)
			{
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				
				s.Shape_Square(playerID, 1, "40 + 1n Mojo", 192 - 32 * v.P_LoopMain[playerID],  32 * v.P_LoopMain[playerID] - 192);
				s.Shape_Square(playerID, 1, "Torrasque", 96 - 16 * v.P_LoopMain[playerID], 16 * v.P_LoopMain[playerID] - 96);
				KillUnitAt(All, "Torrasque", "Anywhere", playerID);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 12)
			{
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				s.SkillWait(500);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 13)
			{
				
				s.Table_Cos(playerID, 30, 96);
				s.Table_Sin(playerID, 30, 96);
				s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
				
				s.Table_Cos(playerID, 60, 96);
				s.Table_Sin(playerID, 60, 96);
				s.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
				
				s.CrossShape(playerID, 1, "50 + 1n Battlecruiser", 0, 5, 48);
				MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
				Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				s.SkillWait(1000);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 14)
			{
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
				s.SkillEnd();
			}
		}
	}
}