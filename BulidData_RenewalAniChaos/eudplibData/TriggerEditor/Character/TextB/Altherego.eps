function NormalText(playerID)
{
	switch (v.Sound_Text2[playerID])
	{
	case 4100:
		PlayWAV("Alther_0.ogg");
		s.stb.print("\n\x13\x1FAlther Ego Λ\n\x13\x04「　멜트 스트라이크!　\x04」\n");
		v.Sound_Text2[playerID] = 0;
		break;
	case 4200:
		PlayWAV("Alther_1.ogg");
		s.stb.print("\n\x13\x1FAlther Ego Λ\n\x13\x04「　작별이네,　\x04」\n");
		v.Sound_Text2[playerID] = 0;
		break;
	case 4201:
		s.stb.print("\n\x13\x1FAlther Ego Λ\n\x13\x04「　마지막으로 사랑을 가르쳐줄게.　\x04」\n");
