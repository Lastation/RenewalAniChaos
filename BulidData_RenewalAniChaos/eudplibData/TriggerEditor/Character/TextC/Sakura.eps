function UiltimateText(cp)
{
	switch (v.Sound_Text_Uilti[cp])
	{
	case 13000:
		PlayWAV("sakura_04.ogg");
		v.Sound_Text_Uilti[cp] = 0;
		break;
	case 13001:
		PlayWAV("sakura_05.ogg");
		f.stb.printAt(3,"\x13\x1BMato Sakura\n\x13\x08이런 사람... 없었으면 좋았을텐데...");
		v.Sound_Text_Uilti[cp] = 0;
		break;
	}
}

