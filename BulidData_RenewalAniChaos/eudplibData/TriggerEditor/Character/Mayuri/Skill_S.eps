import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         s.Shape_Square(playerID, 1, "Target", 50, 0);
         s.Shape_Square(playerID, 1, "Target", 100, 0);

         KillUnitAt(All, "Target", "Anywhere", playerID);

         s.SkillWait(80);

         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 150, 0);
         s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 100, 50);
         s.Shape_Square(playerID, 1, "60 + 1n Danimoth", 50, 100);
         s.Shape_Square(playerID, 1, "40 + 1n Marine", 100, 50);
         s.Shape_Square(playerID, 1, "40 + 1n Marine", 50, 100);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

         s.SkillWait(80);

         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.Shape_Square(playerID, 1, "60 + 1n Archon", 150, 0);
         s.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 50);
         s.Shape_Square(playerID, 1, "60 + 1n Archon", 50, 100);

         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         s.SkillWait(240);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
         s.distance[playerID] = 50;

         s.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0);

         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", s.distance[playerID], s.distance[playerID]);
         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", -s.distance[playerID], -s.distance[playerID]);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", s.distance[playerID], s.distance[playerID]);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -s.distance[playerID], -s.distance[playerID]);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         s.SkillWait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.Shape_Square(playerID, 1, "60 + 1n Archon", 75, 75);

         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", 0, 50);
         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", 0, -50);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, 50);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, -50);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         s.SkillWait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0);

         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", -50, 50);
         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", 50, -50);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -50, 50);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 50, -50);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         s.SkillWait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 6)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         s.Shape_Square(playerID, 1, "60 + 1n Archon", 75, 75);

         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", -50, 0);
         s.Shape_Dot(playerID, 1, "40 + 1n Goliath", 50, 0);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -50, 0);
         s.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 50, 0);

         MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         s.SkillWait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 7)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         s.SkillEnd();
      }
   }
}