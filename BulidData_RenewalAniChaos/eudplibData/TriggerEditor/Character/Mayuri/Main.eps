function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 183;
	v.P_CharacterID[playerID] = 64;
	
	s.UltimateA[playerID] = 500;
	s.UltimateB[playerID] = 250;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 310:
			AAS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 230:
			CCCAAAA.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(playerID)
{
	if (s.Mayuri_Voice[playerID] != 0) { text.main(playerID); }
}

