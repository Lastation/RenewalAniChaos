const looker = PVariable();

function Text(num);
function Looker()
{
	if (looker[getuserplayerid() - 128] > 0)
	{
		Text(looker[getuserplayerid() - 128]);
		looker[getuserplayerid() - 128] = 0;
	}
}

function main(playerID)
{
	if (s.Mayuri_Voice[playerID] > 0)
	{
		Text(s.Mayuri_Voice[playerID]);
		looker[playerID] = s.Mayuri_Voice[playerID];
		s.Mayuri_Voice[playerID] = 0;
	}
}

