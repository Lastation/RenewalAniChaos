import PluginVariables as msqcvar;
import func.shape as s;

const s = StringBuffer();

function main(playerID)
{
   s.HoldPosition(playerID);
   ModifyUnitShields(All, v.P_CharacterID[playerID], playerID, "Anywhere", 1);
   MoveUnit(All, "40 + 1n Marine", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "40 + 1n Ghost", playerID, "Anywhere", "[Skill]HoldPosition");

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {                      
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");
            s.Shape_Square(playerID, 1, "Zerg Devourer", 50 * v.P_LoopMain[playerID] + 50, 0);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 12)
         {
            s.Shape_Square(playerID, 1, "Zerg Devourer", 200 - 25 * (v.P_LoopMain[playerID] - 4), 25 * (v.P_LoopMain[playerID] - 4));
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               s.Shape_Square(playerID, 1, "40 + 1n Marine", 200 - 25 * (v.P_LoopMain[playerID] - 4), 25 * (v.P_LoopMain[playerID] - 4));
               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
               Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;

         }
         else if (v.P_LoopMain[playerID] < 16)
         {                        
            s.Shape_Square(playerID, 1, "Zerg Devourer", 0, 200 - 50 * (v.P_LoopMain[playerID] - 12));
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 16)
         {                        
            s.SkillWait(1080);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 17)
         {                        
            s.Voice_Routine(playerID, 6);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {                        
            s.Shape_Square(playerID, 1, "Zerg Devourer", 50 * v.P_LoopMain[playerID] + 50, 0);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] < 12)
         {
            s.Shape_Square(playerID, 1, "Zerg Devourer", 200 - 25 * (v.P_LoopMain[playerID] - 4), 25 * (v.P_LoopMain[playerID] - 4));
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] % 2 == 1)
            {
               s.Shape_Square(playerID, 1, "40 + 1n Ghost", 200 - 25 * (v.P_LoopMain[playerID] - 4), 25 * (v.P_LoopMain[playerID] - 4));
               MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
               MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
               Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            }

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;

         }
         else if (v.P_LoopMain[playerID] < 16)
         {                        
            s.Shape_Square(playerID, 1, "Zerg Devourer", 0, 200 - 50 * (v.P_LoopMain[playerID] - 12));
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 16)
         {                        
            s.SkillWait(1080);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 17)
         {                        
            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (Bring(playerID, AtLeast, 1, "Protoss Scout", "[Skill]UseSkill") 
         && Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill")
         && s.step[playerID] == 210)
         {
            s.Voice_Routine(playerID, 7);
            s.wait[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            s.step[playerID] = 220;
            KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
            KillUnitAt(1, "Protoss Scout", "[Skill]UseSkill", playerID);
         }
         else 
         {
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
            
            SetDeaths(playerID, SetTo, 0, " `ShieldRecharge");

            s.SkillEnd();
         }



      }
   }
   if (v.P_DelayB[playerID] == 0)
   {
      if ((v.P_CountMain[playerID] == 1) && (v.P_LoopMain[playerID] < 16))
      {
         if (v.P_LoopMainB[playerID] > 3) v.P_LoopMainB[playerID] = 0;

         s.Table_Sin(playerID, (45 * v.P_LoopMainB[playerID] + 22), 50 * v.P_LoopMainB[playerID] + 50);
         s.Table_Cos(playerID, (45 * v.P_LoopMainB[playerID] + 22), 50 * v.P_LoopMainB[playerID] + 50); 

         s.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);

         KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

         s.SkillWaitB(playerID, 160);

         v.P_LoopMainB[playerID] += 1;
      }
   }
   if (v.P_DelayC[playerID] == 0)
   {
      if ((v.P_CountMain[playerID] < 2) && (v.P_LoopMain[playerID] < 16))
      {
         if (v.P_LoopMainC[playerID] > 8) v.P_LoopMainC[playerID] = 0;

         s.Shape_Square(playerID, 1, "40 + 1n Zealot", 240 - 30 * v.P_LoopMainC[playerID], 30 * v.P_LoopMainC[playerID]);
         s.Shape_Square(playerID, 1, "Protoss Dark Templar", 160 - 20 * v.P_LoopMainC[playerID], 20 * v.P_LoopMainC[playerID]);

         KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
         KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

         s.SkillWaitC(playerID, 80);

         v.P_LoopMainC[playerID] += 1;
      }
   }

}