import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {                        
            s.Shape_Square(playerID, 1, "60 + 1n Archon", 200 - v.P_LoopMain[playerID] * 50, 0);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            
            s.SkillWait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            s.Shape_Square(playerID, 4, "60 + 1n Archon", 50, 50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            s.SkillWait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(0);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            s.distance[playerID] = 125;
            
            s.Table_Cos(playerID, 45 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);
            s.Table_Sin(playerID, 45 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);

            s.MoveLoc(v.P_CharacterID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.SkillUnit(playerID, 1, "40 + 1n Wraith");
            s.SkillUnit(playerID, 1, "40 + 1n Goliath");

            s.distance[playerID] = 175;

            s.Table_Cos(playerID, 45 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);
            s.Table_Sin(playerID, 45 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);

            s.MoveLoc(v.P_CharacterID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.SkillUnit(playerID, 1, "40 + 1n Wraith");
            s.SkillUnit(playerID, 1, "40 + 1n Goliath");

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.SkillWait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            s.SkillWait(0);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            s.distance[playerID] = 100;
            
            s.Table_Cos(playerID, 0 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);
            s.Table_Sin(playerID, 0 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);

            s.MoveLoc(v.P_CharacterID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.SkillUnit(playerID, 1, "40 + 1n Wraith");
            s.SkillUnit(playerID, 1, "40 + 1n Goliath");

            s.distance[playerID] = 150;

            s.Table_Cos(playerID, 0 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);
            s.Table_Sin(playerID, 0 + 90 * v.P_LoopMain[playerID], s.distance[playerID]);

            s.MoveLoc(v.P_CharacterID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            s.SkillUnit(playerID, 1, "40 + 1n Wraith");
            s.SkillUnit(playerID, 1, "40 + 1n Goliath");

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            s.SkillWait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            s.SkillWait(50);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         s.SkillEnd();
      }
   }
}