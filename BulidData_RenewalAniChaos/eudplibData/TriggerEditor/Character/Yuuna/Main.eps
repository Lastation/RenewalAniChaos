function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 181;
	v.P_CharacterID[playerID] = 54;
	
	s.UltimateA[playerID] = 650;
	s.UltimateB[playerID] = 300;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (s.wait[playerID] == 0)
	{
		switch(s.step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 120:
			SSSSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 230:
			CAAA.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
		case 320:
			AAAAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

