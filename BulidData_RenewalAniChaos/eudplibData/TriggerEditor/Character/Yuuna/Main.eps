function SkillList(cp);

function main(cp)
{
	f.location[cp] = 181;
	f.heroID[cp] = 54;
	
	f.UltimateA[cp] = 650;
	f.UltimateB[cp] = 300;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 120:
			SSSSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		case 230:
			CAAA.main(cp);
			break;
		case 310:
			AAA.main(cp);
			break;
		case 320:
			AAAAAA.main(cp);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
