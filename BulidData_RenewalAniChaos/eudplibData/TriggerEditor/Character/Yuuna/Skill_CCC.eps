            s.EdgeShape(playerID, 1, "40 + 1n Goliath", 45, 5, 175);
            MoveLocation(v.P_LocationID[playerID], v.P_CharacterID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(320);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

         
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill") 
         && Bring(playerID, AtLeast, 1, "Protoss Scout", "[Skill]UseSkill") 
         && s.step[playerID] == 210)
         {
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            s.Voice_Routine(playerID, 4);
            s.wait[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            s.step[playerID] = 220;
            KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
            KillUnitAt(1, "Protoss Scout", "[Skill]UseSkill", playerID);
         }
         else 
         {
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
            s.SkillEnd();
         }


