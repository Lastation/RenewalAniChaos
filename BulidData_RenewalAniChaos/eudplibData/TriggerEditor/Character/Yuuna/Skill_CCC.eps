            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 200, 200);
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 100);
            s.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 150);
