import PluginVariables as msqcvar;
import func.shape as s;

function main(playerID)
{
   s.HoldPosition(playerID);

   if (v.P_Delay[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(160);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(160);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            RemoveUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            s.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 3, 75);
            s.Shape_NxNSquare(playerID, 1, "50 + 1n Tank", 3, 75);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            s.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 5, 75);
            s.Shape_NxNSquare(playerID, 1, "40 + 1n Goliath", 3, 50);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, "Anywhere");

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            s.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 5, 75);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            s.SkillWait(160);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            s.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 5, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_LoopMain[playerID] += 1;


         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            s.Shape_NxNSquare(playerID, 1, " Unit. Hoffnung 25000", 5, 75);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            s.SkillWait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

         
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         s.SkillEnd();
      }
   }
}