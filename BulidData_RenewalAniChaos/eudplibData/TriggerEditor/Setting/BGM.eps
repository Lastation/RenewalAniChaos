import PluginVariables as msqcvar;
var flag = 0;

function SelectBGM(cp)
{
	switch(flag)
	{
		case 0:	// 선택 비활성
			if(Memory(0x00596A3C, Exactly, 1)) 	// Home key Pressed
			{
				flag = 1;
			}
			break;
		case 1:	// 선택 활성
			if(Memory(0x00596A3C, Exactly, 1)) 	// Home key Pressed
			{
				flag = 0;
			}
			f.stb.printAt(0, "\x19B\x04GM \x1BS\x04elect\n");
			f.stb.printAt(1, "\n");
			f.stb.printAt(2, "\x19[1] \x04Layers \x02[ Re : CREATORS ] \05(Defualt)\n");
			f.stb.printAt(3, "\x19[2] \x04化身の獣 \x02[ 십이대전 ]\n");
			f.stb.printAt(4, "\x19[3] \x04sh0ut \x02[ Re : CREATORS ]\n");
			f.stb.printAt(5, "\x19[3] \x04한 스푼의 공주님 \x02[ 주문은 토끼입니까? ]\n");
			f.stb.printAt(6, "\n");
			f.stb.printAt(7, "\x19[Home] \x04완료 ]\n");
			
			if(Memory(0x00596A48, Exactly, 256)) 			// 1 key Pressed
			{
				flag = 0;
			}
			else if(Memory(0x00596A48, Exactly, 65536)) 		// 2 key Pressed
			{
				flag = 0;
			}
			else if(Memory(0x00596A48, Exactly, 16777216)) 	// 3 key Pressed
			{
				flag = 0;
			}
			else if(Memory(0x00596A4C, Exactly, 1)) 			// 4 key Pressed
			{
				flag = 0;
			}
			break;
	}

}