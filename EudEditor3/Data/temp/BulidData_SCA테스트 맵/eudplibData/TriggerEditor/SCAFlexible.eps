import variable as n0;

const ws = 0x58F44A;
const EntryPointLength = 8;//EntryPointLength
const SpaceLength = 200;//DataBufferSize
const ObjectCount = 9;//ObjectCount

function Init(){
    MPQAddFile('SCARCHIVEMAPCODE', py_open('scakeyfile', 'rb').read());
    MPQAddFile('SCARCHIVEDATA', py_open('scadatafile', 'rb').read());
    //EntryPoint
    dwwrite_epd(EPD(ws) + 0, 2968250430);
    dwwrite_epd(EPD(ws) + 1, 2104957647);
    dwwrite_epd(EPD(ws) + 2, 1536203667);
    dwwrite_epd(EPD(ws) + 3, 2636220041);
    dwwrite_epd(EPD(ws) + 4, 1267262437);
    dwwrite_epd(EPD(ws) + 5, 1927728667);
    dwwrite_epd(EPD(ws) + 6, 1897467734);
    dwwrite_epd(EPD(ws) + 7, 1426524437);
    
}


function Exec(){
    Init();
}


function ResetValue(tagNum, index){
    const cp = getcurpl();
    switch(tagNum){
    case 0:
        n0.v1[cp] = 0;
        break;
    case 1:
        n0.v2[cp] = 0;
        break;
    case 2:
        n0.v3[cp] = 0;
        break;
    case 3:
        n0.v4[cp] = 0;
        break;
    case 4:
        SetDeaths(CurrentPlayer, SetTo, 0, 0);
        break;
    case 5:
        SetDeaths(CurrentPlayer, SetTo, 0, 1);
        break;
    case 6:
        SetDeaths(CurrentPlayer, SetTo, 0, 2);
        break;
    case 7:
        SetDeaths(CurrentPlayer, SetTo, 0, 3);
        break;
    case 8:
        const alen = n0.array1.length / 8;
        for(var i = 0 ; i < alen; i++){
            n0.array1[alen * cp + i] = 0;
        }
        break;
    }
}


function SaveValue(tagNum, Value, index){
    const cp = getcurpl();
    switch(tagNum){
    case 0:
        n0.v1[cp] = Value;
        break;
    case 1:
        n0.v2[cp] = Value;
        break;
    case 2:
        n0.v3[cp] = Value;
        break;
    case 3:
        n0.v4[cp] = Value;
        break;
    case 4:
        SetDeaths(CurrentPlayer, SetTo, Value, 0);
        break;
    case 5:
        SetDeaths(CurrentPlayer, SetTo, Value, 1);
        break;
    case 6:
        SetDeaths(CurrentPlayer, SetTo, Value, 2);
        break;
    case 7:
        SetDeaths(CurrentPlayer, SetTo, Value, 3);
        break;
    case 8:
        const alen = n0.array1.length / 8;
        n0.array1[alen * cp + index] = Value;
        break;
    }
}


