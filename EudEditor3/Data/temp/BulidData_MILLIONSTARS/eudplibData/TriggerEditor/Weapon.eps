function Set_Label(id, value)
{
	wwrite(0x6564E0 + 3584 	+ id * 2, value);
}

function Set_Atk(id, value)
{
	wwrite(0x6564E0 + 2512 	+ id * 2, value);
}

function Set_AtkBouns(id, value)
{
	wwrite(0x6564E0 + 4504 	+ id * 2, value);
}

function Set_AtkSpeed(id, value)
{
	bwrite(0x6564E0 + 2776 		+ id * 1, value);
}

function Set_AtkType(id, value)
{
	bwrite(0x6564E0 + 3448 		+ id * 1, value);
	bwrite(0x6564E0 + 3312 		+ id * 1, 2 +value);
}

function Set_AtkExplosion(id, value)
{
	bwrite(0x6564E0 + 536 		+ id * 1, value);
}

function Set_AtkSplash(id, value)
{
	wwrite(0x6564E0 + 936 		+ id * 2, value / 4);
	wwrite(0x6564E0 + 3048 	+ id * 2, value / 2);
	wwrite(0x6564E0 + 4768 	+ id * 2, value);
}

function Set_AtkRange(id, value)
{
	dwwrite(0x6564E0 + 1336 + id * 4, 0);
	dwwrite(0x6564E0 + 3984 + id * 4, value * 32);
}

function Set_UnitRange(id, value)
{
	bwrite(0x65FD00 + 12472 + id * 1, value);
}

function Set_Rank(id, value)
{
	bwrite(0x65FD00 + 16592 + id * 1, value);
}

//////////////////////////////////////////////////////////////////////////

function Get_Atk(id)
{
	id = wread(0x6564E0 + 2512 + id * 2);
}

function Get_AtkBouns(id)
{
	id = wread(0x6564E0 + 4504 + id * 2);
}

function Get_AtkSpeed(id)
{
	id = bread(0x6564E0 + 2776 + id * 1);
}

function Get_AtkType(id)
{
	id = bread(0x6564E0 + 3448 + id * 1);
}

function Get_AtkExplosion(id, value)
{
	id = bread(0x6564E0 + 536 	+ id * 1);
}

function Get_AtkSplash(id)
{
	id = wread(0x6564E0 + 936 	+ id * 2);
	id = wread(0x6564E0 + 3048 + id * 2);
	id = wread(0x6564E0 + 4768 + id * 2);
}

function Get_Rank(id)
{
	id = bread(0x65FD00 + 16592 + id * 1);
}