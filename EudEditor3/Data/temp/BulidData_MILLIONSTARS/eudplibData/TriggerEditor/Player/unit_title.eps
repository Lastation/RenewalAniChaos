function Set_Title(id)
{
	if (Deaths(P8, AtLeast, 100, id) && Deaths(P8, AtMost, 199, id))	
	{ Title_Value(id, 1); }
	if (Deaths(P8, AtLeast, 200, id) && Deaths(P8, AtMost, 399, id))	
	{ Title_Value(id, 2); }
	if (Deaths(P8, AtLeast, 400, id) && Deaths(P8, AtMost, 799, id))	
	{ Title_Value(id, 3); }
	if (Deaths(P8, AtLeast, 800, id))
	{ Title_Value(id, 4); }
}

