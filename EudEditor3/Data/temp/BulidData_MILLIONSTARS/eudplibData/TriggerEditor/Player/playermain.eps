import Player.shop as sp;
import Player.sound as sd;
import Player.title as te;
import Player.titleInfo as teinfo;
import Player.lesson as ln;
import Player.event as et;
import Player.cloth as cl;

function main(cp)
{
	sp.Marge(cp);
	ln.Marge(cp);
	
	sd.PlaySound(cp);
	if (cp < 5)
	{
		te.Title_Effect(cp);
	}
	teinfo.Title_Info(cp);
	et.event_score(cp);
	cl.Cloth_Ticket(cp);
	cl.Producer_Buff(cp);
}