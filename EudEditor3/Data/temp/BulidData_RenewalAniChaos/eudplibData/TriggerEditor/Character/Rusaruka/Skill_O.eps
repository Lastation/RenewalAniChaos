import Function as f;

function main(cp, location, heroID)
{
	if (f.delay[cp] == 0)
	{
		if (cp < 3)
		{ MoveUnit(All, heroID, cp, "Anywhere", "[Unique]Position_Team1"); }
		else
		{ MoveUnit(All, heroID, cp, "Anywhere", "[Unique]Position_Team2"); }
	
		if (f.count[cp] == 0)
		{
			if (f.loop[cp] == 0)
			{
				f.Voice_Routine(cp, 7);
				f.SkillWait(cp, 5800);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 1)
			{
				f.Voice_Routine(cp, 8);
				f.SkillWait(cp, 5600);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 2)
			{
				f.Voice_Routine(cp, 9);
				f.SkillWait(cp, 6100);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 3)
			{
				f.Voice_Routine(cp, 10);
				f.SkillWait(cp, 6200);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 4)
			{
				f.Voice_Routine(cp, 11);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 5)
			{
				f.loop[cp] = 0;
				f.count[cp] += 1;
			}
		}
		else if (f.count[cp] == 1)
		{
			if (f.loop[cp] == 0)
			{
				f.SkillUnit(1, "50 + 1n Battlecruiser", location, cp);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 1)
			{
				f.NxNSquareShape(heroID, 1, "50 + 1n Battlecruiser", location, cp, 1, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 2)
			{
				f.NxNSquareShape(heroID, 1, "50 + 1n Battlecruiser", location, cp, 3, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 3)
			{
				f.NxNSquareShape(heroID, 1, "50 + 1n Battlecruiser", location, cp, 5, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 4)
			{
				f.NxNSquareShape(heroID, 1, "50 + 1n Battlecruiser", location, cp, 7, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 5)
			{
				if (cp < 3)
				{ 
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P7);
					MoveUnit(1, heroID, cp, "Anywhere", "[Potal]Base7");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P7, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team1");
					MoveUnit(1, heroID, cp, "Anywhere", "[Unique]Position_Team1");
				}
				else
				{
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P8);
					MoveUnit(1, heroID, cp, "Anywhere", "[Potal]Base8");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P8, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team2");
					MoveUnit(1, heroID, cp, "Anywhere", "[Unique]Position_Team2");
				}
				f.SkillWait(cp, 0);
				f.loop[cp] += 1;
			}
			else if (f.loop[cp] == 6)
			{
				f.SkillEnd(cp);
			}
		}
	}
}