import Function as f;

function Shape(cp, Unit : TrgUnit, x, y);

function main(cp)
{
	if (f.delay[cp] == 0)
	{
		switch(f.loop[cp])
		{
		case 0:
		case 4:
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			Shape(cp, "40 + 1n Mojo", 0, 48);
			break;
		case 1:
		case 5:
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			Shape(cp, "40 + 1n Wraith", 42, 24);
			break;
		case 2:
		case 6:
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", cp);
			Shape(cp, "40 + 1n Mutalisk", 24, 42);
			break;
		case 3:
		case 7:
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);
			Shape(cp, "40 + 1n Mojo", 48, 0);
			break;
		case 8:
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			f.SkillEnd(cp);
			break;
		}
	}
}

function Shape(cp, Unit : TrgUnit, x, y)
{
	f.MoveLoc(f.heroID[cp], cp, x, y);
	f.SkillUnit(cp, 1, Unit);
	f.SkillUnit(cp, 1, "Protoss Dark Templar");
	f.MoveLoc(f.heroID[cp], cp, -y, x);
	f.SkillUnit(cp, 1, "Protoss Dark Templar");
	f.MoveLoc(f.heroID[cp], cp, -x, -y);
	f.SkillUnit(cp, 1, Unit);
	f.SkillUnit(cp, 1, "Protoss Dark Templar");
	f.MoveLoc(f.heroID[cp], cp, y, -x);
	f.SkillUnit(cp, 1, "Protoss Dark Templar");
	KillUnitAt(All, "Protoss Dark Templar", "Anywhere", cp);
	MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
	Order(Unit, cp, "Anywhere", Attack, f.location[cp]);
	f.SkillWait(cp, 50);
	f.loop[cp] += 1;
}