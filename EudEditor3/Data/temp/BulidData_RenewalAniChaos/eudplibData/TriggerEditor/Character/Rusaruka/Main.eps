import Function as f;

import Character.Rusaruka.Skill_O as O;
import Character.Rusaruka.Skill_S as S;
import Character.Rusaruka.Skill_C as C;
import Character.Rusaruka.Skill_A as A;

import Character.Rusaruka.Skill_SSS as SSS;
//import Character.Rusaruka.Skill_CCC as CCC;
import Character.Rusaruka.Skill_ACC as ACC;

import Character.Rusaruka.Text as text;
import Character.Rusaruka.Commend as commend;

const stb = StringBuffer();

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 32;
	f.heroID[cp] = 51;
	
	f.UltimateA[cp] = 650;
	
	// Commed Setting
	commend.main(cp);
	SkillList(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 310:
			ACC.main(cp);
			break;
		}
	}
	
}

// Character Voice
function SkillVoice(cp)
{
	if (f.Rusaruka_Voice[cp] != 0) { text.main(cp); }
}

function SkillList(cp)
{
	if(Memory(0x596A44, Exactly, 0x00000100))	// Insert key Pressed
	{
		stb.printAt(0, "\n");
		stb.printAt(1, "\x1F　＃\x04- 루살카 \x1B슈베게린　\x04[ 디에스 이래 ]");
		stb.printAt(2, "　　\x1FA\x04ction List");
		stb.printAt(3, "　　　\x18O \x04창조 \x19[ 15x15 / 적 5초마다 경직 / 파괴될시 5분후 재생 / 설치형 ]");
		stb.printAt(4, "　　　\x04\x1F>SSS \x04>위협 \x19[ 대인 / 공성 ]");
		stb.printAt(5, "　　　\x04\x1F>CCC + AA \x04고문바퀴 \x19[ 공성 / 후속 스킬 귀환봉인, 자리고정 ]");
		stb.printAt(6, "　　　\x04\x1F>ACC \x04고문용 쇠사슬 \x19[ 대인 ]");
		stb.printAt(7, "\n");
		stb.printAt(8, "　　　\x08AAAC \x04갈망 \x19[ 공성 / 선딜없음 /  장판형 스킬 ] \x05", f.UltimateA[cp]);
		stb.printAt(9, "\n");
	}
}