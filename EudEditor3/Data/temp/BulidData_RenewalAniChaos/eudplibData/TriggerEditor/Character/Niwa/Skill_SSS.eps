import Function as f;

function main(cp)
{
   if (f.delay[cp] == 0)
   {
      if (f.count[cp] == 0)
      {
         if (f.loop[cp] == 0)
         {
            f.Table_Sin(cp, 0, 75);
            f.Table_Cos(cp, 0, 75);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Target", x, y);
            KillUnitAt(All, "Target", "Anywhere", cp);

            f.Table_Sin(cp, 45, 75);
            f.Table_Cos(cp, 45, 75);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Target", x, y);
            KillUnitAt(All, "Target", "Anywhere", cp);
         }
         else if (f.loop[cp] == 2)
         {
            f.Table_Sin(cp, 0, 150);
            f.Table_Cos(cp, 0, 150);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n Archon", x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);

            f.Table_Sin(cp, 45, 150);
            f.Table_Cos(cp, 45, 150);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            f.SquareShape(cp, 1, "60 + 1n Archon", x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", cp);

            f.Table_Sin(cp, 0, 75);
            f.Table_Cos(cp, 0, 75);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            f.Table_Sin(cp, 45, 75);
            f.Table_Cos(cp, 45, 75);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "Kakaru (Twilight)", x, y);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         }
         else if (f.loop[cp] == 4)
         {
            f.Table_Sin(cp, 0, 150);
            f.Table_Cos(cp, 0, 150);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Mutalisk", x, y);
            f.SquareShape(cp, 1, " Unit. Hoffnung 25000", x, y);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", cp);

            f.Table_Sin(cp, 45, 150);
            f.Table_Cos(cp, 45, 150);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "40 + 1n Mutalisk", x, y);
            f.SquareShape(cp, 1, " Unit. Hoffnung 25000", x, y);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
         }
         else if (f.loop[cp] == 6)
         {
            f.Table_Sin(cp, 0, 150);
            f.Table_Cos(cp, 0, 150);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "50 + 1n Tank", x, y);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);

            f.Table_Sin(cp, 45, 150);
            f.Table_Cos(cp, 45, 150);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "50 + 1n Tank", x, y);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);
         }
         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 8)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 1)
      {
         if (f.loop[cp] < 8)
         {
            f.Table_Sin(cp, 450 - 45 * f.loop[cp], 150);
            f.Table_Cos(cp, 450 - 45 * f.loop[cp], 150);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.DotShape(cp, 1, "40 + 1n Gantrithor", x, y);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", cp);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 8)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 2)
      {
         if (f.loop[cp] == 0)
         {
            f.Table_Sin(cp, 0, 75);
            f.Table_Cos(cp, 0, 75);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "80 + 1n Mutalisk", x, y);
            KillUnitAt(All, "80 + 1n Mutalisk", "Anywhere", cp);

            f.Table_Sin(cp, 45, 75);
            f.Table_Cos(cp, 45, 75);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "80 + 1n Mutalisk", x, y);
            KillUnitAt(All, "80 + 1n Mutalisk", "Anywhere", cp);
         }
         else if (f.loop[cp] == 2)
         {
            f.Table_Sin(cp, 0, 150);
            f.Table_Cos(cp, 0, 150);

            var x = f.CosAngle[cp];
            var y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "50 + 1n Battlecruiser", x, y);
            f.SquareShape(cp, 1, "40 + 1n Goliath", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);

            f.Table_Sin(cp, 45, 150);
            f.Table_Cos(cp, 45, 150);

            x = f.CosAngle[cp];
            y = f.SinAngle[cp];

            f.SquareShape(cp, 1, "50 + 1n Battlecruiser", x, y);
            f.SquareShape(cp, 1, "40 + 1n Goliath", x, y);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            Order("40 + 1n Goliath", cp, "Anywhere", Attack, f.location[cp]);
         }
         else if (f.loop[cp] == 3)
         {
            RemoveUnitAt(All, "40 + 1n Goliath", "Anywhere", cp);
         }
         
         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 10)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 3)
      {
         if (f.loop[cp] < 8)
         {
            f.MoveLoc("40 + 1n Mutalisk", cp, 0, 0);
            RemoveUnitAt(1, "40 + 1n Mutalisk", "Anywhere", cp);
            f.SkillUnit(cp, 1, "40 + 1n Drone");
            f.SkillUnit(cp, 1, " Unit. Hoffnung 25000");

            if (f.loop[cp] % 2 == 0)
            {
               f.SkillUnit(cp, 1, "40 + 1n Gantrithor");
            }
            else if (f.loop[cp] % 2 == 1)
            {
               f.SkillUnit(cp, 1, "50 + 1n Battlecruiser");
            }

            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", cp);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", cp);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            MoveUnit(All, "40 + 1n Drone", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 16)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 4)
      {
         if (f.loop[cp] == 0)
         {
            for (var i = 0; i < 8; i++)
            {
               f.MoveLoc("40 + 1n Drone", cp, 0, 0);
               RemoveUnitAt(1, "40 + 1n Drone", "Anywhere", cp);
               f.SkillUnit(cp, 1, "40 + 1n Mojo");
               f.SkillUnit(cp, 1, "60 + 1n Hydralisk");
            }

            KillUnitAt(All, "60 + 1n Hydralisk", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
         }
         if (f.loop[cp] == 1)
         {
            for (var i = 0; i < 8; i++)
            {
               f.MoveLoc("40 + 1n Mojo", cp, 0, 0);
               RemoveUnitAt(1, "40 + 1n Mojo", "Anywhere", cp);
               f.SkillUnit(cp, 1, "Kakaru (Twilight)");
            }

            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);
         }
         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 2)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 5)
      {
         f.SkillEnd(cp);
      }
   }
}