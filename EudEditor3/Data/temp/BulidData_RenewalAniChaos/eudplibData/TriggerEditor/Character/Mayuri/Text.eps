import PluginVariables as msqcvar;
import Function as f;

const looker = PVariable();

function Text(num);
function Looker()
{
	if (looker[getuserplayerid() - 128] > 0)
	{
		Text(looker[getuserplayerid() - 128]);
		looker[getuserplayerid() - 128] = 0;
	}
}

function main(cp)
{
	if (f.Mayuri_Voice[cp] > 0)
	{
		Text(f.Mayuri_Voice[cp]);
		looker[cp] = f.Mayuri_Voice[cp];
		f.Mayuri_Voice[cp] = 0;
	}
}

