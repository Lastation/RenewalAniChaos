import Character.Mayuri.Skill_AAAAA as AAAAA;

import Character.Mayuri.Text as text;
import Character.Mayuri.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 183;
	f.heroID[cp] = 64;
	
	f.UltimateA[cp] = 500;
	f.UltimateB[cp] = 250;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 310:
			AAS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		case 230:
			CCCAAAA.main(cp);
			break;
		case 320:
			AAA.main(cp);
			break;
		case 330:
			AAAAA.main(cp);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Mayuri_Voice[cp] != 0) { text.main(cp); }
}

