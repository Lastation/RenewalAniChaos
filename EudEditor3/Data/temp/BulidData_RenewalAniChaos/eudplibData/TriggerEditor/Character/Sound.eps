function Marge(cp);
function MargeSound(cp);

function main(cp)
{
	rusarukaMain.SkillVoice(cp);
	yuunaMain.SkillVoice(cp);
	mayuriMain.SkillVoice(cp);
	nanamiMain.SkillVoice(cp);
	milimMain.SkillVoice(cp);
	yumeMain.SkillVoice(cp);
	
	Marge(cp);
}

function Marge(cp)
{
	f.stb.printAt(0, "\x13\x04cp = ", cp, " getuserplayerid() = ", getuserplayerid());
	f.stb.printAt(1, "\x13\x04 v.sound_Text1[cp] = ", v.Sound_Text1[cp], " v.sound_Text2[cp] = ", v.Sound_Text2[cp]);
	f.stb.printAt(2, "\x13\x04 v.sound_Text3[cp] = ", v.Sound_Text3[cp], " v.sound_Text4[cp] = ", v.Sound_Text4[cp]);
	f.stb.printAt(2, "\x13\x04 v.Sound_Text_Uilti[cp] = ", v.Sound_Text_Uilti[cp], " v.Sound_Text_Uniq[cp] = ", v.Sound_Text_Uniq[cp]);
	
	if (cp < 6 && getuserplayerid() < 128)
	{
		MargeSound(cp);
	}
	else if (cp >= 8 && getuserplayerid() >= 128)
	{
		MargeSound(cp);
	}
}

