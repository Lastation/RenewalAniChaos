import Character.Nanami.Skill_UltB as UltB;

import Character.Nanami.Text as text;
import Character.Nanami.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 185;
	f.heroID[cp] = 64;
	
	f.UltimateA[cp] = 700;
	f.UltimateB[cp] = 500;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		case 230:
			CCA.main(cp);
			break;
		case 240:
			CCAAA.main(cp);
			break;
		case 250:
			CCAAAAA.main(cp);
			break;
		case 260:
			UltA.main(cp);
			break;
		case 310:
			UltB.main(cp);
			break;
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Nanami_Voice[cp] != 0) { text.main(cp); }
}

