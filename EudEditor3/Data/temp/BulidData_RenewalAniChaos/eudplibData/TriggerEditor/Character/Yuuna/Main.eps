const stb = StringBuffer();
const s = StringBuffer();
function SkillList(cp);

function main(cp)
{
	f.location[cp] = 181;
	f.heroID[cp] = 54;

	SkillList(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			//O.main(cp, f.location[cp], f.heroID[cp]);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 120:
			SSSSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		}
	}
	
	// Commed Setting
	commend.main(cp);
}

// Character Voice
function SkillVoice(cp)
{
	if (f.Yuuna_Voice[cp] != 0) { text.main(cp); }
}

