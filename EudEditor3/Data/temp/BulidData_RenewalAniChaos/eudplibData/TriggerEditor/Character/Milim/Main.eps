function main(cp)
{
	f.location[cp] = 188;
	f.heroID[cp] = 54;
	
	f.UltimateA[cp] = 650;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCAA.main(cp);
			break;
		case 220:
			CCC.main(cp);
			break;
		case 310:
			AAAA.main(cp);
			break;
			
		}
	}
	
	// Commed Setting

}

