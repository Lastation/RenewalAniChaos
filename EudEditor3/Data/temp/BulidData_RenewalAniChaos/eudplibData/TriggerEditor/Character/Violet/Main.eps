	f.heroID[cp] = 87;
	
	f.UltimateA[cp] = 650;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 100:
			S.main(cp);
			break;		
		}
	}
	
	// Commed Setting

}

