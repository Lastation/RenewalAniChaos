import Function as f;

import Character.Yume.Skill_O as O;
import Character.Yume.Skill_S as S;
import Character.Yume.Skill_C as C;
import Character.Yume.Skill_A as A;
import Character.Yume.Skill_SSS as SSS;
import Character.Yume.Skill_CC as CC;
import Character.Yume.Skill_CCA as CCA;
import Character.Yume.Skill_CAA as CAA;
import Character.Yume.Skill_CAAAC as CAAAC;
import Character.Yume.Skill_AAA as AAA;

import Character.Yume.Text as text;
import Character.Yume.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 190;
	f.heroID[cp] = 74;
	
	f.UltimateA[cp] = 500;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 230:
			CC.main(cp);
			break;
		case 240:
			CCA.main(cp);
			break;
		case 210:
			CAA.main(cp);
			break;
		case 220:
			CAAAC.main(cp);
			break;
		case 310:
			AAA.main(cp);
			break;
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Yume_Voice[cp] != 0) { text.main(cp); }
}

