import Function as f;

function NormalText(num)
{
	switch (num)
	{
	case 3110:
		PlayWAV("HotoMoka_01.ogg");
		f.stb.print("\n\n\x13\x1BHoto Moka\n\x13\x04어서오세욧~\n\n");
		break;
	case 3210:
		PlayWAV("HotoMoka_02.ogg");
		f.stb.print("\n\n\x13\x1BHoto Moka\n\x13\x04코코아가 젤 첨에 되고 싶었던거 기억나?\n\n");
		break;
	case 3220:
		PlayWAV("HotoMoka_03.ogg");
		break;
	case 3221:
		f.stb.print("\n\n\x13\x1BHoto Moka\n\x13\x04마법사!\n\n");
		break;
	}
}

function UiltimateText(num)
{
	switch (num)
	{
	case 9000:
		PlayWAV("HotoMoka_Uiltimate.ogg");
		f.stb.printAt(3,"\x13\x1BHoto Moka\n\n\x13\x04나, 준비해올게");
