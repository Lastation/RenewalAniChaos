import Function as f;

function main(cp)
{
	if (f.delay[cp] == 0)
	{
		if (f.count[cp] == 0)
		{
			f.CrossShape(cp, 1, "40 + 1n Wraith", 45, 3, 64);
			f.CrossShape(cp, 1,"60 + 1n Dragoon", 45, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Wraith", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 1)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", cp);
		
			f.LineShape(cp, 1, "40 + 1n Mojo", 45, 3, 64, 64);
			f.LineShape(cp, 1, "40 + 1n Mojo", 225, 3, 64, 64);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 45, 3, 64, 64);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 225, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 2)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
		
			f.LineShape(cp, 1, "40 + 1n Mojo", 45, 1, 64, 128);
			f.LineShape(cp, 1, "40 + 1n Mojo", 225, 1, 64, 128);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 45, 1, 64, 128);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 225, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 3)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
		
			f.LineShape(cp, 1, "40 + 1n Mutalisk", 135, 3, 64, 64);
			f.LineShape(cp, 1, "40 + 1n Mutalisk", 315, 3, 64, 64);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 135, 3, 64, 64);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 315, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 4)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);
		
			f.LineShape(cp, 1, "40 + 1n Mutalisk", 135, 1, 64, 128);
			f.LineShape(cp, 1, "40 + 1n Mutalisk", 315, 1, 64, 128);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 135, 1, 64, 128);
			f.LineShape(cp, 1, "60 + 1n Dragoon", 315, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		if (f.count[cp] == 5)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);
			
			f.CrossShape(cp, 1, "40 + 1n Wraith", 0, 3, 64);
			f.CrossShape(cp, 1,"60 + 1n Dragoon", 0, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", cp);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Wraith", cp, "Anywhere", Attack, f.location[cp]);
			f.SkillWait(cp, 90);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 6)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", cp);
			f.SkillEnd(cp);
		}
	}
}