import Function as f;

import Character.Chtholly.Skill_O as O;
import Character.Chtholly.Skill_S as S;
import Character.Chtholly.Skill_C as C;
import Character.Chtholly.Skill_A as A;

import Character.Chtholly.Text as text;
import Character.Chtholly.Commend as commend;

const stb = StringBuffer();

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 32;
	f.heroID[cp] = 51;
	
	f.UltimateA[cp] = 700;
	f.UltimateB[cp] = 700;
	
	// Commed Setting
	commend.main(cp);
	SkillList(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		case 310:
			ACC.main(cp);
			break;
		case 320:
			AAAC.main(cp);
			break;
		}
	}
	
}

// Character Voice
function SkillVoice(cp)
{
	if (f.Rusaruka_Voice[cp] != 0) { text.main(cp); }
}

function SkillList(cp)
{
