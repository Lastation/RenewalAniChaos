import PluginVariables as msqcvar;
import Function as f;

function main(cp)
{
	MoveUnit(All, "40 + 1n Marine", cp, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "40 + 1n Mojo", cp, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "50 + 1n Tank", cp, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "60 + 1n Danimoth", cp, "Anywhere", "[Skill]HoldPosition");
	f.BanReturn(cp);

	if (f.delay[cp] == 0)
	{
		if (f.count[cp] == 0 && f.loop[cp] <= 6)
		{
			f.SquareShape(cp, 1, "80 + 1n Tank", 16 * (f.loop[cp] + 1), 16 * (f.loop[cp] + 1));
			f.SquareShape(cp, 1, "Zerg Devourer", 16 * (f.loop[cp] + 1), 16 * (f.loop[cp] + 1));
			KillUnitAt(All, "80 + 1n Tank", "Anywhere", cp);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 0 && f.loop[cp] == 7)
		{
			f.SquareShape(cp, 1, "50 + 1n Tank", 16 * (f.loop[cp] + 1), 16 * (f.loop[cp] + 1));
			f.SquareShape(cp, 1, "Zerg Devourer", 16 * (f.loop[cp] + 1), 16 * (f.loop[cp] + 1));
			KillUnitAt(All, "Zerg Devourer", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 1 && f.loop[cp] <= 7)
		{
			f.SquareShape(cp, 1, "80 + 1n Tank", 20 * (f.loop[cp] + 1), 0);
			f.SquareShape(cp, 1, "Zerg Devourer", 20 * (f.loop[cp] + 1), 0);
			KillUnitAt(All, "80 + 1n Tank", "Anywhere", cp);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 1 && f.loop[cp] == 8)
		{
			f.SquareShape(cp, 1, "50 + 1n Tank", 20 * (f.loop[cp] + 1), 0);
			f.SquareShape(cp, 1, "Zerg Devourer",  20 * (f.loop[cp] + 1), 0);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			MoveUnit(All, "50 + 1n Tank", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
			Order("50 + 1n Tank", cp, "Anywhere", Attack, f.location[cp]);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", cp);
			f.SkillWait(cp, 300);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 2)
		{
			f.SquareShape(cp, 1, "40 + 1n Marine", 64, 64);
			f.SquareShape(cp, 1, "40 + 1n Marine", 90, 0);
			f.SquareShape(cp, 8, "40 + 1n Lurker",  64, 64);
			f.SquareShape(cp, 8, "40 + 1n Lurker",  90, 0);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			MoveUnit(All, "40 + 1n Marine", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
			Order("40 + 1n Marine", cp, "Anywhere", Attack, f.location[cp]);
			KillUnitAt(All, "40 + 1n Lurker", "Anywhere", cp);
			f.SkillWait(cp, 3500);
			f.count[cp] += 1;
		}
		else if (f.count[cp] == 3 && f.loop[cp] <= 3)
		{
			f.SquareShape(cp, 8, "60 + 1n High Templar", 32 * (f.loop[cp] + 1), 32 * (f.loop[cp] + 1));
			f.SquareShape(cp, 8, "60 + 1n High Templar", 45 * (f.loop[cp] + 1), 0);
			KillUnitAt(All, "60 + 1n High Templar", "Anywhere", cp);
			f.SkillWait(cp, 100);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 3 && f.loop[cp] == 4)
		{
			f.SquareShape(cp, 1, "40 + 1n Mojo", 64, 64);
			f.SquareShape(cp, 8, "Torrasque", 90, 0);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("40 + 1n Mojo", cp, "Anywhere", Attack, f.location[cp]);
			KillUnitAt(All, "Torrasque", "Anywhere", cp);
			f.SkillWait(cp, 0);
			f.loop[cp] += 1;
		}
		else if (f.count[cp] == 3 && f.loop[cp] == 5)
		{
			f.Voice_Routine(cp, 2);
			f.SquareShape(cp, 1, "60 + 1n Danimoth", 180, 0);
			f.SquareShape(cp, 8, "Torrasque", 128, 128);
			f.MoveLoc(f.heroID[cp], cp, 0, 0);
			Order("60 + 1n Danimoth", cp, "Anywhere", Attack, f.location[cp]);
			KillUnitAt(All, "Torrasque", "Anywhere", cp);
			f.SkillWait(cp, 2750);
			f.count[cp] += 1;
			f.loop[cp] = 0;
		}
		else if (f.count[cp] == 4)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", cp);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", cp);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", cp);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", cp);
			f.SkillEnd(cp);
		}
	}
}