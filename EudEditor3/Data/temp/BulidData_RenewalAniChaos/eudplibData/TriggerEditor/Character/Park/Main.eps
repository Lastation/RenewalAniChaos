import Function as f;

import Character.Park.Skill_S as S;
import Character.Park.Skill_C as C;

import Character.Park.Text as text;
import Character.Park.Commend as commend;

var loop  = 0;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 199;
	f.heroID[cp] = 10;
	
	f.UltimateA[cp] = 500;

	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		}
	}
	

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Park_Voice[cp] != 0) { text.main(cp); }
}

