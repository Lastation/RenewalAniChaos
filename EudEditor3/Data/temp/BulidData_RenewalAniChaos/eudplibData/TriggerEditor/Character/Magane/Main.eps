import Function as f;

import Character.Magane.Skill_O as O;
import Character.Magane.Skill_S as S;
import Character.Magane.Skill_C as C;
import Character.Magane.Skill_A as A;

import Character.Magane.Skill_SSS as SSS;
import Character.Magane.Skill_ASSS as ASSS;
import Character.Magane.Skill_CCC as CCC;
import Character.Magane.Skill_CCCAA as CCCAA;

import Character.Magane.Skill_AAA as AAA;

import Character.Magane.Text as text;
import Character.Magane.Commend as commend;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 191;
	f.heroID[cp] = 53;
	
	f.UltimateA[cp] = 500;
	f.UltimateB[cp] = 250;
	
	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 310:
			ASSS.main(cp);
			break;
		case 210:
			CCC.main(cp);
			break;
		case 220:
			CCCAA.main(cp);
			break;
		case 320:
			AAA.main(cp);
			break;
			
			
		}
	}
	
	// Commed Setting

}

// Character Voice
function SkillVoice(cp)
{
	if (f.Magane_Voice[cp] != 0) { text.main(cp); }
}

