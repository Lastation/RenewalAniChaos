import Function as f;

function EdgeShapeAt(cp : TrgPlayer, count, Unit : TrgUnit, degree, n, interval, x, y);
function EdgeShapeAtBurrowed(cp : TrgPlayer, count, Unit : TrgUnit, degree, n, interval, x, y);

function main(cp)
{
   if (f.delay[cp] == 0)
   {
      if (f.count[cp] == 0)
      {
         RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);

         var i = f.loop[cp];
         var d = 25 + 25 * i;

         f.SquareShape(cp, 1, "40 + 1n Guardian", d, 0);
         EdgeShapeAt(cp, 1, "Bengalaas (Jungle)", 45, 2, 25, d, 0);
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", cp);

         EdgeShapeAtBurrowed(cp, 1, "Zerg Defiler", 45, 2, 25, d, 0);

         if (f.loop[cp] % 2 == 0)
         {
            f.DotShape(cp, 1, "Kakaru (Twilight)", 0, 0);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            f.EdgeShape(cp, 1, "40 + 1n Mutalisk", 45, 3, 50);
            f.EdgeShape(cp, 1, "Scantid (Desert)", 45, 3, 50);
            KillUnitAt(All, "Scantid (Desert)", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 8)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 1)
      {
         if (f.loop[cp] == 0)
         {
            f.EdgeShapeBurrowed(cp, 1, "Zerg Defiler", 45, 3, 50);
         }

         RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);

         var i = (f.loop[cp] % 4);
         var x = 50 + 50 * i;
         var y = 150 - 50 * i;

         f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
         EdgeShapeAt(cp, 1, "Rhynadon (Badlands)", 45, 3, 50, x, y);
         EdgeShapeAt(cp, 1, "40 + 1n Mutalisk", 45, 3, 50, x, y);
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", cp);

         EdgeShapeAtBurrowed(cp, 1, "Zerg Defiler", 45, 2, 50, x, y);

         if (f.loop[cp] % 2 == 0)
         {
            f.DotShape(cp, 1, "Kakaru (Twilight)", 0, 0);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            f.EdgeShape(cp, 1, "40 + 1n Mutalisk", 45, 3, 50);
            f.EdgeShape(cp, 1, "Scantid (Desert)", 45, 3, 50);
            KillUnitAt(All, "Scantid (Desert)", "Anywhere", cp);
         }

         MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
         Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 8)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 2)
      {
         if (f.loop[cp] == 0)
         {
            EdgeShapeAtBurrowed(cp, 1, "Zerg Defiler", 45, 3, 50, 160, 160);
         }

         if (f.loop[cp] % 2 == 0 && f.loop[cp] < 4)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);

            f.DotShape(cp, 1, "Kakaru (Twilight)", 0, 0);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            f.EdgeShape(cp, 1, "40 + 1n Mutalisk", 45, 3, 50);
            f.EdgeShape(cp, 1, "Scantid (Desert)", 45, 3, 50);
            KillUnitAt(All, "Scantid (Desert)", "Anywhere", cp);

            var i = (f.loop[cp] % 4) / 2;

            var x = 200;
            var y = 200;

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            EdgeShapeAt(cp, 1, "Rhynadon (Badlands)", 45, 3, 50, x, y);
            EdgeShapeAt(cp, 1, "40 + 1n Mutalisk", 45, 3, 50, x, y);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
         }
         if (f.loop[cp] == 4)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);

            f.EdgeShape(cp, 1, "Kakaru (Twilight)", 45, 3, 50);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", cp);

            var x = 200;
            var y = 200;

            f.SquareShape(cp, 1, "40 + 1n Guardian", x, y);
            EdgeShapeAt(cp, 1, "Protoss Dark Archon", 45, 3, 50, x, y);
            EdgeShapeAt(cp, 1, "40 + 1n Guardian", 45, 3, 50, x, y);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            Order("40 + 1n Guardian", cp, "Anywhere", Attack, f.location[cp]);
         }
         if (f.loop[cp] == 8)
         {
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);

            var x = 200;
            var y = 200;

            EdgeShapeAt(cp, 1, " Unit. Hoffnung 25000", 45, 3, 50, x, y);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", cp);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 19)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 3)
      {
         if (Bring(cp, AtLeast, 1, "Zerg Defiler", "Anywhere"))
         {
            CreateUnit(2, "40 + 1n Mutalisk", dwrand() % 8 + 33, cp);
            CreateUnit(2, "40 + 1n Guardian", dwrand() % 8 + 33, cp);
            CreateUnit(1, " Unit. Hoffnung 25000", dwrand() % 8 + 33, cp);
            CreateUnit(1, "40 + 1n Zergling", dwrand() % 8 + 33, cp);
            CreateUnit(2, " Creep. Licht", dwrand() % 8 + 33, cp);
            CreateUnit(4, "80 + 1n Guardian", dwrand() % 8 + 33, cp);
            CreateUnit(4, "Protoss Dark Archon", dwrand() % 8 + 33, cp);
            SetInvincibility(Enable, "Any unit", cp, "[Skill]Unit_Wait_ALL");
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "Protoss Dark Archon", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "80 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Zergling", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "Protoss Dark Archon", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Mutalisk", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "80 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, " Creep. Licht", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "80 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, " Unit. Hoffnung 25000", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "80 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, " Creep. Licht", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "Protoss Dark Archon", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Mutalisk", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "80 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Zergling", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveLocation(f.location[cp], "Zerg Defiler", cp, "Anywhere");
            RemoveUnitAt(2, "Zerg Defiler", "Anywhere", cp);
            MoveUnit(1, "Protoss Dark Archon", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(1, "40 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);

            KillUnitAt(All, "80 + 1n Guardian", "Anywhere", cp);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", cp);

            MoveLocation(f.location[cp], f.heroID[cp], cp, "Anywhere");
            MoveUnit(All, " Unit. Hoffnung 25000", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(All, "40 + 1n Zergling", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(All, " Creep. Licht", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(All, "40 + 1n Mutalisk", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            MoveUnit(All, "40 + 1n Guardian", cp, "[Skill]Unit_Wait_ALL", f.location[cp]);
            Order(" Unit. Hoffnung 25000", cp, "Anywhere", Attack, f.location[cp]);
            Order("40 + 1n Zergling", cp, "Anywhere", Attack, f.location[cp]);
            Order(" Creep. Licht", cp, "Anywhere", Attack, f.location[cp]);
            Order("40 + 1n Mutalisk", cp, "Anywhere", Attack, f.location[cp]);
            Order("40 + 1n Guardian", cp, "Anywhere", Attack, f.location[cp]);
         }

         f.SkillWait(cp, 80);

         f.loop[cp] += 1;

         if (f.loop[cp] == 20)
         {
            f.count[cp] += 1;
            f.loop[cp] = 0;         
         }
      }
      else if (f.count[cp] == 4)
      {
         KillUnitAt(All, "40 + 1n Zergling", "Anywhere", cp);
         KillUnitAt(All, "40 + 1n Drone", "Anywhere", cp);
         KillUnitAt(All, "60 + 1n Siege", "Anywhere", cp);
         KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", cp);
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", cp);
         KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", cp);
         KillUnitAt(All, " Creep. Licht", "Anywhere", cp);

         f.SkillEnd(cp);
      }
   }
}

function EdgeShapeAt(cp : TrgPlayer, count, Unit : TrgUnit, degree, n, interval, x, y)
{
   f.EdgeShapeAt(cp, count, Unit, degree, n, interval, x, y);
   f.EdgeShapeAt(cp, count, Unit, degree, n, interval, -x, -y);
   f.EdgeShapeAt(cp, count, Unit, degree, n, interval, -y, x);
   f.EdgeShapeAt(cp, count, Unit, degree, n, interval, y, -x);
}

function EdgeShapeAtBurrowed(cp : TrgPlayer, count, Unit : TrgUnit, degree, n, interval, x, y)
{
   f.EdgeShapeAtBurrowed(cp, count, Unit, degree, n, interval, x, y);
   f.EdgeShapeAtBurrowed(cp, count, Unit, degree, n, interval, -x, -y);
   f.EdgeShapeAtBurrowed(cp, count, Unit, degree, n, interval, -y, x);
   f.EdgeShapeAtBurrowed(cp, count, Unit, degree, n, interval, y, -x);
}