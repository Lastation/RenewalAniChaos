import Function as f;

import Character.Oda.Skill_A as A;
import Character.Oda.Skill_S as S;
import Character.Oda.Skill_C as C;
import Character.Oda.Skill_O as O;

import Character.Oda.Skill_SSS as SSS;
import Character.Oda.Skill_SSSCC as SSSCC;
import Character.Oda.Skill_SSSCCAA as SSSCCAA;

import Character.Oda.Skill_CC as CC;
import Character.Oda.Skill_CCA as CCA;
import Character.Oda.Skill_CCAAA as CCAAA;
import Character.Oda.Skill_CCAAAAA as CCAAAAA;

import Character.Oda.Skill_AASS as AASS;

import Character.Oda.Skill_AAAA as AAAA;
import Character.Oda.Skill_AAAAAA as AAAAAA;
import Character.Oda.Skill_AO as AO;

import Character.Oda.Text as text;
import Character.Oda.Commend as commend;

var loop  = 0;

function SkillList(cp);

function main(cp)
{
	f.location[cp] = 193;
	f.heroID[cp] = 99;
	
	f.UltimateA[cp] = 650;
	f.UltimateB[cp] = 300;

	SkillList(cp);
	commend.main(cp);
	
	if (f.wait[cp] == 0)
	{
		switch(f.step[cp])
		{
		case 1:
			O.main(cp);
			break;
		case 100:
			S.main(cp);
			break;
		case 110:
			SSS.main(cp);
			break;
		case 120:
			SSSCC.main(cp);
			break;
		case 130:
			SSSCCAA.main(cp);
			break;
		case 200:
			C.main(cp);
			break;
		case 210:
			CC.main(cp);
			break;
		case 220:
			CCA.main(cp);
			break;
		case 230:
			CCAAA.main(cp);
			break;
		case 240:
			CCAAAAA.main(cp);
			break;
		case 300:
			A.main(cp);
			break;
		case 310:
			AAAA.main(cp);
			break;
		case 320:
			AAAAAA.main(cp);
			break;
		case 330:
			AASS.main(cp);
			break;
		case 340:
			AO.main(cp);
			break;
			
		}
	}
	
	loop++;
	if (loop > 12)
	{
		O.savePosRoutine(cp);
		loop = 0;
	}
}

// Character Voice
function SkillVoice(cp)
{
	if (f.Oda_Voice[cp] != 0) { text.main(cp); }
}

function SkillList(cp)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      f.stb.printAt(0, "\n");
	      f.stb.printAt(1, "\x1F　＃\x1B- 오다 사쿠노스케　\x04[ 문호 스트레이독스 ]");
	      f.stb.printAt(2, "　　\x1FA\x04ction List");
	      f.stb.printAt(3, "　　　\x18O \x04천의무봉 \x19[ 5초 전의 장소로 이동 ] \x053분");
	      f.stb.printAt(4, "　　　\x04\x1FSSS + CC + AA \x04꿈 \x19[ 공성 / CC - 자리고정 ]");
	      f.stb.printAt(5, "　　　\x04\x1FCC + A + AA + AA \x04실의 \x19[ 공성 / 자리고정 / 각성시 사용 가능 ]");
	      f.stb.printAt(6, "　　　\x04\x1FAA \x04전투술 \x19[ 대인 / 순간딜 / 각성시 사용 가능 ]");
	      f.stb.printAt(7, "　　　\x08AO \x04킬러 \x04 \x19[ 절망 사용시 사용 가능 / 영구 각성 / 자리고정 / 30초 ]");
	      f.stb.printAt(8, "　　　\x08ACCC \x04절규 \x19[ 공성 / 자리고정 ] \x05", f.UltimateA[cp]);
	      f.stb.printAt(9, "　　　\x08 + AA \x04절망 \x19[ 공성 / 선딜 10초 / 선딜 후 자리고정 ] \x05", f.UltimateB[cp]);
	      f.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}