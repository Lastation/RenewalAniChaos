import PluginVariables as msqcvar;
import func.trig as trg;
import variable as v;
import system.text as text;

import character.group6.hodaka.main 	as hodaka;


function main(playerID)
{
	trg.Main_WaitLoop();
	
	text.BuildingText(playerID);
	text.ShopText(playerID);
	
	v.P_UltimateGauge[playerID] = 1000;
	
	ModifyUnitHitPoints(1, 155, playerID, "Anywhere", 100 - (v.P_UniqueCool[playerID] / 10));
	ModifyUnitShields(1, 167, playerID, "Anywhere", v.P_UltimateGauge[playerID] / 10);
	
	if 	(v.P_HeroID[playerID] == 32) 	hodaka.main(playerID);
	
	SetDeaths(playerID, SetTo, v.P_Shield[playerID], " `SystemShield");
		
	//Test
       if(playerID < 6 && bread(0x58D2B0 + 0x207C * (0 / 46) + playerID * (46 - 31 * (0 / 46)) + (0 % 46)) != 80)
       {
		bwrite(0x58D2B0 + 0x207C * (0 / 46) + playerID * (46 - 31 * (0 / 46)) + (0 % 46), 60);
		bwrite(0x58D2B0 + 0x207C * (7 / 46) + playerID * (46 - 31 * (7 / 46)) + (7 % 46), 80);
		
		bwrite(0x58D2B0 + 0x207C * (2 / 46) + playerID * (46 - 31 * (2 / 46)) + (2 % 46), 60); 	//방어력 최종 적용
		bwrite(0x58D2B0 + 0x207C * (9 / 46) + playerID * (46 - 31 * (9 / 46)) + (9 % 46), 80);		//공격력 최종 적용
	}
}