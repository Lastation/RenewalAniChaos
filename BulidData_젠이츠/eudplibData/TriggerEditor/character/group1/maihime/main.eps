import PluginVariables as msqcvar;
import variable as v;

import character.group1.maihime.commend as commend;

import character.group1.maihime.common.skill1 as S;
import character.group1.maihime.common.skill2 as C;
import character.group1.maihime.common.skill3 as A;

import character.group1.maihime.combo.combo1 as SSS;
import character.group1.maihime.combo.combo2 as CCC;
import character.group1.maihime.combo.combo3 as AACC;
import character.group1.maihime.combo.combo4 as ACCCC;

import character.group1.maihime.ultimate.ultimate1 as AAAA;


function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 모리시마 \x1B호다카　\x04[ 날씨의 아이 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18 O\x04 관심 \x19[ 상대방 o 쿨타임 10초 증가 ] \x05개인쿨 5분");
		v.stb.printAt(4, "　　　\x1F SSS\x04 가출 \x19[ 만능 ]");
		v.stb.printAt(5, "　　　\x1F CCC\x04 장소 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(6, "　　　\x1F AACC + 200 Gas\x04 날씨 \x19[ 만능 / 자리고정 / 순간딜 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08 AAAA \x04 기도\x19[ 공성 / 선딜 10초 / 쉴드 1고정 ] \x1F[1] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 3;
	v.P_UnitID[playerID] 			= 74;
	v.P_LocationID[playerID] 		= 121;
	
	v.P_Ultimate1[playerID] = 750;
	
	
	commend.main(playerID);
	skillList(playerID);
	
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 310:
			AACC.main(playerID);
			break;
		case 320:
			AAAA.main(playerID);
			break;
		case 330:
			ACCCC.main(playerID);
			break;		
		}
	}
}