import TitleVariable as tv;
import Variable as v;

function Total_CS_Setting(cp)
{
	if 		(v.CS_Total[cp] < tv.Total_CS_Need[0])			{ tv.Enable_Title_Num[cp] = 0; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[1])			{ tv.Enable_Title_Num[cp] = 1; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[2])			{ tv.Enable_Title_Num[cp] = 2; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[3])			{ tv.Enable_Title_Num[cp] = 3; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[4])			{ tv.Enable_Title_Num[cp] = 4; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[5])			{ tv.Enable_Title_Num[cp] = 5; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[6])			{ tv.Enable_Title_Num[cp] = 6; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[7])			{ tv.Enable_Title_Num[cp] = 7; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[8])			{ tv.Enable_Title_Num[cp] = 8; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[9])			{ tv.Enable_Title_Num[cp] = 9; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[10])		{ tv.Enable_Title_Num[cp] = 10; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[11])		{ tv.Enable_Title_Num[cp] = 11; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[12])		{ tv.Enable_Title_Num[cp] = 12; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[13])		{ tv.Enable_Title_Num[cp] = 13; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[14])		{ tv.Enable_Title_Num[cp] = 14;	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[15])		{ tv.Enable_Title_Num[cp] = 15; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[16])		{ tv.Enable_Title_Num[cp] = 16; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[17])		{ tv.Enable_Title_Num[cp] = 17; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[18])		{ tv.Enable_Title_Num[cp] = 18; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[19])		{ tv.Enable_Title_Num[cp] = 19; 	}
	else if 	(v.CS_Total[cp] < tv.Total_CS_Need[20])		{ tv.Enable_Title_Num[cp] = 20; 	}
	else 												{ tv.Enable_Title_Num[cp] = 21; 	}
	
	tv.Title_Max[cp] = 21;
}