// 필드에 몬스터가 있고 플레이어가 없으면 30초 뒤에 몬스터를 모두 제거

const Goblin = EUDArray(5);

function GoblinSpawn()
{
	for(var i = 0; i < 5; i++)
	{
		if(Goblin[i] == 0)
		{
			Goblin[i] = epdread_epd(EPD(0x628438));
			CreateUnit(1, "Zerg Zergling", "Start", P1);
		}
		
		if(bread_epd(Goblin[i] + 0x04D / 4,  0x04D % 4) == 0)
		{
			Goblin[i] = 0;
		}
	}
}