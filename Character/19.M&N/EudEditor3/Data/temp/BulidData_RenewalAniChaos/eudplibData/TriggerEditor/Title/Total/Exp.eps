	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[5])		{ tv.Enable_Title_Num[cp] = 5; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[6])		{ tv.Enable_Title_Num[cp] = 6; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[7])		{ tv.Enable_Title_Num[cp] = 7; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[8])		{ tv.Enable_Title_Num[cp] = 8; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[9])		{ tv.Enable_Title_Num[cp] = 9; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[10])		{ tv.Enable_Title_Num[cp] = 10; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[11])		{ tv.Enable_Title_Num[cp] = 11; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[12])		{ tv.Enable_Title_Num[cp] = 12; 	}
