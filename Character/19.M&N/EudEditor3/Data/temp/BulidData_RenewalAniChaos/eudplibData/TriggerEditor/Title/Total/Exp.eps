	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[19])		{ tv.Enable_Title_Num[cp] = 19; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[20])		{ tv.Enable_Title_Num[cp] = 20; 	}
	else 												{ tv.Enable_Title_Num[cp] = 21; 	}
	
	tv.Title_Max[cp] = 21;
}