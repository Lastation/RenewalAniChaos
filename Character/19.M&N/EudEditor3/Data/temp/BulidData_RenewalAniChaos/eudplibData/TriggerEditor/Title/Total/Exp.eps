	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[14])		{ tv.Enable_Title_Num[cp] = 14;	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[15])		{ tv.Enable_Title_Num[cp] = 15; 	}
	else if 	(v.Exp_Total[cp] < tv.Total_Exp_Need[16])		{ tv.Enable_Title_Num[cp] = 16; 	}
