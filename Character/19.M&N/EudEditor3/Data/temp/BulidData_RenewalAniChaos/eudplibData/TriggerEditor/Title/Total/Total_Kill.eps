import TitleVariable as tv;
import Variable as v;

function Total_Kill_Setting(cp)
{
	if 		(v.Exp_Total[cp] < 	50		)		{ tv.Enable_Title_Num = 0; 	}
	else if 	(v.Exp_Total[cp] < 	100		)		{ tv.Enable_Title_Num = 1; 	}
	else if 	(v.Exp_Total[cp] < 	200		)		{ tv.Enable_Title_Num = 2; 	}
	else if 	(v.Exp_Total[cp] < 	300		)		{ tv.Enable_Title_Num = 3; 	}
	else if 	(v.Exp_Total[cp] < 	400		)		{ tv.Enable_Title_Num = 4; 	}
	else if 	(v.Exp_Total[cp] < 	500		)		{ tv.Enable_Title_Num = 5; 	}
	else if 	(v.Exp_Total[cp] < 	600		)		{ tv.Enable_Title_Num = 6; 	}
	else if 	(v.Exp_Total[cp] < 	700		)		{ tv.Enable_Title_Num = 7; 	}
	else if 	(v.Exp_Total[cp] < 	777		)		{ tv.Enable_Title_Num = 8; 	}
	else if 	(v.Exp_Total[cp] < 	800		)		{ tv.Enable_Title_Num = 9; 	}
	else if 	(v.Exp_Total[cp] < 	900		)		{ tv.Enable_Title_Num = 10; 	}
	else if 	(v.Exp_Total[cp] < 	1000	)		{ tv.Enable_Title_Num = 11; 	}
	else if 	(v.Exp_Total[cp] < 	2000	)		{ tv.Enable_Title_Num = 12; 	}
	else if 	(v.Exp_Total[cp] < 	3000	)		{ tv.Enable_Title_Num = 13; 	}
	else if 	(v.Exp_Total[cp] < 	4000	)		{ tv.Enable_Title_Num = 14;	}
	else if 	(v.Exp_Total[cp] < 	5000	)		{ tv.Enable_Title_Num = 15; 	}
	else if 	(v.Exp_Total[cp] < 	6000	)		{ tv.Enable_Title_Num = 16; 	}
	else if 	(v.Exp_Total[cp] < 	7000	)		{ tv.Enable_Title_Num = 17; 	}
	else if 	(v.Exp_Total[cp] < 	8000	)		{ tv.Enable_Title_Num = 18; 	}
	else if 	(v.Exp_Total[cp] < 	9000	)		{ tv.Enable_Title_Num = 19; 	}
	else if 	(v.Exp_Total[cp] < 	10000	)		{ tv.Enable_Title_Num = 20; 	}
	else 										{ tv.Enable_Title_Num = 21; 	}
}