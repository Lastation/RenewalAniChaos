import TitleVariable as tv;
import Variable as v;

function Total_Exp_Setting(cp)
{
	if 		(v.Exp_Total[cp] < 	500		)		{ tv.Enable_Title_Num = 0; 	}
	else if 	(v.Exp_Total[cp] < 	1000	)		{ tv.Enable_Title_Num = 1; 	}
	else if 	(v.Exp_Total[cp] < 	2500	)		{ tv.Enable_Title_Num = 2; 	}
	else if 	(v.Exp_Total[cp] < 	5000	)		{ tv.Enable_Title_Num = 3; 	}
	else if 	(v.Exp_Total[cp] < 	7500	)		{ tv.Enable_Title_Num = 4; 	}
	else if 	(v.Exp_Total[cp] < 	10000	)		{ tv.Enable_Title_Num = 5; 	}
	else if 	(v.Exp_Total[cp] < 	20000	)		{ tv.Enable_Title_Num = 6; 	}
	else if 	(v.Exp_Total[cp] < 	25000	)		{ tv.Enable_Title_Num = 7; 	}
	else if 	(v.Exp_Total[cp] < 	30000	)		{ tv.Enable_Title_Num = 8; 	}
	else if 	(v.Exp_Total[cp] < 	35000	)		{ tv.Enable_Title_Num = 9; 	}
	else if 	(v.Exp_Total[cp] < 	40000	)		{ tv.Enable_Title_Num = 10; 	}
	else if 	(v.Exp_Total[cp] < 	45000	)		{ tv.Enable_Title_Num = 11; 	}
	else if 	(v.Exp_Total[cp] < 	50000	)		{ tv.Enable_Title_Num = 12; 	}
	else if 	(v.Exp_Total[cp] < 	55000	)		{ tv.Enable_Title_Num = 13; 	}
	else if 	(v.Exp_Total[cp] < 	60000	)		{ tv.Enable_Title_Num = 14;	}
	else if 	(v.Exp_Total[cp] < 	65000	)		{ tv.Enable_Title_Num = 15; 	}
	else if 	(v.Exp_Total[cp] < 	70000	)		{ tv.Enable_Title_Num = 16; 	}
	else if 	(v.Exp_Total[cp] < 	75000	)		{ tv.Enable_Title_Num = 17; 	}
	else if 	(v.Exp_Total[cp] < 	80000	)		{ tv.Enable_Title_Num = 18; 	}
	else if 	(v.Exp_Total[cp] < 	90000	)		{ tv.Enable_Title_Num = 19; 	}
	else if 	(v.Exp_Total[cp] < 	100000	)		{ tv.Enable_Title_Num = 20; 	}
	else 										{ tv.Enable_Title_Num = 21; 	}
}