import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

var x = 0;
function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Double(playerID,1,"60 + 1n High Templar",96,0);
				trg.Shape_Double(playerID,1,"60 + 1n High Templar",0,96);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",0,96);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",96,0);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n High Templar",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Double(playerID,1,"60 + 1n High Templar",48,48);
				trg.Shape_Double(playerID,1,"60 + 1n High Templar",-48,48);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",48,48);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",-48,48);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);	
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n High Templar",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Double(playerID,1,"60 + 1n Dragoon",144,0);
				trg.Shape_Double(playerID,1,"60 + 1n Dragoon",0,144);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",144,0);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",0,144);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Dragoon",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Double(playerID,1,"60 + 1n Dragoon",72,72);
				trg.Shape_Double(playerID,1,"60 + 1n Dragoon",-72,72);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",72,72);
				trg.Shape_Double(playerID,1,"40 + 1n Mojo",-72,72);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Dragoon",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Main_Wait(600);
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				trg.Shape_Edge(playerID,1,"60 + 1n Danimoth",45,3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Danimoth",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(400);
			}
			else if (v.P_LoopMain[playerID] <= 13)
			{
				KillUnitAt(1,"60 + 1n Dragoon","Anywhere",playerID);
				KillUnitAt(1,"60 + 1n High Templar","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if (v.P_LoopMain[playerID] <= 21)
			{
				KillUnitAt(1,"60 + 1n Danimoth","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if (v.P_LoopMain[playerID] == 22)
			{
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}