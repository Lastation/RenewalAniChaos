import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.trigepic as epic;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			RemoveUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			RemoveUnitAt(All," Creep. Dunkelheit", "Anywhere", playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Goliath",64,64);
				trg.Shape_Square(playerID,1,"40 + 1n Gantrithor",64,64);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Goliath",45,3,128);
				trg.Shape_Edge(playerID,1,"40 + 1n Gantrithor",45,3,128);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Double(playerID,1,"40 + 1n Wraith",96,0);
				trg.Shape_Double(playerID,1,"Protoss Dark Archon",96,0);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Double(playerID,1,"40 + 1n Wraith",0,96);
				trg.Shape_Double(playerID,1,"Protoss Dark Archon",0,96);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Wraith",96,96);
				trg.Shape_Square(playerID,1,"60 + 1n Archon",96,96);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}