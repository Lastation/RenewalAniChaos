import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

function main(playerID)
{
	if(v.P_LoopMain[playerID] >= 16)
	{
		trg.Buff_ShieldFix(1);
	}
	if (v.P_WaitMain[playerID] == 0)
	{
		
		if (v.P_CountMain[playerID] == 0)
		{
			RemoveUnitAt(All,"40 + 1n Ghost","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Mutalisk","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Guardian","Anywhere",playerID);
			RemoveUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
			
			if(v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Mutalisk",45,3,48);
				trg.Shape_Square(playerID,1,"40 + 1n Marine",64,64);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",64,64);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.Shape_Edge(playerID,1,"60 + 1n Siege",0,4,192);
				trg.Shape_Dot(playerID,1,"40 + 1n Mutalisk",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mutalisk",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 1)
			{
				trg.Main_Wait(1440);
			}
			else if(v.P_LoopMain[playerID] <= 16)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Wraith",0+30*v.P_LoopMain[playerID],2,128,0);
				trg.Shape_Line(playerID,1,"60 + 1n High Templar",0+30*v.P_LoopMain[playerID],2,128,0);
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 17)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				KillUnitAt(All,"40 + 1n Marine","Anywhere",playerID);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",48,0);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",96,0);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",144,0);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",192,0);
	
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 18)
			{
				trg.Main_Wait(560);
			}
			else if(v.P_LoopMain[playerID] == 19)
			{
				trg.Shape_NxNSquare(playerID,1," Creep. Dunkelheit",3,96);
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Mojo",3,96);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All," Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 20)
			{
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 21)
			{
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Ghost",3,96);
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Guardian",3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Guardian",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 22)
			{
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 23)
			{
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Mojo",3,96);
				trg.Shape_Square(playerID,1,"40 + 1n Ghost",96,0);
				trg.Shape_Square(playerID,1,"40 + 1n Ghost",96,96);
				trg.Shape_Dot(playerID,1,"40 + 1n Ghost",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(320);
			}
			else if(v.P_LoopMain[playerID] == 24)
			{
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Wraith",3,96);
				trg.Shape_Square(playerID,1,"40 + 1n Ghost",96,0);
				trg.Shape_Square(playerID,1,"40 + 1n Ghost",96,96);
				trg.Shape_Dot(playerID,1,"40 + 1n Ghost",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
