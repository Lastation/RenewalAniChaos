import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

var x = 0;
function main(playerID)
{
	trg.Buff_ShieldFix(1);
	if (v.P_WaitMain[playerID] == 0)
	{
		
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Goliath","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if(v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Marine",45,4,96);
				trg.Shape_Edge(playerID,1,"Protoss Observer",45,4,96);
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Marine",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 1)
			{
				trg.Main_Wait(1800);
			}
			else if(v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(400);
			}
			else if(v.P_LoopMain[playerID] == 3)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(400);
			}
			else if(v.P_LoopMain[playerID] == 4)
			{
				trg.Main_Wait(1440);
			}
			else if(v.P_LoopMain[playerID] <= 8)
			{
				RemoveUnitAt(All,"40 + 1n Marine","Anywhere",playerID);
				trg.Shape_Square(playerID,1,"40 + 1n Goliath",48+48*x,0);
				if(v.P_LoopMain[playerID] % 2 == 0)
				{
					trg.Shape_Square(playerID,1,"40 + 1n Wraith",48+48*x,0);
				}
				else
				{
					epic.Shape_Square(playerID,1,"40 + 1n Wraith",48+48*x,0,1);
				}
				trg.Shape_Square(playerID,1,"Protoss Observer",48+48*x,0);
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				x += 1;
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 9)
			{
				x = 0;
				trg.Main_Wait(0);
			}
			else if(v.P_LoopMain[playerID] <= 13)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Goliath",48+48*x,48+48*x);
				trg.Shape_Square(playerID,1,"Protoss Observer",48+48*x,48+48*x);
				if(v.P_LoopMain[playerID] % 2 == 0)
				{
					trg.Shape_Square(playerID,1,"40 + 1n Wraith",48+48*x,48+48*x);
				}
				else
				{
					epic.Shape_Square(playerID,1,"40 + 1n Wraith",48+48*x,48+48*x,1);
				}
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				x += 1;
				Order("40 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 14)
			{
				x = 0;
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 15)
			{
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}