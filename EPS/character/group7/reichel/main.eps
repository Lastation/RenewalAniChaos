import PluginVariables as msqcvar;
import variable as v;

import character.group7.reichel.commend as commend;

import character.group7.reichel.common.skill0 as O;
import character.group7.reichel.common.skill1 as S;
import character.group7.reichel.common.skill2 as C;
import character.group7.reichel.common.skill3 as A;

import character.group7.reichel.combo.combo1 as SSS;
import character.group7.reichel.combo.combo2 as CCC;
import character.group7.reichel.combo.combo3 as CCCAS;
import character.group7.reichel.combo.combo4 as CCCASAC;
import character.group7.reichel.combo.combo5 as ACC;


import character.group7.reichel.ultimate.ultimate1 as AAA;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 레이첼 \x1B가드너　\x04[ 살육의 천사 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18 O\x04 치료 \x19[ 1초에 한번씩 자기 회복 + 자기 마나 30 회복/ 7초지속/ 지속중 스킬사용 불가  ] \x05개인쿨 4분");
		v.stb.printAt(4, "　　　\x1F SSS\x04 신 \x19[ 만능 ]");
		v.stb.printAt(5, "　　　\x1F CCS + AA + CS\x04 맹세 \x19[ 대인 / 지속딜 ]");
		v.stb.printAt(6, "　　　\x1F ACCC\x04 가족 \x19[ 대인 / 순간딜]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08 AAA \x04 거짓말\x19 [ 대인 / 쉴드고정 20 / 앞타 후 컴동해제  ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 40;
	v.P_LocationID[playerID] 		= 202;
	v.P_UnitID[playerID] 			= 34;
	
	v.P_Ultimate1[playerID] = 500;
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAS.main(playerID);
			break;
		case 230:
			CCCASAC.main(playerID);
			break;	
		case 300:
			A.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;;		
		case 320:
			AAA.main(playerID);
			break;		
		}
	}
	
}