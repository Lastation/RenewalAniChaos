import PluginVariables as msqcvar;
import variable as v;

import character.group7.ayano.commend as commend;

import character.group7.ayano.common.skill1 as S;
import character.group7.ayano.common.skill2 as C;
import character.group7.ayano.common.skill3 as A;

import character.group7.ayano.combo.combo1 as SSS;
import character.group7.ayano.combo.combo2 as SSSCCCC;
import character.group7.ayano.combo.combo3 as ACC;


import character.group7.ayano.ultimate.ultimate1 as AAA;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 하네사키 \x1B아야노　\x04[ 하네배드! ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18 O\x04 부모 \x19[ 자기마나 + 400 ] \x05개인쿨 2분");
		v.stb.printAt(4, "　　　\x1F SS+AACC\x04 승리 \x19[ 대인 / 지속딜 ]");
		v.stb.printAt(5, "　　　\x1F ACS\x04 가능성 \x19[ 대인 / 순간딜 ]");
		v.stb.printAt(6, "\n");
		v.stb.printAt(7, "　　　\x08 AAA \x04 필요\x19[ 대인 / 무적  ]  \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(8, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 39;
	v.P_LocationID[playerID] 		= 209;
	v.P_UnitID[playerID] 			= 77;
	
	v.P_Ultimate1[playerID] = 500;
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 120:
			SSSCCCC.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;;		
		case 320:
			AAA.main(playerID);
			break;		
		}
	}
	
}