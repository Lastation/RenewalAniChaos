import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.trigadv as adv;
import func.sound as s;
import func.trigepic as epic;

function main(playerID)
{
	trg.Buff_ShieldFix(20);
	if (v.P_WaitMain[playerID] == 0)
	{
		MoveUnit(All,"80 + 1n Ghost", playerID, "Anywhere", "[Skill]HoldPosition");
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All,"80 + 1n Ghost","Anywhere",playerID);
			RemoveUnitAt(All,"80 + 1n Vulture","Anywhere",playerID);
			RemoveUnitAt(All,"60 + 3n Ghost","Anywhere",playerID);
			KillUnitAt(All,"100 + 1n Hyperion","Anywhere",playerID);
			if (v.P_LoopMain[playerID] <= 16)
			{	
				trg.ComputerAlly(1);
				SetInvincibility(Enable, v.P_UnitID[playerID], playerID, "Anywhere");
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",90+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",90+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",90+30*v.P_LoopMain[playerID],2,384,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",75+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",75+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",75+30*v.P_LoopMain[playerID],2,384,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",105+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",105+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"80 + 1n Vulture",105+30*v.P_LoopMain[playerID],2,384,0);
				
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",90+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",90+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",90+30*v.P_LoopMain[playerID],2,384,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",75+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",75+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",75+30*v.P_LoopMain[playerID],2,384,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",105+30*v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",105+30*v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",105+30*v.P_LoopMain[playerID],2,384,0);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("80 + 1n Vulture", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if (v.P_LoopMain[playerID] == 17)
			{
				trg.Shape_NxNSquare(playerID,1,"60 + 3n Ghost",4,96);
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Ghost",10,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("60 + 3n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("80 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if (v.P_LoopMain[playerID] == 18)
			{
				trg.Main_Wait(1440);
			}
			else if (v.P_LoopMain[playerID] == 19)
			{
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Guardian",6,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("80 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 20)
			{
				RemoveUnitAt(All,"80 + 1n Guardian","Anywhere",playerID);
				trg.Shape_NxNSquare(playerID,1,"130 + 1n Norad",10,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("130 + 1n Norad", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] <= 30)
			{
				for(var i =0; i < 10; i++)
				{
					MoveLocation(v.P_LocationID[playerID],"130 + 1n Norad",playerID,"Anywhere");
					RemoveUnitAt(1,"130 + 1n Norad","Anywhere",playerID);
					trg.SkillUnit(playerID,5,"80 + 1n Vulture");
					trg.SkillUnit(playerID,1,"50 + 1n Battlecruiser");
					KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
					trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
					Order("80 + 1n Vulture", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				}
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 31)
			{
				trg.ComputerAlly(0);
				SetInvincibility(Disable, v.P_UnitID[playerID], playerID, "Anywhere");
				trg.Main_Wait(0);
			}
			else if(v.P_LoopMain[playerID] == 32)
			{
				SetSwitch("Recall - Chtholly",Clear);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}