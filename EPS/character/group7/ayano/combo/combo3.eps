import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

function main(playerID)
{
	trg.Buff_ShieldFix(1);
	if (v.P_WaitMain[playerID] == 0)
	{
		
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Ghost","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Goliath","Anywhere",playerID);
			RemoveUnitAt(All,"100 + 1n Hyperion","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if(v.P_LoopMain[playerID] <= 3)
			{
				trg.Shape_Line(playerID,1," Creep. Dunkelheit",30+30*v.P_LoopMain[playerID],2,256-64*v.P_LoopMain[playerID],0);
				trg.Shape_Line(playerID,1," Creep. Dunkelheit",120+30*v.P_LoopMain[playerID],2,256-64*v.P_LoopMain[playerID],0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",0+30*v.P_LoopMain[playerID],2,352,0);
				trg.Shape_Line(playerID,1,"40 + 1n Ghost",0+30*v.P_LoopMain[playerID],2,352,0);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All," Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] <= 7)
			{
				trg.Shape_Line(playerID,1," Creep. Dunkelheit",30+30*v.P_LoopMain[playerID],2,-192+64*v.P_LoopMain[playerID],0);
				trg.Shape_Line(playerID,1," Creep. Dunkelheit",120+30*v.P_LoopMain[playerID],2,-192+64*v.P_LoopMain[playerID],0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",0+30*v.P_LoopMain[playerID],2,352,0);
				trg.Shape_Line(playerID,1,"40 + 1n Ghost",0+30*v.P_LoopMain[playerID],2,352,0);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All," Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 8)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(320);
			}
			else if(v.P_LoopMain[playerID] == 9)
			{
				trg.Main_Wait(960);
			}
			else if(v.P_LoopMain[playerID] == 10)
			{
				trg.Shape_Edge(playerID,1," Creep. Dunkelheit",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All," Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 11)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Ghost",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 12)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",0,1);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 13)
			{
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}