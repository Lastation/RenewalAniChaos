import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{	
			RemoveUnitAt(All,"40 + 1n Guardian","Anywhere",playerID);
			RemoveUnitAt(All,"100 + 1n Hyperion","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Guardian",128,0);
				trg.Shape_Square(playerID,1,"Protoss Dark Templar",128,0);
				trg.Shape_Square(playerID,1,"60 + 1n High Templar",64,0);
				KillUnitAt(All,"Protoss Dark Templar","Anywhere",playerID);
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Guardian",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Guardian",128,128);
				trg.Shape_Square(playerID,1,"Protoss Dark Templar",128,128);
				trg.Shape_Square(playerID,1,"60 + 1n High Templar",64,64);
				KillUnitAt(All,"Protoss Dark Templar","Anywhere",playerID);
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Guardian",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Wraith",64,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(320);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				KillUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
				trg.Main_Wait(1120);
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(400);
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				KillUnitAt(3,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Main_Wait(50);
			}
			else if (v.P_LoopMain[playerID] == 6)
			{
				KillUnitAt(2,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Main_Wait(50);
			}
			else if (v.P_LoopMain[playerID] == 7)
			{
				KillUnitAt(3,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Main_Wait(50);
			}
			else if (v.P_LoopMain[playerID] == 8)
			{
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}