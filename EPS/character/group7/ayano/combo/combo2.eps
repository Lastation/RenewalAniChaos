import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

function main(playerID)
{
	trg.Buff_ShieldFix(1);
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			RemoveUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Guardian","Anywhere",playerID);
			KillUnitAt(All,"80 + 1n Guardian","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
			KillUnitAt(All,"80 + 1n Artanis","Anywhere",playerID);
			RemoveUnitAt(All,"100 + 1n Hyperion","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{	
				epic.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",0,4,192,1);
				trg.Shape_Edge(playerID,1,"60 + 1n Siege",45,3,192);
				trg.Shape_Edge(playerID,1,"Protoss Dark Archon",45,3,192);
				
				trg.Shape_Line(playerID,1,"40 + 1n Mojo",30+30*v.P_LoopMain[playerID],2,128,0);
				trg.Shape_Line(playerID,1,"40 + 1n Mojo",120+30*v.P_LoopMain[playerID],2,128,0);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] <= 68)
			{
				if(v.P_LoopMain[playerID] % 2 == 0)
				{
					trg.Shape_Line(playerID,1,"40 + 1n Mojo",30+30*v.P_LoopMain[playerID],2,224,0);
					epic.Shape_Line(playerID,1,"80 + 1n Artanis",120+30*v.P_LoopMain[playerID],2,224,0,1);
				}
				else if(v.P_LoopMain[playerID] % 3 == 0)
				{
					trg.Shape_Line(playerID,1,"40 + 1n Mojo",30+30*v.P_LoopMain[playerID],2,320,0);
					epic.Shape_Line(playerID,1,"40 + 1n Mojo",120+30*v.P_LoopMain[playerID],2,320,0,1);
				}
				else
				{
					trg.Shape_Line(playerID,1,"40 + 1n Mojo",30+30*v.P_LoopMain[playerID],2,128,0);
					epic.Shape_Line(playerID,1,"80 + 1n Artanis",120+30*v.P_LoopMain[playerID],2,128,0,1);
				}
				
				if(v.P_LoopMain[playerID] == 22)
				{
					for(var i = 0; i < 12; i++)
					{
						MoveLocation(v.P_LocationID[playerID],"50 + 1n Battlecruiser",playerID,"Anywhere");
						RemoveUnitAt(1,"50 + 1n Battlecruiser","Anywhere",playerID);
						trg.SkillUnit(playerID,1,"40 + 1n Guardian");
						trg.SkillUnit(playerID,1,"130 + 1n Norad");
						KillUnitAt(All,"130 + 1n Norad","Anywhere",playerID);
						trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
						Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
					}
				}
				else if(v.P_LoopMain[playerID] == 30)
				{
					trg.Shape_Edge(playerID,1,"40 + 1n Wraith",45,3,128);
					trg.Shape_Edge(playerID,1,"Zerg Overlord",45,3,128);
					trg.Shape_Edge(playerID,1,"40 + 1n Goliath",45,3,128);
					MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
					Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
					KillUnitAt(All,"Zerg Overlord","Anywhere",playerID);
				}
				else if(v.P_LoopMain[playerID] == 52)
				{
					trg.Shape_Edge(playerID,1,"100 + 1n Hyperion",45,2,64);
					trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,2,64);
					trg.Shape_Edge(playerID,1,"40 + 1n Marine",45,2,64);
					Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
					KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				}
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("100 + 1n Hyperion", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("80 + 1n Artanis", playerID, "Anywhere",Move, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 69)
			{
				KillUnitAt(All,"40 + 1n Marine","Anywhere",playerID);
				trg.Shape_Edge(playerID,1,"40 + 1n Guardian",45,2,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(480);
			}
			else if(v.P_LoopMain[playerID] == 70)
			{
				KillUnitAt(All,"40 + 1n Goliath","Anywhere",playerID);
				trg.Shape_Edge(playerID,1,"40 + 1n Guardian",45,3,128);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(240);
				
			}
			else if(v.P_LoopMain[playerID] == 71)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				trg.Shape_Edge(playerID,1,"40 + 1n Guardian",45,4,192);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 72)
			{
				trg.SkillEnd();
			}	
			v.P_LoopMain[playerID] += 1;
		}
		
	
	}
}