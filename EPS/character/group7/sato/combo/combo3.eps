import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

var x = 0;
function main(playerID)
{
	trg.Debuff_Stop();
	trg.Debuff_BanReturn();
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Mutalisk","Anywhere",playerID);
			RemoveUnitAt(All,"40 + 1n Guardian","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if(v.P_LoopMain[playerID] == 0)
			{
				trg.Main_Wait(0);
				//trg.Main_Wait(200);
			}
			else if(v.P_LoopMain[playerID] == 1)
			{
				//trg.Shape_NxNSquare(playerID,1," Unit. Hoffnung 25000",4,96);
				//KillUnitAt(All," Unit. Hoffnung 25000","Anywhere",playerID);
				trg.Main_Wait(0);
				//trg.Main_Wait(1600);
			}
			else if(v.P_LoopMain[playerID] <= 5)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Guardian",120 - x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",120 - x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",120 - x,2,128,0);
				trg.Shape_Dot(playerID,1,"60 + 1n Archon",0,0);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				x += 30;
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Guardian",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(120);
			}
			else if(v.P_LoopMain[playerID] == 6)
			{
				x = 0;
				trg.Main_Wait(150);
			}
			else if(v.P_LoopMain[playerID] <= 12)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Mutalisk",0 + x,4,82,0);
				trg.Shape_Line(playerID,1,"Protoss Dark Archon",0 + x,4,82,0);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				x += 30;
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mutalisk",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(60);
			}
			else if(v.P_LoopMain[playerID] == 13)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				x = 0;
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if(v.P_LoopMain[playerID] == 14)
			{
				trg.Main_Wait(550);
			}
			else if(v.P_LoopMain[playerID] == 15)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",6,64);
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Shape_Edge(playerID,3," Creep. Licht",45,3,192);
				trg.Shape_Edge(playerID,3," Creep. Licht",45,3,224);
				trg.Shape_Edge(playerID,3," Creep. Licht",45,3,160);
				trg.Shape_Edge(playerID,3," Creep. Licht",45,3,176);
				trg.Shape_Edge(playerID,3," Creep. Licht",45,3,208);
				
				trg.Shape_Edge(playerID,1," Unit. Hoffnung 25000",45,3,192);
				trg.Shape_Edge(playerID,1," Unit. Hoffnung 25000",45,3,224);
				trg.Shape_Edge(playerID,1," Unit. Hoffnung 25000",45,3,160);
				trg.Shape_Edge(playerID,1," Unit. Hoffnung 25000",45,3,176);
				trg.Shape_Edge(playerID,1," Unit. Hoffnung 25000",45,3,208);
				
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveUnit(All," Creep. Licht", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All," Unit. Hoffnung 25000", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order(" Unit. Hoffnung 25000",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order(" Creep. Licht",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(800);
			}
			else if(v.P_LoopMain[playerID] <= 25)
			{
				for(var i = 0; i < 4; i++)
				{
					MoveLocation(v.P_LocationID[playerID]," Unit. Hoffnung 25000",playerID,"Anywhere");
					RemoveUnitAt(1," Unit. Hoffnung 25000","Anywhere",playerID);
					if(v.P_LoopMain[playerID] % 2 == 0)
					{
						trg.SkillUnit(playerID,1,"40 + 1n Drone");
						trg.SkillUnit(playerID,1,"Torrasque");
						KillUnitAt(All,"Torrasque","Anywhere",playerID);
					}
					else if(v.P_LoopMain[playerID] % 2 == 1)
					{
						epic.SkillUnit(playerID,1,"40 + 1n Drone",1);
						trg.SkillUnit(playerID,1,"Torrasque");
						KillUnitAt(All,"Torrasque","Anywhere",playerID);
					}
					trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				}
				KillUnitAt(12," Creep. Licht","Anywhere",playerID);
				Order("40 + 1n Drone",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] <= 35)
			{
				KillUnitAt(4,"40 + 1n Drone","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 36)
			{
				x = 0;
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}