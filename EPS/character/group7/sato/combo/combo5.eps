import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

var x = 0;
function main(playerID)
{
	trg.Debuff_Stop();
	trg.Debuff_BanReturn();
	if (v.P_WaitMain[playerID] == 0)
	{
		MoveUnit(All,"50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
		MoveUnit(All,"40 + 1n Drone", playerID, "Anywhere", "[Skill]HoldPosition");
		MoveUnit(All,"60 + 1n Hydralisk", playerID, "Anywhere", "[Skill]HoldPosition");
		if (v.P_CountMain[playerID] == 0)
		{
			
			if(v.P_LoopMain[playerID] == 0)
			{
				trg.Main_Wait(800);
			}
			else if(v.P_LoopMain[playerID] == 1)
			{
				s.CharacterVoice(14);
				trg.Main_Wait(0);
			}
			else if(v.P_LoopMain[playerID] <= 11)
			{
			//	if(v.P_LoopMain[playerID] % 2 == 0)
			//	{
			//		trg.Shape_Line(playerID,1,"60 + 1n Hydralisk",0+30*v.P_LoopMain[playerID],2,128+x,0);
			//		trg.Shape_Line(playerID,1,"60 + 1n Hydralisk",90+30*v.P_LoopMain[playerID],2,128+x,0);
			//	}
				trg.Shape_Line(playerID,1,"40 + 1n Drone",0+30*v.P_LoopMain[playerID],2,128+x,0);
				trg.Shape_Line(playerID,1,"40 + 1n Drone",90+30*v.P_LoopMain[playerID],2,128 +x,0);
				trg.Shape_Line(playerID,1,"Protoss Observer",0+30*v.P_LoopMain[playerID],2,128+x,0);
				trg.Shape_Line(playerID,1,"Protoss Observer",90+30*v.P_LoopMain[playerID],2,128+x,0);
				x += 64;
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				trg.Main_Wait(150);
			}
			else if(v.P_LoopMain[playerID] <= 16)
			{
				trg.Main_Wait(150);
			}
			else if(v.P_LoopMain[playerID] <= 26)
			{
				trg.Shape_Line(playerID,1,"60 + 1n Siege",0+v.P_LoopMain[playerID] *15,2,640,0);
				trg.Shape_Line(playerID,1,"60 + 1n Siege",90+v.P_LoopMain[playerID] *15,2,640,0);
				
				trg.Shape_Line(playerID,1,"Protoss Observer",0+v.P_LoopMain[playerID] *15,2,640,0);
				trg.Shape_Line(playerID,1,"Protoss Observer",90+v.P_LoopMain[playerID] *15,2,640,0);
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				trg.Main_Wait(100);
			}
			else if(v.P_LoopMain[playerID] == 27)
			{
				x = 0;
				trg.Main_Wait(1700);
			}
			else if(v.P_LoopMain[playerID] == 28)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Gantrithor",128,128);
				trg.Shape_Square(playerID,1,"60 + 1n Archon",128,128);
				ModifyUnitHangarCount(3,All,"40 + 1n Gantrithor",playerID,"Anywhere");
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Gantrithor",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(1800);
			}
			else if(v.P_LoopMain[playerID] <= 39)
			{
				for(var i =0; i < 4; i++)
				{
					MoveLocation(v.P_LocationID[playerID],"40 + 1n Drone",playerID,"Anywhere");
					RemoveUnitAt(1,"40 + 1n Drone","Anywhere",playerID);
					epic.SkillUnit(playerID,1,"40 + 1n Lurker",0);
					trg.SkillUnit(playerID,1,"60 + 3n Siege");
					KillUnitAt(All,"60 + 3n Siege","Anywhere",playerID);
				}
				trg.Main_Wait(100);
			}
			else if(v.P_LoopMain[playerID] <= 49)
			{
				for(var i =0; i < 4; i++)
				{
					MoveLocation(v.P_LocationID[playerID],"60 + 1n Siege",playerID,"Anywhere");
					RemoveUnitAt(1,"60 + 1n Siege","Anywhere",playerID);
					trg.SkillUnit(playerID,1,"50 + 1n Tank");
					trg.SkillUnit(playerID,1,"60 + 3n Siege");
					KillUnitAt(All,"60 + 3n Siege","Anywhere",playerID);
				}
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] <= 59)
			{
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				for(var i =0; i < 4; i++)
				{
					MoveLocation(v.P_LocationID[playerID],"50 + 1n Tank",playerID,"Anywhere");
					RemoveUnitAt(1,"50 + 1n Tank","Anywhere",playerID);
					trg.SkillUnit(playerID,1,"40 + 1n Mutalisk");
					trg.SkillUnit(playerID,1,"80 + 1n Mutalisk");
					KillUnitAt(All,"80 + 1n Mutalisk","Anywhere",playerID);
				}
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mutalisk",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 60)
			{
				trg.Main_Wait(900);
			}
			else if(v.P_LoopMain[playerID] <= 70)
			{
				for(var i =0; i < 6; i++)
				{
					MoveLocation(v.P_LocationID[playerID],"40 + 1n Lurker",playerID,"Anywhere");
					RemoveUnitAt(1,"40 + 1n Lurker","Anywhere",playerID);
					trg.SkillUnit(playerID,1,"Protoss Observer");
					KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				}
				KillUnitAt(4,"40 + 1n Mutalisk","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if (v.P_LoopMain[playerID] == 71 
			&& Bring(playerID, AtLeast, 1,"Protoss Arbiter", "[Skill]UseSkill")
			&& Bring(playerID, AtLeast, 1,"Protoss Carrier", "[Skill]UseSkill"))
			{
				s.CharacterVoice(17);
				KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);	
				KillUnitAt(1, "Protoss Carrier", "[Skill]UseSkill", playerID);	
				x = 0;
				v.P_Step[playerID] = 330;
				v.P_CountMain[playerID] = 0;
				v.P_LoopMain[playerID] = 0;
				
			}
			else if(v.P_LoopMain[playerID] == 71)
			{
				x = 0;
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}