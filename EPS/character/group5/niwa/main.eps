import character.group5.niwa.common.skill4 as A;

import character.group5.niwa.combo.combo1 as SSS;
import character.group5.niwa.combo.combo2 as CCA;
import character.group5.niwa.combo.combo3 as CCAAA;
import character.group5.niwa.combo.combo4 as CCCC;

import character.group5.niwa.ultimate.ultimate1 as AAA;
import character.group5.niwa.ultimate.ultimate2 as AAAAA;

