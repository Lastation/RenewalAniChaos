function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 니와 료카　\x04[ 십이대전 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04유[酉]의 전사 \x19[ 20초 간 공격력 10 증가 ] \x052분");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04우노메타카노메 \x19[ 공성 ]");
		v.stb.printAt(5, "　　　\x04\x1FCCA + AA \x04이중 스파이 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(6, "　　　\x04\x1FCCCS \x04십이대전 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAA + AA \x04조장 \x19[ 공성 / AAA - 플레이어 동맹 /  AA - 자리고정 / 쉴드고정1 ] \x05", v.P_Ultimate1[playerID], " + ", v.P_Ultimate2[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}
function main(playerID)
{
	v.P_HeroID[playerID] = 30;
	v.P_LocationID[playerID] = 197;
	v.P_UnitID[playerID] = 53;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 200;
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCCC.main(playerID);
			break;
		case 220:
			CCA.main(playerID);
			break;
		case 230:
			CCAAA.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
		case 320:
			AAAAA.main(playerID);
			break;
			
		}
	}
}
