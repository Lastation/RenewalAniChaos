import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

var x = 0;
function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
			KillUnitAt(All,"60 + 1n Danimoth","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] <= 3)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Wraith",32 + x *32 ,0);
				trg.Shape_Square(playerID,1,"Protoss Dark Templar",32 + x *32 ,0);
				KillUnitAt(All,"Protoss Dark Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				x += 1;
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Square(playerID,1,"50 + 1n Tank",160,0);
				trg.Shape_Square(playerID,1,"Protoss Dark Archon",160,0);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(80);
				x = 0;
			}
			else if (v.P_LoopMain[playerID] <= 7)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Wraith",32 + x *32 ,32 + x*32);
				trg.Shape_Square(playerID,1,"Protoss Dark Templar",32 + x *32 ,32 + x*32);
				KillUnitAt(All,"Protoss Dark Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Move, v.P_LocationID[playerID]);
				x += 1;
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 8)
			{
				trg.Shape_Square(playerID,1,"50 + 1n Tank",160,160);
				trg.Shape_Square(playerID,1,"Protoss Dark Archon",160,160);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(800);
				x = 0;
			}
			else if (v.P_LoopMain[playerID] == 9)
			{
				trg.Shape_Edge(playerID,1,"60 + 1n Danimoth",45,3,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 10)
			{
				trg.Main_Wait(400);
			}
			else if (v.P_LoopMain[playerID] == 11)
			{
				trg.Shape_Dot(playerID,1,"60 + 1n Danimoth",0,0);
				Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 12)
			{
				KillUnitAt(All,"50 + 1n Tank","Anywhere",playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}