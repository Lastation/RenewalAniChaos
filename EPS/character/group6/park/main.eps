var loop  = 0;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      v.stb.printAt(0, "\n");
	      v.stb.printAt(1, "\x1F　＃\x1B- 박 일표　\x04[ 갓 오브 하이스쿨 ]");
	      v.stb.printAt(2, "　　\x1FA\x04ction List");
	      v.stb.printAt(3, "　　　\x18O \x04택견 - 무음신속 \x19[ 10초 간 첫 스킬에 한해 스킬 사용시 적에게 돌진 ] \x051분");
	      v.stb.printAt(4, "　　　\x04\x1FSSS\x04 소용 \x19[ 대인 / 순간딜 ]");
	      v.stb.printAt(5, "　　　\x04\x1FCCC + AC + AC\x04 약점 \x19[ 대인 / 순간딜 ]");
	      v.stb.printAt(6, "　　　\x04\x1FAAC \x04도발 \x19[ 대인 / 순간딜 ]");
	      v.stb.printAt(7, "　　　\x08AAA \x04택견 오의 - 화축 \x19[ 대인 / 쉴드고정 20 ] \x05", v.P_Ultimate1[playerID]);
	      v.stb.printAt(8, "　　　\x08 + AA \x04 최종 오의 - 삼염멸도각 \x19[ 공성 / 쉴드고정 1 / 플레이어동맹] \x05", v.P_Ultimate2[playerID]);
	      v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 31;
	v.P_LocationID[playerID] = 199;
	v.P_UnitID[playerID] = 10;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 250;

	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCA.main(playerID);
			break;
		case 220:
			CCAAC.main(playerID);
			break;
		case 230:
			CCAACCC.main(playerID);
			break;
		case 310:
			AAC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;
		}
	}
	

}
