import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 12)
         {
            trg.Shape_Square(playerID, 1, "Bengalaas (Jungle)", 32, 32);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 12 )
         {
            trg.Shape_Square(playerID, 1, "Bengalaas (Jungle)", 64, 64);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 64, 0);
            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }

       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }

      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 8 )
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 100);
            trg.Shape_NxNSquare(playerID, 1, "Protoss Dark Archon", 3, 50);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }

      else if (v.P_CountMain[playerID] == 3)
      {
         SetDeaths(playerID, SetTo, 720, " `UniqueCoolTime");
         SetDeaths(playerID, SetTo, 120, " `UniqueSkill");
         trg.SkillEnd();
      }
   }
}