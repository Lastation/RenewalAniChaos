import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{	
			KillUnitAt(All,"60 + 1n Danimoth","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID,1,"60 + 1n Siege",64,64);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",64,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Square(playerID,1,"60 + 1n Siege",128,128);
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",128,128);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Main_Wait(480);
			}
			else if(v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",96,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",-96,-96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 5)
			{
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",-96,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 6)
			{
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",96,-96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 7)
			{
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 8)
			{
				trg.Shape_Double(playerID,1,"60 + 1n Danimoth",96,0);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Danimoth",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 9)
			{
				trg.Shape_Double(playerID,1,"60 + 1n Danimoth",0,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Danimoth",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 10)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Battlecruiser",45,3,96);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if (v.P_LoopMain[playerID] == 11)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}