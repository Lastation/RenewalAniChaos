import PluginVariables as msqcvar;
import variable as v;

import character.group8.gozo.commend as commend;

import character.group8.gozo.common.skill0 as O;
import character.group8.gozo.common.skill1 as S;
import character.group8.gozo.common.skill2 as C;
import character.group8.gozo.common.skill3 as A;

import character.group8.gozo.combo.combo1 as SSS;
import character.group8.gozo.combo.combo2 as SSSAAAA;
import character.group8.gozo.combo.combo3 as CCC;
import character.group8.gozo.combo.combo4 as CCCAC;

import character.group8.gozo.ultimate.ultimate1 as AAA;


function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 고죠 \x1B사토루　\x04[ 주술회전 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18 O\x04 무한 \x19[ 무적 6초 / 자기 마나 200회복 ] \x05개인쿨 2분");
		v.stb.printAt(4, "　　　\x1F SSS + CC(+1000Gas 소모)\x04 술식 \x19[ 대인 / 부분 자리고정 ]");
		v.stb.printAt(5, "　　　\x1F CCC+AA\x04 죽음 \x19[ 대인 ]");
		v.stb.printAt(6, "\n");
		v.stb.printAt(7, "　　　\x08 AAA \x04 영역\x19[ 대인 / 무적  / 상대 컴퓨터 동맹 /  지속 28초 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 41;
	v.P_UnitID[playerID] 			= 99;
	v.P_LocationID[playerID] 		= 210;
	
	v.P_Ultimate1[playerID] = 500;
	
	commend.main(playerID);
	skillList(playerID);
	
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 120:
			SSSAAAA.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAC.main(playerID);
			break;	
		case 310:
			AAA.main(playerID);
			break;
		
		}
	}
}