import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import func.trigepic as epic;

function main(playerID)
{	
	trg.Buff_ShieldFix(20);
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			RemoveUnitAt(All,"80 + 1n Goliath","Anywhere",playerID);
			RemoveUnitAt(All,"80 + 1n Ghost","Anywhere",playerID);
			RemoveUnitAt(All,"60 + 3n Ghost","Anywhere",playerID);
			RemoveUnitAt(All,"80 + 1n Tom Kazansky","Anywhere",playerID);
			KillUnitAt(All,"130 + 1n Norad","Anywhere",playerID);
			RemoveUnitAt(All,"80 + 1n Guardian","Anywhere",playerID);
			RemoveUnitAt(All,"80 + 1n Vulture","Anywhere",playerID);
			if(v.P_LoopMain[playerID] == 0)
			{
				trg.ComputerAlly(1);
				SetInvincibility(Enable, v.P_UnitID[playerID], playerID, "Anywhere");
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_NxNSquare(playerID,1,"130 + 1n Norad",3,128);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				Order("130 + 1n Norad",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 2)
			{
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_NxNSquare(playerID,1,"130 + 1n Norad",4,128);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				Order("130 + 1n Norad",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(240);
			}
			else if(v.P_LoopMain[playerID] == 4)
			{
				trg.Main_Wait(400);
			}
			else if(v.P_LoopMain[playerID] == 5)
			{
				trg.Shape_NxNSquare(playerID,1,"100 + 1n Hyperion",9,64);
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Ghost",9,64);
				trg.Shape_Edge(playerID,1,"60 + 3n Ghost",45,8,256);
				trg.Shape_NxNSquare(playerID,1,"Protoss Observer",9,64);
				KillUnitAt(All,"Protoss Observer","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveUnit(All,"80 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"60 + 3n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("100 + 1n Hyperion",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("80 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("60 + 3n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 6)
			{
				trg.Main_Wait(1000);
			}
			else if(v.P_LoopMain[playerID] == 7)
			{
				MoveLocation(v.P_LocationID[playerID],"100 + 1n Hyperion",playerID,"Anywhere");
				KillUnitAt(All,"100 + 1n Hyperion","Anywhere",playerID);
				trg.SkillUnit(playerID,1,"40 + 1n Gantrithor");
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				Order("80 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				
				trg.Main_Wait(880);
			}
			else if(v.P_LoopMain[playerID] == 8)
			{
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Ghost",9,64);
				trg.Shape_Edge(playerID,1,"60 + 3n Ghost",45,8,256);
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Guardian",9,64);
				KillUnitAt(All,"40 + 1n Guardian","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveUnit(All,"80 + 1n Ghost", playerID, "Anywhere", "[Skill]HoldPosition");
				MoveUnit(All,"60 + 3n Ghost", playerID, "Anywhere", "[Skill]HoldPosition");
				MoveUnit(All,"80 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"60 + 3n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("80 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("60 + 3n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(900);
			}
			 
			else if (v.P_LoopMain[playerID] == 9)
			{ 
			 SetInvincibility(Disable, v.P_UnitID[playerID], playerID, "Anywhere");
			trg.Shape_NxNSquare(playerID,1,"40 + 1n Zergling",9,64);
			KillUnitAt(All,"40 + 1n Zergling","Anywhere",playerID);
			KillUnitAt(All,"80 + 1n Ghost","Anywhere",playerID);
			KillUnitAt(All,"60 + 3n Ghost","Anywhere",playerID);
			trg.ComputerAlly(0);
			       trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}