import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

var x = 0;
var y = 0;

function main(playerID)
{
       if (v.P_WaitMain[playerID] == 0)
       {
              if (v.P_CountMain[playerID] == 0)
              {
                     if (v.P_LoopMain[playerID] == 0) 
                     {
                            trg.Shape_Cross(playerID, 1, "40 + 1n Mutalisk", 0, 4, 50);
                            trg.Shape_Cross(playerID, 1, "Protoss Dark Archon", 0, 4, 50);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

                            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 4)
                     {
                            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 5)
                     {
                            trg.Shape_Cross(playerID, 1, "40 + 1n Guardian", 0, 4, 50);
                            trg.Shape_Cross(playerID, 1, " Unit. Hoffnung 25000", 0, 4, 50);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

                            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
                     } 
                     else if (v.P_LoopMain[playerID] == 8) 
                     {
                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                     }

                     trg.Main_Wait(80);

                     v.P_LoopMain[playerID] += 1;

                     if (v.P_LoopMain[playerID] == 21)
                     {                        
                            v.P_CountMain[playerID] += 1;
                            v.P_LoopMain[playerID] = 0;
                     }
              }
              else if (v.P_CountMain[playerID] == 1)
              {
                     if (v.P_LoopMain[playerID] < 4) 
                     {
                            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

                            trg.Shape_Square(playerID, 1, "Scantid (Desert)", 40 - 40 * v.P_LoopMain[playerID], 80);
                            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 40 - 40 * v.P_LoopMain[playerID], 80);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

                            KillUnitAt(All, "Scantid (Desert)", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] < 8)
                     {
                            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

                            var i = v.P_LoopMain[playerID] - 4;

                            trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 50 + 50 * i, 50 + 50 * i);
                            trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 50 + 50 * i, 50 + 50 * i);

                            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
                            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 10)
                     {
                            trg.Shape_Cross(playerID, 1, "Kakaru (Twilight)", 0, 4, 50);

                            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
                     } 
                     else if (v.P_LoopMain[playerID] == 14) 
                     {
                            trg.Shape_Edge(playerID, 1, "Protoss Dark Archon", 45, 3, 50);
                            trg.Shape_Edge(playerID, 1, "40 + 1n Mutalisk", 45, 3, 50);

                            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
                            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 15) 
                     {
                            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 3, 75);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
                     }
                     else if (v.P_LoopMain[playerID] == 18) 
                     {
                            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
                     }

                     trg.Main_Wait(80);

                     v.P_LoopMain[playerID] += 1;

                     if (v.P_LoopMain[playerID] == 19)
                     {                        
                            v.P_CountMain[playerID] += 1;
                            v.P_LoopMain[playerID] = 0;
                     }
              }
              else if (v.P_CountMain[playerID] == 2)
              {
                     trg.Shape_Edge(playerID, 1, "Kakaru (Twilight)", 45, 3, 75);
                     KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

                     trg.SkillEnd();
              }
       }
}