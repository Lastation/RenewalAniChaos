import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.trigadv as adv;
import func.trigepic as epic;
import func.sound as s;
import py_math;

function main(playerID)
{
     
          trg.Buff_ShieldFix(1);
          
          
       if (v.P_WaitMain[playerID] == 0)
       {
              if (v.P_CountMain[playerID] == 0)
              {
                     if (v.P_LoopMain[playerID] == 0) 
                     {
                            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 45, 3, 64);
                            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 45, 3, 64);

                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 1) 
                     {
                            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 45, 3, 128);
                            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 45, 3, 128);

                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 2)
                     {
                            trg.Shape_Edge(playerID, 1, "Kakaru (Twilight)", 45, 3, 64);
                            trg.Shape_Edge(playerID, 1, "Kakaru (Twilight)", 45, 3, 128);

                            trg.Shape_Edge(playerID, 1, "40 + 1n Marine", 45, 3, 64);
                            trg.Shape_Edge(playerID, 1, "40 + 1n Drone", 45, 3, 128);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            MoveUnit(All, "40 + 1n Drone", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
                            Order("40 + 1n Drone", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  

                            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
                     }

                     else if (v.P_LoopMain[playerID] == 15)
                     {
                            s.CharacterVoice(4);

                            trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, 0);

                            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
                     }

                     trg.Main_Wait(160);

                     v.P_LoopMain[playerID] += 1;

                     if (v.P_LoopMain[playerID] == 20)
                     {                        
                            v.P_CountMain[playerID] += 1;
                            v.P_LoopMain[playerID] = 0;
                     }
              }
              else if (v.P_CountMain[playerID] == 1)
              {
                     if (v.P_LoopMain[playerID] == 0) 
                     {
                            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 5, 64);

                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 1)
                     {
                            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 7, 64);

                            trg.Shape_Edge(playerID, 1, " Creep. Licht", 45, 5, 192);
                            epic.Shape_Edge(playerID, 1, " Unit. Hoffnung 25000", 45, 3, 128, 1);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            MoveUnit(All, " Creep. Licht", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            MoveUnit(All, " Unit. Hoffnung 25000", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            Order(" Creep. Licht", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
                            Order(" Unit. Hoffnung 25000", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  

                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 2)
                     {
                            trg.Shape_Edge(playerID, 1, "40 + 1n Mutalisk", 45, 3, 64);
                            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 45, 3, 128);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
                            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
                     }

                     trg.Main_Wait(160);

                     v.P_LoopMain[playerID] += 1;

                     if (v.P_LoopMain[playerID] == 15)
                     {                        
                            v.P_CountMain[playerID] += 1;
                            v.P_LoopMain[playerID] = 0;
                     }
              }  
              else if (v.P_CountMain[playerID] == 2)
              {
                     if (v.P_LoopMain[playerID] < 8)
                     {
                            KillUnitAt(1, "40 + 1n Mutalisk", "Anywhere", playerID);
                            KillUnitAt(1, "40 + 1n Guardian", "Anywhere", playerID);
                            KillUnitAt(1, "40 + 1n Drone", "Anywhere", playerID);
                            KillUnitAt(1, "40 + 1n Marine", "Anywhere", playerID);
                            KillUnitAt(4, " Creep. Licht", "Anywhere", playerID);
                            KillUnitAt(2, " Unit. Hoffnung 25000", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 8)
                     {
                            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
                            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
                            KillUnitAt(All, "40 + 1n Drone", "Anywhere", playerID);
                            KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
                            KillUnitAt(All, " Creep. Licht", "Anywhere", playerID);
                            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 9)
                     {
                            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);

                            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
                     }
                     else if (v.P_LoopMain[playerID] == 11)
                     {
                            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 5, 75);

                            trg.Shape_Edge(playerID, 1, "40 + 1n Goliath", 45, 3, 64);
                            trg.Shape_Edge(playerID, 1, "50 + 1n Tank", 45, 5, 128);

                            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
                            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  
                            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);  

                            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
                     }


                     if (v.P_LoopMain[playerID] >= 9 && v.P_LoopMain[playerID] < 25)
                     {
                            var i = (v.P_LoopMain[playerID] - 9) % 8;

                            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 125 - 25 * i);

                            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
                     }

                     if (v.P_LoopMain[playerID] >= 12)
                     {
                            var tx = 0;
                            var ty = 0;

                            if (v.P_LoopMain[playerID] % 4 == 0) { tx = 75; ty = 75; }
                            else if (v.P_LoopMain[playerID] % 4 == 1) { tx = 75; ty = -75; }
                            else if (v.P_LoopMain[playerID] % 4 == 2) { tx = -75; ty = -75; }
                            else if (v.P_LoopMain[playerID] % 4 == 3) { tx = -75; ty = 75; }

                            adv.Shape_NxNSquareAt2(playerID, 1, "Terran Science Vessel", 3, 75, tx, ty);

                            KillUnitAt(All, "Terran Science Vessel", "Anywhere", playerID);

                            if (v.P_LoopMain[playerID] % 2 == 0) 
                            {
                                   RemoveUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

                                   trg.Shape_NxNSquare(playerID, 1, " Creep. Dunkelheit", 2, 50);

                                   MoveUnit(All, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
                                   Order(" Creep. Dunkelheit", playerID, "Anywhere", Attack, "Anywhere");  
                            }
                     }

                     trg.Main_Wait(80);

                     v.P_LoopMain[playerID] += 1;

                     if (v.P_LoopMain[playerID] == 25)
                     {                        
                            v.P_CountMain[playerID] += 1;
                            v.P_LoopMain[playerID] = 0;
                     }
              }      

              else if (v.P_CountMain[playerID] == 3)
              {
                     KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
                     KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
                     KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
                     RemoveUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

                     trg.SkillEnd();
              }
       }
}