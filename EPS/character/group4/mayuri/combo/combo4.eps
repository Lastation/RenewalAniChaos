import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   trg.Buff_ShieldFix(1);

   MoveUnit(All, "40 + 1n Marine", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "40 + 1n Ghost", playerID, "Anywhere", "[Skill]HoldPosition");

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {       
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 50, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 100, 0);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {       
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 50, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 100, 100);

            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {       
            trg.Shape_Edge(playerID, 1, "40 + 1n Guardian", 0, 5, 100);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {       
            trg.Shape_Edge(playerID, 1, "Protoss Dark Archon", 0, 7, 150);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 200, 125);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 125, 200);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 200, 275);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 275, 200);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {       
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 200, 125);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 125, 200);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 200, 275);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 275, 200);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 200, 125);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 125, 200);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 200, 275);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 275, 200);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 150, 0);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 150, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);

            trg.Main_Wait(960);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 16)
         {        
            KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] % 3 == 0)
            {
               trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 0, 7, 150);
               KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

               trg.Shape_Edge(playerID, 1, "Target", 0, 5, 100);
               KillUnitAt(All, "Target", "Anywhere", playerID);
            }

            MoveLocation(v.P_LocationID[playerID], "40 + 1n Marine", playerID, "Anywhere");
            RemoveUnitAt(1, "40 + 1n Marine", "Anywhere", playerID);

            CreateUnit(2, " Creep. Dunkelheit", "[Skill]Unit_Wait_1", playerID);
            CreateUnit(6, "80 + 1n Vulture", "[Skill]Unit_Wait_1", playerID);
            SetInvincibility(Enable, "80 + 1n Vulture", playerID, "[Skill]Unit_Wait_ALL");
            SetInvincibility(Enable, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL");
            MoveUnit(2, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            MoveUnit(6, "80 + 1n Vulture", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            KillUnitAt(All, "80 + 1n Vulture", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, " Creep. Dunkelheit", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order(" Creep. Dunkelheit", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 16)
         {
            KillUnitAt(All, " Creep. Dunkelheit", "Anywhere", playerID);
            KillUnitAt(All, "80 + 1n Vulture", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         
         KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
         trg.SkillEnd();
      }
   }
}