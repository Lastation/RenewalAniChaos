         }

         else if (v.P_LoopMain[playerID] == 7)
         {
            adv.Shape_NxNSquareAt2(playerID, 1, "40 + 1n Wraith", 2, 75, 150, 150);
            adv.Shape_NxNSquareAt2(playerID, 1, "40 + 1n Wraith", 2, 75, -150, -150);
