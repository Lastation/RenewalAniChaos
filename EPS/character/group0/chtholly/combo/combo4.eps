import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

function main(playerID)
{
	trg.Debuff_BanReturn();
	if (v.P_CountMain[playerID] < 7)
	{
		trg.Debuff_Stop();
	}
	
	if (v.P_CountMain[playerID] >= 6 &&v.P_CountMain[playerID] <= 7)
	{
		trg.Buff_ShieldFix(1);
	}

	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] <= 23)
		{
			trg.Table_Cos(playerID, 15 * v.P_LoopMain[playerID], 160);
			trg.Table_Sin(playerID, 15 * v.P_LoopMain[playerID], 160);
			trg.Shape_Square(playerID, 1, "40 + 1n Gantrithor", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			trg.Shape_Square(playerID, 8, "Protoss Dark Templar", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
			KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 24)
		{
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			trg.Shape_Square(playerID, 1, "60 + 1n Siege", 192, 192);
			trg.Shape_Square(playerID, 1, "60 + 1n Siege", 128, 192);
			trg.Shape_Square(playerID, 1, "60 + 1n Siege", 192, 128);
			trg.Shape_Square(playerID, 1, "60 + 1n Siege", 128, 128);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 192, 192);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 192);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 192, 128);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 128);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 192, 64);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 64, 192);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 64);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 64, 128);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 128, 32);
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 32, 128);
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 128, 64);
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 64, 128);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 96, 96);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 160, 96);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 96, 160);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 160, 160);
			trg.Shape_Square(playerID, 1, "40 + 1n Firebat", 64, 64);
			trg.Shape_Square(playerID, 1, "40 + 1n Firebat", 64, 0);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			Order("40 + 1n Firebat", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(0);
			s.CharacterVoice(6);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] <= 24)
		{
			trg.Table_Cos(playerID, 15 * v.P_LoopMain[playerID], 224);
			trg.Table_Sin(playerID, 15 * v.P_LoopMain[playerID], 224);
			trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] == 25)
		{
			trg.Main_Wait(500);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			trg.Main_Wait(2000);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 5)
		{
			s.CharacterVoice(7);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			trg.Main_Wait(5000);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 6)
		{
			s.CharacterVoice(8);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.Main_Wait(2500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 7)
		{
			s.CharacterVoice(8);
			KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Firebat", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 8 
		&& Bring(playerID, AtLeast, 1, "Protoss Corsair", "[Skill]UseSkill")
		&& v.P_UltimateGauge[playerID] >= v.P_Ultimate1[playerID])
		{
			if (Switch("UiltimateSwitch", Cleared))
			{
               		CreateUnit(1, " Item. Flag", "[Uiltimate]Flag", CurrentPlayer);
				s.CharacterVoice(11);
				SetSwitch("UiltimateSwitch", Set);
				v.P_Step[playerID] = 240;
				v.P_CountMain[playerID] = 0;
				v.P_LoopMain[playerID] = 0;
				SetDeaths(playerID, Subtract, v.P_Ultimate1[playerID], 205);
				KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
				SetSwitch("Recall - Chtholly", Set);
			}
			else
			{
				SetResources(CurrentPlayer, Add, 60, Gas);
				KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
				SetDeaths(CurrentPlayer, SetTo, 999, " `SYSTEMTEXT");
				trg.SkillEnd();
			}
		}
		else if (v.P_CountMain[playerID] == 8)
		{
			trg.SkillEnd();
		}
	}
}