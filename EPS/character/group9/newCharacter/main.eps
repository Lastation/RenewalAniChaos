import PluginVariables as msqcvar;
import variable as v;

import character.group8.alice.commend as commend;

import character.group8.alice.common.skill2 as S;
import character.group8.alice.common.skill3 as C;
import character.group8.alice.common.skill4 as A;

import character.group8.alice.combo.combo1 as SSS;
import character.group8.alice.combo.combo2 as CCAA;
import character.group8.alice.combo.combo3 as CCAAAA;

import character.group8.alice.ultimate.ultimate1 as AAAA;
function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 앨리스리제 루 네뷸리스 9세　\x04[ 너와 나의 최후의 전장, 혹은 세계가 시작되는 성전 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04얼음벽 \x19[ 자신무적 3초, 자신마나 200회복 / 쿨 4분 ]");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04빙화 \x19[ 공성 ]");
		v.stb.printAt(5, "　　　\x04\x1FCCAA(+400Gas 소모) + AA(+800Gas 소모) \x04 꿰뚫어라 빙화 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(6, "\n");
		v.stb.printAt(7, "　　　\x08AAAA \x04대빙화 \x19[ 공성 / 쉴드 1고정 / 자리고정]\x1F \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(8, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 45;
	v.P_UnitID[playerID] 			= 34;
	v.P_LocationID[playerID] 		= 214;
	
	v.P_Ultimate1[playerID] = 700;
	
	
	commend.main(playerID);
	skillList(playerID);
	
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCAA.main(playerID);
			break;
		case 220:
			CCAAAA.main(playerID);
			break;
		case 310:
			AAAA.main(playerID);
			break;
		}
	}
	
}