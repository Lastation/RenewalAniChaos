	else if 	(v.P_HeroID[playerID] == 14) 	yashiro.main(playerID);

	else if 	(v.P_HeroID[playerID] == 17) 	kiana.main(playerID);
	
	else if 	(v.P_HeroID[playerID] == 22) 	yuuna.main(playerID);
	else if 	(v.P_HeroID[playerID] == 23) 	mayuri.main(playerID);
	else if 	(v.P_HeroID[playerID] == 24) 	nanami.main(playerID);
