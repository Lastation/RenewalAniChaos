	else if 	(v.P_HeroID[playerID] == 43) 	tanya.main(playerID);
	else if 	(v.P_HeroID[playerID] == 44) 	megumi.main(playerID);
	else if 	(v.P_HeroID[playerID] == 45) 	alice.main(playerID);
	
	SetDeaths(playerID, SetTo, v.P_Shield[playerID], " `SystemShield");
	
	debuff.main(playerID);
