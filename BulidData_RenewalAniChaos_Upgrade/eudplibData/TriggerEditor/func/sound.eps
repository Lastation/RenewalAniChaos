		count = dwread_epd(EPD(0x58A364 + 48 * 186 + 4 * 7));
		
		foreach(idx : EUDLoopRange(6))
		{
			tenshi.P_type[idx] = 1;  		tenshi.P_player[idx] = count; 			tenshi.P_observer[idx] = count;
