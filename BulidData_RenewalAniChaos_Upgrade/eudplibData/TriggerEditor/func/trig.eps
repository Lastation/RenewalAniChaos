/***
 * @Type
 * F
 * @Summary.ko-KR
 * 캐릭터에게 리콜이펙트를 줄때 사용합니다.
***/
function Effect_Recall()
{
	var playerID = getcurpl();
	MoveLocation(v.P_HoldLocation[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
	
	if (Switch(53 + playerID, Cleared))
	{ SetSwitch(53 + playerID, Set); }
}

/***
