import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;
import character.marge as marge;

import character.group1.maihime.main as maihime;

const stb = StringBuffer();

function onPluginStart() 	// 초기화 함수
{
}

function beforeTriggerExec() 
{

	randomize();
	
	// Player Loop
	foreach(playerID : EUDLoopPlayer())
	{
		setcurpl(playerID);
		marge.main(playerID);
		maihime.main(playerID);
		MuteUnitSpeech();
		s.main(playerID);	
	}
	
	// Observer Loop
	foreach (observerID : EUDLoopRange(128, 132))
	{
		setcurpl(observerID);
		MuteUnitSpeech();
		s.main(observerID);	
	}
}