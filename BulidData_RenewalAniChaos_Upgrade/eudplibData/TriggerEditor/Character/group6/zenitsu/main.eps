import character.group6.zenitsu.ultimate.ultimate2 as AAAAA;

function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 아가츠마 \x1B젠이츠　\x04[ 귀멸의 칼날 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List\r\n　　　");
	      	v.stb.printAt(3, "　　　\x07O \x19 벽력일섬 [ 일직선 대시 / 적 6초 이속 감소 / 4초 무적  ] \x05개인쿨 3분");
		v.stb.printAt(4, "　　　\x1FSSC \x046개의 기술 \x19[ 만능 ]");
		v.stb.printAt(5, "　　　\x1FCCC + AS \x04기대 \x19[ 대인, AS부터 쉴드고정 1 ]\x05");
		v.stb.printAt(6, "　　　\x1FACC \x04꿈 \x19[ 대인, 쉴드고정 1, 순간딜 ] ");
		v.stb.printAt(7, "　　　\x08AAA \x04벽력일섬6연 \x19[ 대인 / 쉴드 20고정 / 컴퓨터동맹 ] \x1F[1] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(8, "　　　\x08 \x1F[1] + \x08AA \x04전집중 호흡 \x19[ 공성 / 장판형 / 쉴드 1고정 ] \x05", v.P_Ultimate2[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 33;
	v.P_UnitID[playerID] 			= 54;
	v.P_LocationID[playerID] 		= 204;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 250;
	
	commend.main(playerID);
	skillList(playerID);
	
	O.debuff(playerID);
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSC.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAS.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;	
		}
	}
}
