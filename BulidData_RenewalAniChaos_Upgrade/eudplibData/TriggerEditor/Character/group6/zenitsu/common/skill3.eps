import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			
			if (v.P_LoopMain[playerID] <= 2)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",48*v.P_LoopMain[playerID],48*v.P_LoopMain[playerID]);
				trg.Shape_Square(playerID,1,"Protoss Dark Archon",48*v.P_LoopMain[playerID],48*v.P_LoopMain[playerID]);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(200);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Mojo", 48, 48);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",48,0);
				trg.Shape_Square(playerID,1,"40 + 1n Goliath", 48, 48);
				trg.Shape_Edge(playerID,1,"60 + 1n High Templar",45,5,48);
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(200);
		
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}