import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;


function main(playerID)
{	
	trg.Buff_ShieldFix(1);
	if (v.P_WaitMain[playerID] == 0)
	{
		MoveUnit(All,"50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
		MoveUnit(All,"40 + 1n Marine", playerID, "Anywhere", "[Skill]HoldPosition");
		if(v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All,"80 + 1n Vulture","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Wraith","Anywhere",playerID);
			KillUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(150);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);		
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,128);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);	
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(150);
			}
			else if (v.P_LoopMain[playerID] <= 11)
			{
				MoveLocation(v.P_LocationID[playerID],"50 + 1n Battlecruiser",playerID,"Anywhere");
				KillUnitAt(1,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.SkillUnit(playerID,1,"80 + 1n Vulture");
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);	
				Order("80 + 1n Vulture",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(100);
			}
			else if (v.P_LoopMain[playerID] <= 20)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Wraith",15 * v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n High Templar",15 * v.P_LoopMain[playerID],2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Mojo",15 * v.P_LoopMain[playerID],2,128,0);
				trg.Shape_Dot(playerID,1,"Kakaru",0,0);
				KillUnitAt(All,"60 + 1n High Templar","Anywhere",playerID);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				KillUnitAt(All,"Kakaru","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);	
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(150);
			}
			else if (v.P_LoopMain[playerID] == 21)
			{
				trg.Shape_Edge(playerID,1,"50 + 1n Tank",45,4,128);
				trg.Shape_Edge(playerID,1,"40 + 1n Gantrithor",45,4,128);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				trg.Main_Wait(500);
			}
			else if (v.P_LoopMain[playerID] == 22)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(230);
			}
			else if (v.P_LoopMain[playerID] == 23)
			{
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Artanis",3,48);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("80 + 1n Artanis",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 24)
			{
				KillUnitAt(All,"80 + 1n Artanis","Anywhere",playerID);
				trg.Main_Wait(500);
			}
			else if (v.P_LoopMain[playerID] <= 36)
			{
				MoveLocation(v.P_LocationID[playerID],"50 + 1n Tank",playerID,"Anywhere");
				KillUnitAt(1,"50 + 1n Tank","Anywhere",playerID);
				trg.SkillUnit(playerID,1,"40 + 1n Marine");
				MoveUnit(All,"40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				trg.Main_Wait(85);
			}
				else if (v.P_LoopMain[playerID] <= 38)
			{
				trg.Shape_Edge(playerID,1," Creep. Dunkelheit",0,3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(200);
			}
			else if (v.P_LoopMain[playerID] == 39)
			{
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}