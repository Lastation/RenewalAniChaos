import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

var x = 0;
function main(playerID)
{
	trg.Buff_ShieldFix(1);
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All," Creep. Dunkelheit","Anywhere",playerID);
			KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if(v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Edge(playerID,1," Creep. Dunkelheit",45,3,64);
				trg.Shape_Edge(playerID,1,"40 + 1n Mojo",45,3,64);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if(v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Edge(playerID,1," Creep. Dunkelheit",45,6,128);
				trg.Shape_Edge(playerID,1,"40 + 1n Mojo",45,6,128);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order(" Creep. Dunkelheit",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if(v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Mojo",45,3,64);
				trg.Shape_Edge(playerID,1,"60 + 1n Archon",45,3,64);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if(v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Edge(playerID,1,"40 + 1n Mojo",45,6,128);
				trg.Shape_Edge(playerID,1,"60 + 1n Archon",45,6,128);
				KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if(v.P_LoopMain[playerID] == 5)
			{
				trg.Main_Wait(1500);
			}
			else if(v.P_LoopMain[playerID] == 6)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				trg.Shape_NxNSquare(playerID,1,"80 + 1n Vulture",3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("80 + 1n Vulture",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(400);
			}
			else if(v.P_LoopMain[playerID] == 7)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				KillUnitAt(All,"80 + 1n Vulture","Anywhere",playerID);
				trg.Main_Wait(1500);
			}
			else if(v.P_LoopMain[playerID] <= 13)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Goliath",60 * v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Ghost",45 * v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",60 * v.P_LoopMain[playerID],2,320,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",45 * v.P_LoopMain[playerID],2,320,0);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				MoveUnit(All,"40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Goliath",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Ghost",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(100);
			}
			else if(v.P_LoopMain[playerID] <= 15)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			
			else if(v.P_LoopMain[playerID] == 16)
			{
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				KillUnitAt(All,"40 + 1n Goliath","Anywhere",playerID);
				KillUnitAt(All,"40 + 1n Ghost","Anywhere",playerID);
				x = 0;
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}