import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

var x = 0;
function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{	
			KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Tank","Anywhere",playerID);
			if (v.P_LoopMain[playerID] <= 3)
			{
				trg.Shape_Square(playerID,1,"50 + 1n Tank",0+32*v.P_LoopMain[playerID],128- 32*v.P_LoopMain[playerID]);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",0+32*v.P_LoopMain[playerID],128 - 32*v.P_LoopMain[playerID]);
				KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(250);
			}
			else if (v.P_LoopMain[playerID] <= 11)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",128-16*x,32+16*x);
				trg.Shape_Square(playerID,1,"Protoss Dark Archon",128-16*x,32+16*x);
				trg.Shape_Square(playerID,1,"Kakaru",64-8*x,64+8*x);
				KillUnitAt(All,"Kakaru","Anywhere",playerID);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				x += 1;
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 12)
			{
				x = 0 ;
				trg.Shape_Dot(playerID,1,"50 + 1n Battlecruiser",0,0);
				Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(300);
			}
			else if (v.P_LoopMain[playerID] == 13)
			{
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				trg.Main_Wait(500);
			}
			else if (v.P_LoopMain[playerID] == 14)
			{
				trg.Shape_Square(playerID,4,"40 + 1n Ghost",128,128);
				trg.Shape_Square(playerID,1,"Kakaru",128,128);
				trg.Shape_Dot(playerID,1,"Kakaru",0,0);
				KillUnitAt(All,"Kakaru","Anywhere",playerID);
				Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(200);
			}
			else if (v.P_LoopMain[playerID] == 15)
			{
				KillUnitAt(All,"40 + 1n Ghost","Anywhere",playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}