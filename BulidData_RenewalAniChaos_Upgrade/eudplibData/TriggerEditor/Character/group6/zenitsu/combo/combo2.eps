import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

var x = 0;
function main(playerID)
{
	if(v.P_LoopMain[playerID] >= 16)
	{
		trg.Buff_ShieldFix(1);
	}
	if (v.P_WaitMain[playerID] == 0)
	{
		//MoveUnit(All, "40 + 1n Marine", playerID, "Anywhere", "[Skill]HoldPosition");
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All,"40 + 1n Mojo","Anywhere",playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",64,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",64,0);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Main_Wait(125);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Battlecruiser",3,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(375);
			}
			else if (v.P_LoopMain[playerID] <= 8)
			{	
				KillUnitAt(All,"50 + 1n Battlecruiser","Anywhere",playerID);
				//trg.Shape_Square(playerID,1,"40 + 1n Marine",48*x,0);
				trg.Shape_Square(playerID,1,"80 + 1n Artanis",48*x,0);
				x += 1;
				KillUnitAt(All,"80 + 1n Artanis","Anywhere",playerID);
				//Order("40 + 1n Marine",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(60);
			}
			else if(v.P_LoopMain[playerID] == 9)
			{
				x = 0;
			}
			else if (v.P_LoopMain[playerID] <= 14)
			{	
				//trg.Shape_Square(playerID,1,"40 + 1n Marine",48*x,48*x);
				trg.Shape_Square(playerID,1,"80 + 1n Artanis",48*x,48*x);
				x += 1;
				KillUnitAt(All,"80 + 1n Artanis","Anywhere",playerID);
				//trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				//Order("40 + 1n Marine",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(60);
			}
				else if(v.P_LoopMain[playerID] == 15)
			{
				x = 0;
			}
			else if(v.P_LoopMain[playerID] == 16)
			{
				RemoveUnitAt(All,"40 + 1n Marine","Anywhere",playerID);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",48,0);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",96,0);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",144,0);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",192,0);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",240,0);
				
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",48,48);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",96,96);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",144,144);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",192,192);
				trg.Shape_Square(playerID,1,"40 + 1n Mojo",240,240);
				
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if(v.P_LoopMain[playerID] == 17)
			{
				trg.Main_Wait(300);
			}
			else if(v.P_LoopMain[playerID] == 18)
			{
				trg.Shape_Square(playerID,1,"50 + 1n Battlecruiser",64,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(600);
			}
			else if(v.P_LoopMain[playerID] <= 28)
			{
				trg.Shape_Line(playerID,1,"40 + 1n Mojo",30 * v.P_LoopMain[playerID],4,64,0);
				trg.Shape_Line(playerID,1,"40 + 1n Zealot",30 * v.P_LoopMain[playerID],4,64,0);
				KillUnitAt(All,"40 + 1n Zealot","Anywhere",playerID);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Mojo",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(125);
			}
			else if(v.P_LoopMain[playerID] == 29)
			{
				trg.Shape_Square(playerID,1,"60 + 1n Siege",256,0);
				trg.Shape_Square(playerID,1,"60 + 1n Siege",192,64);
				trg.Shape_Square(playerID,1,"60 + 1n Siege",128,128);
				trg.Shape_Square(playerID,1,"60 + 1n Siege",64,192);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Siege",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(300);
			}
			else if (v.P_LoopMain[playerID] == 30 
			&& Bring(playerID, AtLeast, 2,"Protoss Arbiter", "[Skill]UseSkill"))
			{
				
				s.CharacterVoice(5);
				KillUnitAt(2, "Protoss Arbiter", "[Skill]UseSkill", playerID);
				
			
				v.P_Step[playerID] = 220;
				v.P_CountMain[playerID] = 0;
				v.P_LoopMain[playerID] = 0;
			}
			else if(v.P_LoopMain[playerID] == 30)
			{
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);	
				x = 0;
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
		
	
	}
}