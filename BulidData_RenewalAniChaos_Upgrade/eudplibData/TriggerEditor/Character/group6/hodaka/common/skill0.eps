import variable as v;
import func.trig as trg;

import func.sound as s;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {      

            trg.Shape_Edge(playerID, 1, "60 + 1n Dragoon", 45, 3, 50);
            KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
         
         	if (playerID < 3)
         	{
 	       	SetDeaths(P4, Add, 120, " `UniqueCoolTime");
 	       	SetDeaths(P5, Add, 120, " `UniqueCoolTime");
       	 	SetDeaths(P6, Add, 120, " `UniqueCoolTime");
         	}
         	else
         	{
 	       	SetDeaths(P1, Add, 120, " `UniqueCoolTime");
 	       	SetDeaths(P2, Add, 120, " `UniqueCoolTime");
       	 	SetDeaths(P3, Add, 120, " `UniqueCoolTime");
         	}


            trg.Main_Wait(80);
            
            v.P_CountMain[playerID] = 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         SetDeaths(playerID, SetTo, 1440, " `UniqueCoolTime");
         trg.SkillEnd();
      }

   }
}