	      v.stb.printAt(4, "　　　\x04\x1FSSS\x04 귀여워 \x19[ 공성 ]");
	      v.stb.printAt(5, "　　　\x04\x1FCCC + A\x04 추억 \x19[ 공성 / 순간딜 / 자리고정 ]");
	      v.stb.printAt(6, "　　　\x04\x1FCAS + CC + CC \x04첫 경험 \x19[ 공성 / 지속딜 / 자리고정 ] \x1F[1]");
	      v.stb.printAt(7, "\n");
      	      v.stb.printAt(8, "　　　\x1F[1]\x08 + AA \x04인생의 동반자 \x19[ 공성 / 선딜 5.5초 ] \x05", v.P_Ultimate1[playerID]);
	      v.stb.printAt(9, "　　　\x08AAAA \x04인생의 동반자 \x19[ 공성 / 선딜 5.5초 ] \x05", v.P_Ultimate2[playerID]);
	      v.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 34;
	v.P_LocationID[playerID] = 203;
	v.P_UnitID[playerID] = 53;
	
	v.P_Ultimate1[playerID] = 700;
	v.P_Ultimate2[playerID] = 650;

	SkillList(playerID);
	commend.main(playerID);

	O.main(playerID);

	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCA.main(playerID);
			break;
		case 230:
			CCA.main(playerID);
			break;
		case 240:
			CCAAA.main(playerID);
			break;
		case 250:
			CCAAACC.main(playerID);
			break;
		case 260:
			CCAAACCAA.main(playerID);
			break;
		case 310:
			AAAA.main(playerID);
			break;
		}
		
		
	}
	

}
