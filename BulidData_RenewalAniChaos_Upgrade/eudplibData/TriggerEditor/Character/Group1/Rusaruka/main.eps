import PluginVariables as msqcvar;
import variable as v;

import character.group1.rusaruka.commend as commend;

import character.group1.rusaruka.common.skill1 as O;
import character.group1.rusaruka.common.skill2 as S;
import character.group1.rusaruka.common.skill3 as C;
import character.group1.rusaruka.common.skill4 as A;

import character.group1.rusaruka.combo.combo1 as SSS;
import character.group1.rusaruka.combo.combo2 as CCC;
import character.group1.rusaruka.combo.combo3 as CCCAA;
import character.group1.rusaruka.combo.combo4 as ACC;

import character.group1.rusaruka.ultimate.ultimate1 as AAAC;

function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 루살카 \x1B슈베게린　\x04[ 디에스 이래 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04창조 \x19[ 15x15 / 적 5초마다 경직 / 파괴될시 5분후 재생 / 설치형 ]");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04위협 \x19[ 대인 / 공성 ]");
		v.stb.printAt(5, "　　　\x04\x1FCC + AAS \x04고문바퀴 \x19[ 공성 / 후속 스킬 귀환봉인, 자리고정 ]");
		v.stb.printAt(6, "　　　\x04\x1FACS \x04고문용 쇠사슬 \x19[ 대인 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAAC \x04갈망 \x19[ 공성 / 선딜없음 /  장판형 스킬 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 1;
	v.P_UnitID[playerID] 			= 51;
	v.P_LocationID[playerID] 		= 32;
	
	v.P_Ultimate1[playerID] = 650;
	
	commend.main(playerID);
	skillList(playerID);
	
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 320:
			AAAC.main(playerID);
			break;
		}
	}
	
}