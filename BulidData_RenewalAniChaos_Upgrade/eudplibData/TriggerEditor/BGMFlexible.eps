const bgmlen = [0x43464040];
const str = StringBuffer();
function PlayOGG(bgmindex ,bgmcode, track){
    SetMemoryEPD(str.epd, SetTo, bgmcode);
    SetMemoryEPD(str.epd + 1, SetTo, track);
    str.Play();

    if (MemoryEPD(EPD(bgmlen) + bgmindex, Exactly, track)){
        return 0;
    }else{
        return 1;
    }
}
