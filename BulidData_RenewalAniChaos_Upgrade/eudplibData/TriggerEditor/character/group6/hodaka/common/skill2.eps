
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All,"50 + 1n Battlecruiser", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			
			if (v.P_LoopMain[playerID] <= 2)
			{
				if (v.P_LoopMain[playerID] % 2 == 0) { trg.Shape_Square(playerID, 1,"40 + 3n Zeratul", 48, 48); 	}
				else 								{ trg.Shape_Square(playerID, 1, "40 + 3n Zeratul", 48, 0); 	}
				KillUnitAt(All,"40 + 3n Zeratul","Anywhere",playerID);
				trg.Main_Wait(100);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Square(playerID, 1,"50 + 1n Battlecruiser", 96, 64);
				trg.Shape_Square(playerID,1,"40 + 1n Goliath",96,64);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(250);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_Square(playerID, 1,"50 + 1n Battlecruiser", 64, 96);
				trg.Shape_Square(playerID,1,"40 + 1n Goliath",64,96);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("50 + 1n Battlecruiser",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(250);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				trg.Shape_Double(playerID,1,"40 + 1n Wraith",48,0);
				trg.Shape_Double(playerID,1,"40 + 1n Wraith",0,48);
				trg.Shape_Dot(playerID,1,"40 + 1n Wraith",0,0);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("40 + 1n Wraith",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(130);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 6)
			{
				trg.SkillEnd();
			}
		}
	}
}