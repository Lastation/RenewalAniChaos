import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Buff_ShieldFix(1);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 25);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 25, 75);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 75, 25);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 25, 75);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 2)
         {
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 75, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 50, 25);
            trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 25, 50);
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         if (v.P_LoopMain[playerID] == 10)
         {
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 75, 0);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 50);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 100, 100);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 125, 0);
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 3, 75);
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         }
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
         v.P_CountMain[playerID] += 1;
         v.P_LoopMain[playerID] = 0;
         }

      }
         
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
         KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
         trg.Shape_Double(playerID, 1, "50 + 1n Battlecruiser", 125, 0);
         trg.Shape_Edge(playerID, 1, "40 + 1n Goliath", 45,2, 32);
         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]); 
         Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);     
         }
         if (v.P_LoopMain[playerID] == 4 )
         {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 6 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 8 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 10 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 12 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, 0);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 14 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 50);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         if (v.P_LoopMain[playerID] == 16 )
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 50);
         Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         
       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 18)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 4 )
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Ghost", 3, 75);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);    
         }

         if (v.P_LoopMain[playerID] == 6 )
         {
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 3, 75);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 0, 150);
            trg.Shape_NxNSquare(playerID, 1, "Terran Science Vessel", 3, 75);
            trg.Shape_Square(playerID, 1, "Terran Science Vessel", 0, 150);
            KillUnitAt(All, "Terran Science Vessel", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);    
         }


       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 10)
         {
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] == 6 )
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Ghost", 3, 75);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);    
         }
         if (v.P_LoopMain[playerID] == 8 )
         {
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Guardian", 3, 75);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 0, 150);
            trg.Shape_NxNSquare(playerID, 1, "Terran Science Vessel", 3, 75);
            trg.Shape_Square(playerID, 1, "Terran Science Vessel", 0, 150);
            KillUnitAt(All, "Terran Science Vessel", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);   
         }

       trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 12)
         {
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            
            trg.SkillEnd();
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
   }
}
