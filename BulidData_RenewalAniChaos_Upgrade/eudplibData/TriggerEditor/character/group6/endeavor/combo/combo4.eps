import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

var x = 0;
function main(playerID)
{	
	trg.Debuff_Stop();
	trg.Debuff_BanReturn();
	if(v.P_LoopMain[playerID] >= 24)
	{
		trg.Buff_ShieldFix(1);
	}
	if (v.P_WaitMain[playerID] == 0)
	{
		if(v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] >= 40)
				RemoveUnitAt(All,"60 + 1n Danimoth","Anywhere",playerID);
			KillUnitAt(All,"60 + 1n Archon","Anywhere",playerID);
			if (v.P_LoopMain[playerID] <= 6)
			{
				trg.Shape_Edge(playerID,1,"Protoss Dark Archon",45,3+x,64+32*x);
				KillUnitAt(All,"Protoss Dark Archon","Anywhere",playerID);
				x += 1;
				trg.Main_Wait(80);
			}
			if (v.P_LoopMain[playerID] == 7)
			{
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",256,256);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",256,192);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",192,256);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",192,192);
				
				trg.Shape_Square(playerID,1,"50 + 1n Tank",256,256);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",256,192);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",192,256);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",192,192);
				
				trg.Shape_Square(playerID,1,"50 + 1n Tank",256,32);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",256,-32);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",192,32);
				trg.Shape_Square(playerID,1,"50 + 1n Tank",192,-32);
				
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",256,32);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",256,-32);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",192,32);
				trg.Shape_Square(playerID,1,"60 + 1n Danimoth",192,-32);
				
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Gantrithor",6,64);
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Firebat",4,160);
				trg.Shape_NxNSquare(playerID,1,"40 + 1n Marine",4,160);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
				MoveUnit(All,"40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				MoveUnit(All,"40 + 1n Firebat", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
				Order("40 + 1n Firebat",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("40 + 1n Marine",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("50 + 1n Tank",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				Order("60 + 1n Danimoth",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				x = 0;
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				trg.Main_Wait(1500);
			}
			else if (v.P_LoopMain[playerID] <= 23)
			{
				KillUnitAt(2,"60 + 1n Danimoth","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if (v.P_LoopMain[playerID] <= 39)
			{
				KillUnitAt(2,"50 + 1n Tank","Anywhere",playerID);
				KillUnitAt(1,"40 + 1n Firebat","Anywhere",playerID);
				KillUnitAt(1,"40 + 1n Marine","Anywhere",playerID);
				trg.Main_Wait(80);
			}
			else if(v.P_LoopMain[playerID] == 40)
			{
				trg.Main_Wait(800);
			}
			else if (v.P_LoopMain[playerID] <= 58)
			{
				trg.Shape_Line(playerID,1,"60 + 1n Archon",0+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",90+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"Terran Science Vessel",0+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"Terran Science Vessel",90+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Siege",90+15*x,2,320,0);
				KillUnitAt(All,"Terran Science Vessel","Anywhere",playerID);
				x += 1;
				trg.Main_Wait(160);
				
			}
			else if (v.P_LoopMain[playerID] <= 76)
			{
				KillUnitAt(2,"60 + 1n Siege","Anywhere",playerID);
				trg.Shape_Line(playerID,1,"60 + 1n Danimoth",90+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Danimoth",0+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",90+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"60 + 1n Archon",0+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",90+15*x,2,256,0);
				trg.Shape_Line(playerID,1,"40 + 1n Gantrithor",0+15*x,2,256,0);
				trg.MoveLoc(v.P_UnitID[playerID],playerID,0,0);
				Order("60 + 1n Danimoth",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				KillUnitAt(All,"40 + 1n Gantrithor","Anywhere",playerID);
				x += 1;
				trg.Main_Wait(160);
			}
			else if(v.P_LoopMain[playerID] == 77)
			{
				x = 0;
				trg.Shape_NxNSquare(playerID,1,"50 + 1n Tank",7,64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID,0,0);
				Order("50 + 1n Tank",playerID,"Anywhere",Attack,v.P_LocationID[playerID]);
				trg.Main_Wait(200);
			}
			else if (Bring(playerID, AtLeast, 1,"Protoss Corsair", "[Skill]UseSkill") 
			&& v.P_LoopMain[playerID] == 78
			&& v.P_UltimateGauge[playerID] >= v.P_Ultimate2[playerID]
			&& v.P_Step[playerID] == 320)
			{
				if (Switch("UiltimateSwitch", Cleared))
				{
					s.CharacterVoice(10);
					SetSwitch("UiltimateSwitch", Set);
               			CreateUnit(1, " Item. Flag", "[Uiltimate]Flag", CurrentPlayer);
					v.P_Step[playerID] = 330;
					v.P_SkillDelay[playerID] = 0;
					v.P_CountSub1[playerID] = 1;
					SetDeaths(playerID, Subtract, v.P_Ultimate2[playerID], " `UltimateCoolTime");
					v.P_CountMain[playerID] = 0;
					v.P_LoopMain[playerID] = 0;
					KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
					KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
					KillUnitAt(All,"50 + 1n Tank","Anywhere",playerID);
				}
				else
				{
					SetResources(CurrentPlayer, Add, 60, Gas);
					KillUnitAt(1, "Protoss Corsair", "[Skill]UseSkill", playerID);
					KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
					KillUnitAt(All,"50 + 1n Tank","Anywhere",playerID);
					SetDeaths(CurrentPlayer, SetTo, 999, " `SYSTEMTEXT");
					trg.SkillEnd();
				}	
			}
			else if(v.P_LoopMain[playerID] == 78)
			{	
				KillUnitAt(All,"60 + 1n Siege","Anywhere",playerID);
				KillUnitAt(All,"50 + 1n Tank","Anywhere",playerID);
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}