import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

import character.group2.sakura.common.skill1 as O;
import character.group2.sakura.common.skill2 as S;
import character.group2.sakura.common.skill3 as C;
import character.group2.sakura.common.skill4 as A;

import character.group2.sakura.combo.combo1 as SSA;
import character.group2.sakura.combo.combo2 as CCC;
import character.group2.sakura.combo.combo3 as CCCAA;

import character.group2.sakura.ultimate.ultimate2 as AAAA;
import character.group2.sakura.ultimate.ultimate1 as CCCAAAA;

import character.group2.sakura.text as text;
import character.group2.sakura.commend as commend;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 마토 사쿠라　\x04[ Fate/stay night - Heaven's Feel' ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04마력 흡수 \x19[ 적 전체 가스 400 제거 / 가스 400 회복 ] \x053분");
		v.stb.printAt(4, "　　　\x04\x1FSSA \x04포식 \x19[ 대인 / 순간딜 ]");
		v.stb.printAt(5, "　　　\x04\x1FCCC + AA \x04불행 \x19[ 공성 / 자리고정 / 귀환봉인 ] \x1F[1]");
		v.stb.printAt(6, "\n");
		v.stb.printAt(7, "　　　\x1F[1]\x08+ AA \x04바램 \x19[ 공성 / 선딜 5초 / 장판형 / 쉴드 1 고정 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(8, "　　　\x08AAAA \x04바램 \x19[ 공성 / 선딜 5초 / 장판형 / 쉴드 1 고정 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}
function main(playerID)
{
	v.P_HeroID[playerID] = 13;
	v.P_LocationID[playerID] = 163;
	v.P_UnitID[playerID] = 16;
	
	v.P_Ultimate1[playerID] = 650;
	v.P_Ultimate2[playerID] = 650;
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSA.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 310:
			CCCAAAA.main(playerID);
			break;
		case 320:
			AAAA.main(playerID);
			break;
			
		}
	}
}
