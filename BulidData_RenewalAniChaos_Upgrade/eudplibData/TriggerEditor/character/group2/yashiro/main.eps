function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 야시로 가쿠　\x04[ 나만이 없는 거리 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04조소 \x19[ 10초 간 공격력 10 증가 ] \x052분 30초");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04헛된 소망 \x19[ 만능 ]");
		v.stb.printAt(5, "　　　\x04\x1FAAC \x04게임 오버 \x19[ 대인 / 순간딜 ]");
		v.stb.printAt(6, "　　　\x04\x1FCCC + A \x04손에 넣는 것 \x19[ 대인 / 지속딜 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAA \x04타임 오버 \x19[ 대인 / 컴퓨터 동맹 / 무적 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}
function main(playerID)
{
	v.P_HeroID[playerID] = 14;
	v.P_LocationID[playerID] = 164;
	v.P_UnitID[playerID] = 99;
	
	v.P_Ultimate1[playerID] = 500;
	SkillList(playerID);
	commend.main(playerID);
	
	//P.main(playerID);
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
			
		}
	}
}
