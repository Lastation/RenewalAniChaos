import variable as v;
import func.trig as trg;

import character.group5.yume.common.skill1 as O;
import character.group5.yume.common.skill2 as S;
import character.group5.yume.common.skill3 as C;
import character.group5.yume.common.skill4 as A;

import character.group5.yume.combo.combo1 as SSS;
import character.group5.yume.combo.combo2 as CC;
import character.group5.yume.combo.combo3 as CCA;
import character.group5.yume.combo.combo4 as CAAS;
import character.group5.yume.combo.combo5 as CAASAC;

import character.group5.yume.ultimate.ultimate1 as AAA;

import character.group5.yume.text as text;
import character.group5.yume.commend as commend;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 츠바쿠로 유메　\x04[ 도사의 무녀 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04기습 \x19[ 순간딜 / 25 x 25 적 영웅에게 이동 ] \x051분 30초");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04여유 \x19[ 대인 ]");
		v.stb.printAt(5, "　　　\x04\x1FCC + A \x04닛카리 아오에 \x19[ 대인 ]");
		v.stb.printAt(6, "　　　\x04\x1FCAAS + AC \x04전투광 \x19[ 대인 / 쉴드고정 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAA \x04시간 낭비 \x19[ 선입력 0.5초 / 대인 / 시전 중 무적 / 25 x 25 영웅 로케 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 26;
	v.P_LocationID[playerID] = 190;
	v.P_UnitID[playerID] = 74;
	
	v.P_Ultimate1[playerID] = 500;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 230:
			CC.main(playerID);
			break;
		case 240:
			CCA.main(playerID);
			break;
		case 210:
			CAAS.main(playerID);
			break;
		case 220:
			CAASAC.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
			
		}
	}
}
