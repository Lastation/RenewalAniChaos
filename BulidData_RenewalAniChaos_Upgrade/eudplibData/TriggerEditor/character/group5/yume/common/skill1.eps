import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 2)
         {          
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, 0);

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Main_Wait(320);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {                        
            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 2)
         {          
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, 0);

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Main_Wait(320);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {                        
            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 4)
         {          
            trg.Shape_Dot(playerID, 8, "Bengalaas (Jungle)", 0, 0);
            trg.Shape_Dot(playerID, 1, "40 + 1n Guardian", 0, 0);

            KillUnitAt(All, "Bengalaas (Jungle)", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {                        
            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {         
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveLocation("26.Yume_Bozo", v.P_UnitID[playerID], playerID, "Anywhere");

            if (playerID < 3)
            {
               if (Bring(3, AtLeast, 1, v.P_UnitID[3], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[3], 3, "Anywhere");
               }
               else if (Bring(4, AtLeast, 1, v.P_UnitID[4], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[4], 4, "Anywhere");
               }
               else if (Bring(5, AtLeast, 1, v.P_UnitID[5], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[5], 5, "Anywhere");
               }

            }
            else if (playerID >= 3)
            {
               if (Bring(0, AtLeast, 1, v.P_UnitID[0], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[0], 0, "Anywhere");
               }
               else if (Bring(1, AtLeast, 1, v.P_UnitID[1], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID],v.P_UnitID[1], 1, "Anywhere");
               }
               else if (Bring(2, AtLeast, 1, v.P_UnitID[2], "26.Yume_Bozo"))
               {
                  MoveLocation(v.P_LocationID[playerID], v.P_UnitID[2], 2, "Anywhere");
               }
            }
            MoveUnit(All, v.P_UnitID[playerID], playerID, "Anywhere", v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 4)
         {          
            trg.Shape_Edge(playerID, 1, "Kakaru (Twilight)", 0, 3 + 2 * v.P_LoopMain[playerID], 50 + 50 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            if (v.P_LoopMain[playerID] % 2 == 0)
            {
               trg.Shape_Edge(playerID, 1, "40 + 1n Zealot", 0, 3 + 2 * v.P_LoopMain[playerID], 50 + 50 * v.P_LoopMain[playerID]);
               KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
            }
            else
            {
               trg.Shape_Edge(playerID, 1, "Protoss Dark Templar", 0, 3 + 2 * v.P_LoopMain[playerID], 50 + 50 * v.P_LoopMain[playerID]);
               KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);
            }

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {                        
            trg.Shape_Line(playerID, 1, "Target", 45, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "Target", 45, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "Target", 225, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "Protoss Dark Archon", 45, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "Protoss Dark Archon", 45, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "Protoss Dark Archon", 225, 5, 75, 75);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {                        
            trg.Shape_Line(playerID, 1, "80 + 1n Artanis", 135, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "80 + 1n Artanis", 135, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "80 + 1n Artanis", 315, 5, 75, 75);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 135, 5, 75, 75);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 135, 5, 75, 0);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 315, 5, 75, 75);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("80 + 1n Artanis", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {                        
            RemoveUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);

            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 135, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 135, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 315, 5, 75, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            
            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {                        
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 45, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 45, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 225, 5, 75, 75);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 45, 5, 75, 75);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 45, 5, 75, 0);
            trg.Shape_Line(playerID, 1, " Unit. Hoffnung 25000", 225, 5, 75, 75);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {                        
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 45, 5, 75, 75);
            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 45, 5, 75, 0);
            trg.Shape_Line(playerID, 1, "Kakaru (Twilight)", 225, 5, 75, 75);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            
            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }

         else if (v.P_LoopMain[playerID] == 9)
         {         
            SetDeaths(playerID, SetTo, 1080, " `UniqueCoolTime");

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (Bring(playerID, AtLeast, 1, "Protoss Carrier", "[Skill]UseSkill"))
         {
            v.P_SkillDelay[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            v.P_Step[playerID] = 200;
            KillUnitAt(1, "Protoss Carrier", "[Skill]UseSkill", playerID);
         }
         else if (Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill"))
         {
            v.P_SkillDelay[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            v.P_Step[playerID] = 300;
            KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
         }
         
         else if (Bring(playerID, AtLeast, 1, "Protoss Scout", "[Skill]UseSkill"))
         {
            v.P_SkillDelay[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            v.P_Step[playerID] = 100;
            KillUnitAt(1, "Protoss Scout", "[Skill]UseSkill", playerID);
         }
         else
         {
            trg.SkillEnd();
         }
      }
   }
}