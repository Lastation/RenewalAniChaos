import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

import character.group5.ekidona.common.skill1 as O;
import character.group5.ekidona.common.skill2 as S;
import character.group5.ekidona.common.skill3 as C;
import character.group5.ekidona.common.skill4 as A;

import character.group5.ekidona.combo.combo1 as SSS;
import character.group5.ekidona.combo.combo2 as CCA;
import character.group5.ekidona.combo.combo3 as CCAAAC;
import character.group5.ekidona.combo.combo4 as CCC;
import character.group5.ekidona.combo.combo5 as CCCC;

import character.group5.ekidona.ultimate.ultimate1 as AAAAA;
import character.group5.ekidona.ultimate.ultimate2 as AAAAAAAA;

import character.group5.ekidona.text as text;
import character.group5.ekidona.commend as commend;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 에키드나　\x04[ Re : 제로부터 시작하는 이세계 생활 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04계약 \x19[ 아군 한 명과 계약 / 계약 대상 5분(개인쿨)마다 부활 시간 15초 단축 ] \x0530초");
		v.stb.printAt(4, "　　　\x04\x1FSSS \x04즐거움 \x19[ 테러 ]");
		v.stb.printAt(5, "　　　\x04\x1FCCA + AAC \x04당황 \x19[ 공성 / 자리고정 ]");
		v.stb.printAt(6, "　　　\x04\x1FCCC + A \x04흥미 \x19[ 만능 / 자리고정 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAAA + AAA \x04탐욕의 마녀 \x19[ 공성 ] \x05", v.P_Ultimate1[playerID], " + ",  v.P_Ultimate2[playerID]);
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 29;
	v.P_LocationID[playerID] = 195;
	v.P_UnitID[playerID] = 34;
	
	v.P_Ultimate1[playerID] = 650;
	v.P_Ultimate2[playerID] = 150;
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCA.main(playerID);
			break;
		case 220:
			CCAAAC.main(playerID);
			break;
		case 230:
			CCC.main(playerID);
			break;
		case 240:
			CCCC.main(playerID);
			break;
		case 310:
			AAAAA.main(playerID);
			break;
		case 320:
			AAAAAAAA.main(playerID);
			break;
		}
	}
	
	if (Switch("Unique - Ekidona", Set))
	{
		O.contract(playerID);
	}
}

