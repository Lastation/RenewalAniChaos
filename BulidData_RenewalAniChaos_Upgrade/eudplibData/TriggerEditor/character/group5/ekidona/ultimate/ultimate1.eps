import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

//0 -> X
//1 -> 활성
//2 -> 대기
var flag = 0;
var pre_flag = 0;

function main(playerID)
{
   MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "60 + 1n Dragoon", playerID, "Anywhere", "[Skill]HoldPosition");

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] < 9)
      {
         if (v.P_LoopMain[playerID] % 20 == 0)
         {
            flag = 2;
         }
         if (v.P_LoopMain[playerID] % 20 == 1)
         {
            trg.Shape_Dot(playerID, 8, "Zerg Devourer", 0, 0);
            if (playerID < 3)
               GiveUnits(All, "Zerg Devourer", playerID, "Anywhere", P7);
            else if (playerID >= 3)
               GiveUnits(All, "Zerg Devourer", playerID, "Anywhere", P8);
         }
      }

      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            SetSwitch("JunkYardDog - Ekidona", Set);
            trg.Buff_ShieldFix(60);
         }

         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 91)
         {
            s.CharacterVoice(3);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 66)
         {
            s.CharacterVoice(4);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 68)
         {
            s.CharacterVoice(5);

            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P7);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P8);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P11);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P9);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P12);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P10);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 57)
         {
            s.CharacterVoice(6);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 68)
         {
            s.CharacterVoice(7);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 20)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 6)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 16)
         {
            s.CharacterVoice(8);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 7)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 15)
         {
            s.CharacterVoice(9);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 8)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 43)
         {
            s.CharacterVoice(10);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 9)
      {
         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 18)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 10)
      {
         if (Bring(playerID, AtLeast, 3, "Protoss Arbiter", "[Skill]UseSkill") 
         && v.P_UltimateGauge[playerID] >= v.P_Ultimate2[playerID])
         {
            s.CharacterVoice(11);
            v.P_SkillDelay[playerID] = 0;
            v.P_CountMain[playerID] = 0;
            v.P_LoopMain[playerID] = 0;
            v.P_Step[playerID] = 320;
            SetDeaths(playerID, Subtract, v.P_Ultimate2[playerID], " `UltimateCoolTime");
            KillUnitAt(3, "Protoss Arbiter", "[Skill]UseSkill", playerID);
         }
         else
         {
            KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P7);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P8);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P11);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P9);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P12);
            KillUnitAt(All, "Zerg Devourer", "Anywhere", P10);

            SetSwitch("JunkYardDog - Ekidona", Clear);
            SetSwitch("UiltimateSwitch", Clear);
            trg.SkillEnd();
         }
      }

      if (playerID < 3)
      {
         if (Bring(P7, AtLeast, 1, "Zerg Devourer", "Anywhere"))
         {
            for (var i = 0; i < 8; i++)
            {
               MoveLocation(v.P_LocationID[playerID], "Zerg Devourer", P7, "Anywhere");
               GiveUnits(1, "Zerg Devourer", P7, "Anywhere", P9);

               if (i % 2 == 0) 
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Tom Kazansky");
                  trg.SkillUnit(playerID, 1, "Protoss Dark Archon");
               }
               else if (i % 2 == 1)
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Artanis");
                  trg.SkillUnit(playerID, 1, "60 + 1n Archon");
               }
            }

            KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
            KillUnitAt(All, "80 + 1n Tom Kazansky", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
         else
         {
            GiveUnits(All, "Zerg Devourer", P9, "Anywhere", P7);
         }
      }
      else if (playerID >= 3)
      {
         if (Bring(P8, AtLeast, 1, "Zerg Devourer", "Anywhere"))
         {
            for (var i = 0; i < 8; i++)
            {
               MoveLocation(v.P_LocationID[playerID], "Zerg Devourer", P8, "Anywhere");
               GiveUnits(1, "Zerg Devourer", P8, "Anywhere", P9);

               if (i % 2 == 0) 
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Tom Kazansky");
                  trg.SkillUnit(playerID, 1, "Protoss Dark Archon");
               }
               else if (i % 2 == 1)
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Artanis");
                  trg.SkillUnit(playerID, 1, "60 + 1n Archon");
               }
            }

            KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
            KillUnitAt(All, "80 + 1n Tom Kazansky", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
         else
         {
            GiveUnits(All, "Zerg Devourer", P9, "Anywhere", P8);
         }
      }

      if (flag == 1)
      {
         if (pre_flag != flag)
         {
            GiveUnits(All, "Zerg Devourer", P11, "Anywhere", P10);
            GiveUnits(All, "Zerg Devourer", playerID, "Anywhere", P10);

            pre_flag = flag;
         }
         if (Bring(P10, AtLeast, 1, "Zerg Devourer", "Anywhere"))
         {
            for (var i = 0; i < 8; i++)
            {
               MoveLocation(v.P_LocationID[playerID], "Zerg Devourer", P10, "Anywhere");
               GiveUnits(1, "Zerg Devourer", P10, "Anywhere", P11);

               if (i % 2 == 0) 
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Tom Kazansky");
                  trg.SkillUnit(playerID, 1, "Protoss Dark Archon");
               }
               else if (i % 2 == 1)
               {
                  trg.SkillUnit(playerID, 1, "80 + 1n Artanis");
                  trg.SkillUnit(playerID, 1, "60 + 1n Archon");
               }
            }

            KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
            KillUnitAt(All, "80 + 1n Tom Kazansky", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
         }
         else
         {
            GiveUnits(All, "Zerg Devourer", P11, "Anywhere", P10);
         }
      }
      else if (flag == 2)
      {
         if (pre_flag != flag)
         {
            GiveUnits(All, "Zerg Devourer", P9, "Anywhere", playerID);
            GiveUnits(All, "Zerg Devourer", P7, "Anywhere", playerID);
            GiveUnits(All, "Zerg Devourer", P8, "Anywhere", playerID);

            MoveUnit(All, "Zerg Devourer", playerID, "Anywhere", "[Skill]HoldPosition");

            pre_flag = flag;
         }

         if (Bring(playerID, AtLeast, 1, "Zerg Devourer", "Anywhere"))
         {
            for (var i = 0; i < 8; i++)
            {
               MoveLocation(v.P_LocationID[playerID], "Zerg Devourer", playerID, "Anywhere");
               GiveUnits(1, "Zerg Devourer", playerID, "Anywhere", P11);

               if (i % 4 == 0) 
               {
                  trg.SkillUnit(playerID, 1, "50 + 1n Battlecruiser");
                  trg.SkillUnit(playerID, 1, "60 + 1n Siege");
                  trg.SkillUnit(playerID, 1, "130 + 1n Norad");
               }
               else if (i % 4 == 1)
               {
                  trg.SkillUnit(playerID, 1, "60 + 1n Danimoth");
                  trg.SkillUnit(playerID, 1, "60 + 1n Siege");
                  trg.SkillUnit(playerID, 1, "40 + 1n Gantrithor");
               }
               else if (i % 4 == 2)
               {
                  trg.SkillUnit(playerID, 1, "40 + 1n Wraith");
                  trg.SkillUnit(playerID, 1, "50 + 1n Tank");
                  trg.SkillUnit(playerID, 1, "130 + 1n Norad");
               }
               else if (i % 4 == 3)
               {
                  trg.SkillUnit(playerID, 1, "40 + 1n Mojo");
                  trg.SkillUnit(playerID, 1, "60 + 1n Dragoon");
                  trg.SkillUnit(playerID, 1, "40 + 1n Gantrithor");
               }
            }

            KillUnitAt(All, "130 + 1n Norad", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("60 + 1n Dragoon", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else
         {
            flag = 1;
         }
      }
   }

   MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
}