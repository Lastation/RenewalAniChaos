import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 11)
         {
            trg.Shape_Dot(playerID, 1, "40 + 1n Gantrithor", 0, 0);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 11)
         {
            trg.Shape_Dot(playerID, 1, "Target", 0, 0);
            trg.Shape_Dot(playerID, 1, "60 + 1n Archon", 0, 0);
            KillUnitAt(All, "Target", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
         }
         if (v.P_LoopMain[playerID] == 12)
         {
            trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, 0);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         }

         if (v.P_LoopMain[playerID] == 25)
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Gantrithor", 3, 100);
            trg.Shape_NxNSquare(playerID, 1, "60 + 1n Archon", 3, 100);
            KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
         }


         trg.Main_Wait(80);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 26)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         SetDeaths(playerID, SetTo, 1440, " `UniqueCoolTime");
         SetDeaths(playerID, SetTo, 240, " `UniqueSkill");
         trg.SkillEnd();
      }
   }
}