import variable as v;
import func.trig as trg;
import func.trigadv as adv;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {             
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Mutalisk", 0, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, "Anywhere");

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            RemoveUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 2)
         {       
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);
            trg.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 50);
            trg.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 50);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Guardian", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(320);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            RemoveUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            adv.Shape_NxNSquareAt2(playerID, 1, "Kakaru (Twilight)", 2, 50, 100, 100);
            adv.Shape_NxNSquareAt2(playerID, 1, "Kakaru (Twilight)", 2, 50, -100, 100);
            adv.Shape_NxNSquareAt2(playerID, 1, "Kakaru (Twilight)", 2, 50, 100, -100);
            adv.Shape_NxNSquareAt2(playerID, 1, "Kakaru (Twilight)", 2, 50, -100, -100);
            adv.Shape_NxNSquareAt2(playerID, 1, " Unit. Hoffnung 25000", 2, 50, 100, 100);
            adv.Shape_NxNSquareAt2(playerID, 1, " Unit. Hoffnung 25000", 2, 50, -100, 100);
            adv.Shape_NxNSquareAt2(playerID, 1, " Unit. Hoffnung 25000", 2, 50, 100, -100);
            adv.Shape_NxNSquareAt2(playerID, 1, " Unit. Hoffnung 25000", 2, 50, -100, -100);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            trg.Main_Wait(160);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.SkillEnd();
      }
   }
}