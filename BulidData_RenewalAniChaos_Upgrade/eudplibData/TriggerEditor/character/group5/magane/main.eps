import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

import character.group5.magane.common.skill1 as O;
import character.group5.magane.common.skill2 as S;
import character.group5.magane.common.skill3 as C;
import character.group5.magane.common.skill4 as A;

import character.group5.magane.combo.combo1 as SSS;
import character.group5.magane.combo.combo2 as CCC;
import character.group5.magane.combo.combo3 as CCCAA;
import character.group5.magane.combo.combo4 as ACC;

import character.group5.magane.ultimate.ultimate1 as AAA;

import character.group5.magane.text as text;
import character.group5.magane.commend as commend;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      v.stb.printAt(0, "\n");
	      v.stb.printAt(1, "\x1F　＃\x1B- 치쿠조인 마가네　\x04[ Re : CREATORS ]");
	      v.stb.printAt(2, "　　\x1FA\x04ction List");
	      v.stb.printAt(3, "　　　\x18O \x04어휘 무한의 속임수 \x19[ 적 스킬 유닛 무효화 / 테러궁시 취소 ] \x051분 30초");
	      v.stb.printAt(4, "　　　\x04\x1FSSS \x04거짓말 \x19[ 공성 ]");
	      v.stb.printAt(5, "　　　\x04\x1FCCC + AA \x04악역 \x19[ 대인 ]");
	      v.stb.printAt(6, "　　　\x04\x1FACC \x04천재 \x19[ 대인 ]");
	      v.stb.printAt(7, "\n");
	      v.stb.printAt(8, "　　　\x08AAA \x04틴달로스의 사냥개 \x19[ 대인 ] \x05", v.P_Ultimate1[playerID]);
	      v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 27;
	v.P_LocationID[playerID] = 191;
	v.P_UnitID[playerID] = 53;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 250;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 310:
			ACC.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		}
	}
}
