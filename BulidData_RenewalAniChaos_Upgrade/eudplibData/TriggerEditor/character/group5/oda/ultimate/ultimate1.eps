import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.trigepic as epic;
import func.sound as s;

function main(playerID)
{
   trg.Debuff_Stop();
   trg.Debuff_BanReturn();

   MoveUnit(All, "40 + 1n Goliath", playerID, "Anywhere", "[Skill]HoldPosition");
   MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");

   trg.Buff_ShieldFix(1);

   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] > 0)
         {
            SetDeaths(playerID, SetTo, 1, " `ShieldRecharge");

            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 3, 75);
         }
         if (v.P_LoopMain[playerID] > 1)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 5, 150);
         }
         if (v.P_LoopMain[playerID] > 2)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 7, 225);
         }
         if (v.P_LoopMain[playerID] > 3)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 9, 300);
         }
         if (v.P_LoopMain[playerID] > 4)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 11, 375);
         }
         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 32)
         {
            s.CharacterVoice(2);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 1)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 3, 75);
         }
         if (v.P_LoopMain[playerID] < 2)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 5, 150);
         }
         if (v.P_LoopMain[playerID] < 3)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 7, 225);
         }
         if (v.P_LoopMain[playerID] < 4)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 9, 300);
         }
         if (v.P_LoopMain[playerID] < 5)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 11, 375);
         }

         if (v.P_LoopMain[playerID] == 1)
         {
            epic.Shape_Edge(playerID, 1, "40 + 1n Lurker", 45, 3, 75, 0);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            epic.Shape_Edge(playerID, 1, "40 + 1n Lurker", 45, 5, 150, 0);
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            epic.Shape_Edge(playerID, 1, "40 + 1n Lurker", 45, 7, 225, 0);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            epic.Shape_Edge(playerID, 1, "40 + 1n Lurker", 45, 9, 300, 0);
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            epic.Shape_Edge(playerID, 1, "40 + 1n Lurker", 45, 11, 375, 0);
         }

         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         RemoveUnitAt(All, "40 + 1n Lurker", "[Skill]Unit_Wait_ALL", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 14)
         {
            s.CharacterVoice(3);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 10)
         {
            if (v.P_LoopMain[playerID] < 4)
               trg.Shape_Line(playerID, 1, "60 + 1n High Templar", 90, 11, 75, 75 * 4 - 75 * v.P_LoopMain[playerID]);
            else
               trg.Shape_Line(playerID, 1, "60 + 1n High Templar", 270, 11, 75, -75 * 4 + 75 * v.P_LoopMain[playerID]);
         }

         if (v.P_LoopMain[playerID] < 11)
         {
            if (v.P_LoopMain[playerID] < 5)
               trg.Shape_Line(playerID, 1, "60 + 1n Siege", 90, 11, 75, 75 * 5 - 75 * v.P_LoopMain[playerID]);
            else
               trg.Shape_Line(playerID, 1, "60 + 1n Siege", 270, 11, 75, -75 * 5 + 75 * v.P_LoopMain[playerID]);
         }

         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 14)
         {
            KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);

            s.CharacterVoice(4);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         if (v.P_LoopMain[playerID] < 5)
         {
            trg.Shape_Edge(playerID, 1, "60 + 1n High Templar", 45, 3 + 2 * v.P_LoopMain[playerID], 75 * (v.P_LoopMain[playerID] + 1));
         }
         KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);

         if (v.P_LoopMain[playerID] < 6 && v.P_LoopMain[playerID] > 0)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Tank", 45, 1 + 2 * v.P_LoopMain[playerID], 75 * v.P_LoopMain[playerID]);
         }

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         RemoveUnitAt(All, "50 + 1n Tank", "[Skill]Unit_Wait_ALL", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 22)
         {
            s.CharacterVoice(5);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 3, 75);
            trg.Shape_Edge(playerID, 1, "80 + 1n Tank", 45, 3, 75);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 5, 150);
            trg.Shape_Edge(playerID, 1, "80 + 1n Tank", 45, 5, 150);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 7, 225);
            trg.Shape_Edge(playerID, 1, "80 + 1n Tank", 45, 7, 225);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 9, 300);
            trg.Shape_Edge(playerID, 1, "80 + 1n Tank", 45, 9, 300);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);            
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Edge(playerID, 1, "50 + 1n Battlecruiser", 45, 11, 375);
            trg.Shape_Edge(playerID, 1, "80 + 1n Tank", 45, 11, 375);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);            
         }
         KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 24)
         {
            s.CharacterVoice(6);
         }

         if (v.P_LoopMain[playerID] == 48)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            KillUnitAt(All, "60 + 1n Siege", "Anywhere", playerID);
            SetSwitch("UiltimateSwitch", Clear);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         

         trg.SkillEnd();
      }
   }
}