import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.trigadv as adv;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

         trg.Table_Sin(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);
         trg.Table_Cos(playerID, 22 + 45 * v.P_LoopMain[playerID], 100);

         trg.Shape_Square(playerID, 1, "Rhynadon (Badlands)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
         KillUnitAt(All, "Rhynadon (Badlands)", "Anywhere", playerID);

         trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, 0);
         Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Edge(playerID, 1, "50 + 1n Tank", 45, 5, 100);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_Edge(playerID, 1, " Unit. Hoffnung 25000", 45, 5, 100);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 3, 50);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            adv.Shape_LineAt2(playerID, 1, "40 + 1n Wraith", 135, 4, 50, 100, 50);
            adv.Shape_LineAt2(playerID, 1, "40 + 1n Wraith", 135, 4, 50, -100, -50);
            adv.Shape_LineAt2(playerID, 1, "50 + 1n Tank", 135, 4, 50, 100, 50);
            adv.Shape_LineAt2(playerID, 1, "50 + 1n Tank", 135, 4, 50, -100, -50);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            adv.Shape_LineAt2(playerID, 1, "40 + 1n Wraith", 45, 4, 50, -50, 100);
            adv.Shape_LineAt2(playerID, 1, "40 + 1n Wraith", 45, 4, 50, 50, -100);
            adv.Shape_LineAt2(playerID, 1, "50 + 1n Tank", 45, 4, 50, -50, 100);
            adv.Shape_LineAt2(playerID, 1, "50 + 1n Tank", 45, 4, 50, 50, -100);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
            
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
         }
         else if (v.P_LoopMain[playerID] == 6)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 7)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.SkillEnd();
      }
   }
}