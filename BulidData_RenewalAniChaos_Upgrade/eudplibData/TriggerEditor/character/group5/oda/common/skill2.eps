import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 45, 4, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Goliath", 45, 4, 75, 0);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 135, 4, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Goliath", 135, 4, 75, 0);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
         }

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 4)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 75, 0);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 125, 50);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 50, 125);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 45, 4, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Goliath", 45, 4, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Wraith", 135, 4, 75, 0);
            trg.Shape_Line(playerID, 1, "40 + 1n Goliath", 135, 4, 75, 0);
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, "Anywhere");
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 75, 0);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 125, 50);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 50, 125);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
         }

         trg.Main_Wait(160);

         v.P_LoopMain[playerID] += 1;

         if (v.P_LoopMain[playerID] == 5)
         {
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;         
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.SkillEnd();
      }
   }
}