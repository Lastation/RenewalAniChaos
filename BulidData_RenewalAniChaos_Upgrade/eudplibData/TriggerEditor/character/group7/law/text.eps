		v.stb.print("\x13\x04톱니바퀴가... 부숴졌다...!\x04");
		break;
	case 4:
		PlayWAV("law_cccaa.ogg");
		v.stb.print("\x13\x16Trafalgar Law\n");
		v.stb.print("\x13\x04이제... 아무도 되돌릴 수 없어...!\x04");
		break;
	case 5:
		PlayWAV("law_accc.ogg");
