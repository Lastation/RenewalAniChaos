function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
	      v.stb.printAt(0, "\n");
	      v.stb.printAt(1, "\x1F　＃\x16- 트라팔가 로우　\x04[ 원피스 ]");
	      v.stb.printAt(2, "　　\x1FA\x04ction List");
	      v.stb.printAt(3, "　　　\x18O \x04수술수술 열매 \x19[ 8초 간 쉴드고정 ] \x051분");
	      v.stb.printAt(4, "　　　\x04\x1FSSS\x04 끝 \x19[ 만능 ]");
	      v.stb.printAt(5, "　　　\x04\x1FCCC + AS\x04 톱니바퀴 \x19[ 만능 / 순간딜 / 자리고정 ]");
	      v.stb.printAt(6, "　　　\x04\x1FACCC + 400 Gas + AAC \x04칠무해 \x19[ 만능 / 지속딜 / 자리고정 ] \x1F[1]");
	      v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAAA \x04감마나이프 \x19[ 공성 / 선딜 6초 후 자리고정 ] \x05", v.P_Ultimate1[playerID]);
	      v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 36;
	v.P_LocationID[playerID] = 206;
	v.P_UnitID[playerID] = 53;
	
	v.P_Ultimate1[playerID] = 650;

	SkillList(playerID);
	commend.main(playerID);

	O.buff(playerID);
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 310:
			ACCC.main(playerID);
			break;
		case 320:
			ACCCAA.main(playerID);
			break;
		case 330:
			AAAA.main(playerID);
			break;
		}
		
		
	}
	

}
