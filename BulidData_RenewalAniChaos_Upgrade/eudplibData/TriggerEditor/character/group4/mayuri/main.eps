function SkillList(playerID);

function main(playerID)
{
	v.P_LocationID[playerID] = 183;
	v.P_UnitID[playerID] = 64;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 250;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 310:
			AAS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAS.main(playerID);
			break;
		case 230:
			CCCASCC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

