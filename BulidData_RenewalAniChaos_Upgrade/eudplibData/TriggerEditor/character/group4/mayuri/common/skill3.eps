import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {                        
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mojo", 3, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Marine", 50, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Marine", 0, 50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            
            trg.Main_Wait(160);
            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            trg.Shape_Dot(playerID, 1, "Protoss Dark Templar", 100 - 50 * v.P_LoopMain[playerID], 100);
            trg.Shape_Dot(playerID, 1, "Protoss Dark Templar", -100 + 50 * v.P_LoopMain[playerID], -100);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", -100, 100 - 50 * v.P_LoopMain[playerID]);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 100, -100 + 50 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
            trg.Shape_NxNSquare(playerID, 1, "Kakaru (Twilight)", 3, 50);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
            trg.Shape_Edge(playerID, 1, "40 + 1n Ghost", 45, 3, 75);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(320);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
            trg.Shape_Edge(playerID, 1, "60 + 1n Danimoth", 45, 5, 100);
            KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
            trg.Shape_Edge(playerID, 1, "40 + 1n Ghost", 45, 3, 100);
            trg.Shape_Edge(playerID, 1, "60 + 1n Archon", 45, 4, 100);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Ghost", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Ghost", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(320);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         KillUnitAt(All, "40 + 1n Ghost", "Anywhere", playerID);
         trg.SkillEnd();
      }
   }
}