import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         trg.Shape_Square(playerID, 1, "Target", 50, 0);
         trg.Shape_Square(playerID, 1, "Target", 100, 0);

         KillUnitAt(All, "Target", "Anywhere", playerID);

         trg.Main_Wait(80);

         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 150, 0);
         trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 100, 50);
         trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 50, 100);
         trg.Shape_Square(playerID, 1, "40 + 1n Marine", 100, 50);
         trg.Shape_Square(playerID, 1, "40 + 1n Marine", 50, 100);

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);

         trg.Main_Wait(80);

         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 150, 0);
         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 50);
         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 50, 100);

         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         trg.Main_Wait(240);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
         v.P_Distance[playerID] = 50;

         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0);

         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", v.P_Distance[playerID], v.P_Distance[playerID]);
         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -v.P_Distance[playerID], -v.P_Distance[playerID]);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", v.P_Distance[playerID], v.P_Distance[playerID]);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -v.P_Distance[playerID], -v.P_Distance[playerID]);

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         trg.Main_Wait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 4)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 75, 75);

         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 0, 50);
         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 0, -50);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, 50);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 0, -50);

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         trg.Main_Wait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 5)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0);

         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -50, 50);
         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 50, -50);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -50, 50);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 50, -50);

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         trg.Main_Wait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 6)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

         trg.Shape_Square(playerID, 1, "60 + 1n Archon", 75, 75);

         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -50, 0);
         trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 50, 0);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -50, 0);
         trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", 50, 0);

         MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
         MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
         Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

         KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);
         KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

         trg.Main_Wait(160);
         
         v.P_CountMain[playerID] += 1;
      }
      else if (v.P_CountMain[playerID] == 7)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         trg.SkillEnd();
      }
   }
}