function SkillList(playerID);

function main(playerID)
{
	v.P_HeroID[playerID] = 25;
	v.P_LocationID[playerID] = 188;
	v.P_UnitID[playerID] = 54;
	
	v.P_Ultimate1[playerID] = 650;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CCAA.main(playerID);
			break;
		case 220:
			CCC.main(playerID);
			break;
		case 310:
			AAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}

