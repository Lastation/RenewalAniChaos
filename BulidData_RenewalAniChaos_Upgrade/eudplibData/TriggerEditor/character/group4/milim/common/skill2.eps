import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

import func.sound as s;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            trg.Shape_Square(playerID, 1, "Target", 50, 0);
            trg.Shape_Square(playerID, 1, "Target", 100, 0);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            trg.Shape_Square(playerID, 1, "Target", 50, 100);
            trg.Shape_Square(playerID, 1, "Target", 100, 50);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 50);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 50, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 100, 50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 150);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, 100);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 150);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 150, 100);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 100);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 50);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            trg.Main_Wait(320);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {         
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 50, 0);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", -50, 0);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 50, 0);
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -50, 0);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 0, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 0, -50);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 0, -50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 50, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", -50, -50);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 50, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -50, -50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {         
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", -50, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 50, -50);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -50, 50);
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 50, -50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(240);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 9)
         {
            RemoveUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         trg.SkillEnd();
      }
   }
}