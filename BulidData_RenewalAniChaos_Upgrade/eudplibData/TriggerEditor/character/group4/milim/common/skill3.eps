import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

import func.sound as s;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {      
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Table_Sin(playerID, 45 * v.P_LoopMain[playerID], 50);
            trg.Table_Cos(playerID, 45 * v.P_LoopMain[playerID], 50);

            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -v.P_AngleCos[playerID], -v.P_AngleSin[playerID]);
            trg.Shape_Dot(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.Shape_Dot(playerID, 1, "Protoss Dark Archon", -v.P_AngleCos[playerID], -v.P_AngleSin[playerID]);

            trg.Table_Sin(playerID, 45 * v.P_LoopMain[playerID] + 45, 50);
            trg.Table_Cos(playerID, 45 * v.P_LoopMain[playerID] + 45, 50);

            trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.Shape_Dot(playerID, 1, "Kakaru (Twilight)", -v.P_AngleCos[playerID], -v.P_AngleSin[playerID]);

            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {         
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Shape_Square(playerID, 1, "Target", 50, 0);
            trg.Shape_Square(playerID, 1, "Target", 100, 0);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {         
            trg.Shape_Square(playerID, 1, "Target", 50, 50);
            trg.Shape_Square(playerID, 1, "Target", 100, 100);
            KillUnitAt(All, "Target", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {         
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 100);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 50);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 150, 0);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {         
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 50, 100);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 100, 50);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 50, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 100, 50);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 8)
         {         
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 50, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 50);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 50, 100);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 100, 50);

            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 9)
         {         
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 100, 150);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, 100);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 100, 150);
            trg.Shape_Square(playerID, 1, " Unit. Hoffnung 25000", 150, 100);

            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 10)
         {         
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 50, 100);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 50);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 100, 150);
            trg.Shape_Square(playerID, 1, "Kakaru (Twilight)", 150, 100);
            KillUnitAt(All, "Kakaru (Twilight)", "Anywhere", playerID);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 11)
         {       
            trg.Shape_Square(playerID, 1, "80 + 1n Tom Kazansky", 100, 150);
            trg.Shape_Square(playerID, 1, "80 + 1n Tom Kazansky", 150, 100);
            KillUnitAt(All, "80 + 1n Tom Kazansky", "Anywhere", playerID);

            trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 50, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 100, 50);

            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 50, 100);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 100, 50);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 100, 150);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 100);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 12)
         {
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         trg.SkillEnd();
      }
   }
}