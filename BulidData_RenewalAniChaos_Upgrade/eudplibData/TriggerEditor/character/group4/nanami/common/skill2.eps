import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            trg.Shape_Dot(playerID, 1, "Protoss Dark Templar", 100 - 50 * v.P_LoopMain[playerID], 100);
            trg.Shape_Dot(playerID, 1, "Protoss Dark Templar", -100 + 50 * v.P_LoopMain[playerID], -100);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", -100, 100 - 50 * v.P_LoopMain[playerID]);
            trg.Shape_Dot(playerID, 1, "40 + 1n Zealot", 100, -100 + 50 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 100, 100);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 100);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] == 0)
         {         
            trg.Shape_Square(playerID, 1, "Protoss Dark Templar", 50, 0);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {         
            trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 50, 50);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {         
            trg.Shape_Square(playerID, 1, "Protoss Dark Templar", 100, 0);
            KillUnitAt(All, "Protoss Dark Templar", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {         
            trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 100, 100);
            KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 50, 0);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 0);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 100, 0);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 0);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 6)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 50, 50);
            KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 50, 50);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 7)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         trg.SkillEnd();
      }
   }
}