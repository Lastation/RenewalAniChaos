function SkillList(playerID);

function main(playerID)
{
	v.P_HeroID[playerID] = 24;
	v.P_LocationID[playerID] = 185;
	v.P_UnitID[playerID] = 64;
	
	v.P_Ultimate1[playerID] = 650;
	v.P_Ultimate2[playerID] = 450;
	
	SkillList(playerID);
	commend.main(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			ACC.main(playerID);
			break;
		case 220:
			ACCCA.main(playerID);
			break;
		case 230:
			CCC.main(playerID);
			break;
		case 240:
			CCCAA.main(playerID);
			break;
		case 250:
			CCCAAAA.main(playerID);
			break;
		case 260:
			CCCAAAAO.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
		}
	}
}

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x1B- 나나미 치아키　\x04[ 단간론파 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x17Passive \x04초고교급 게이머 \x19[ 아군 마나 회복 증가 +5 ]");
		v.stb.printAt(4, "　　　\x18O \x04모두의 힘 \x19[ 1분 간 아군 공 방 증가 +5 ] \x053분");
		v.stb.printAt(5, "　　　\x04\x1FSSS \x04발상 \x19[ 만능 ]");
		v.stb.printAt(6, "　　　\x04\x1FACC+ CA \x04필살기 \x19[ 만능 / 자리고정 ]");
		v.stb.printAt(7, "　　　\x04\x1FCCC + CCC + AA \x04응원 \x19[ 공성 / CCC + CCC - 자리고정 / 15 x 15 아군 공 방 증가 +5 ] \x1F[1]");
