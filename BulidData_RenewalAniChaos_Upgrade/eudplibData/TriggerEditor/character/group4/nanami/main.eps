		v.stb.printAt(10, "　　　\x08AAA \x04격려 \x19[ 서포트 / 아군 궁게이지 +250 ] \x05", v.P_Ultimate2[playerID]);
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}