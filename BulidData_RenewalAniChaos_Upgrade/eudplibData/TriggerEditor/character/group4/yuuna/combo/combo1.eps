import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] == 0)
         {                        
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 3, 75);
            trg.Shape_NxNSquare(playerID, 1, " Unit. Hoffnung 25000", 3, 75);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Wraith", 5, 75);
            trg.Shape_NxNSquare(playerID, 1, " Unit. Hoffnung 25000", 5, 75);
            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
            KillUnitAt(All, " Unit. Hoffnung 25000", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 2)
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 3)
         {
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 150);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 75);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 0);
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, -75);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, 150);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, 75);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Guardian", 150, -75);
            KillUnitAt(All, "40 + 1n Guardian", "Anywhere", playerID);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 150, 150 - 75 * v.P_LoopMain[playerID]);
            KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);

            trg.Main_Wait(80);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 150, 150);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 150, 75);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 150, 0);
            trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 150, -75);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 150, 150);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 150, 75);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 150, 0);
            trg.Shape_Square(playerID, 1, "50 + 1n Tank", 150, -75);
            KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
            KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);

            trg.Main_Wait(80);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }


      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] == 0)
         {
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 100);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 50);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, 0);
            trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 100, -50);
            trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 100);
            trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 50);
            trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, 0);
            trg.Shape_Square(playerID, 1, "60 + 1n Archon", 100, -50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(160);

            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 1)
         {
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            trg.Shape_NxNSquare(playerID, 1, "40 + 1n Mojo", 3, 50);
            trg.Shape_NxNSquare(playerID, 1, "60 + 1n Archon", 3, 50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

            trg.Main_Wait(160);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);

         trg.SkillEnd();
      }
   }
}