function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 유우키 \x1B유우나　\x04[ 유우키 유우나는 용사다 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18O \x04만개 \x19[ 공 3 업 방 3 다운 / 최대 3 중첩 가능 / 사망 시 해제 ] \x051분");
		v.stb.printAt(4, "　　　\x04\x1FSSS + AS \x04인간 \x19[ 대인 / 순간딜 / AS - 자리고정 ]");
		v.stb.printAt(5, "　　　\x04\x1FCC + CS \x04강인함 \x19[ 공성 / 순간딜 / 자리고정 ]");
		v.stb.printAt(6, "　　　\x04\x1FCAAA + 100 Gas \x04희생 \x19[ 공성 / 지속딜 / 자리고정 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08AAAA \x04대만개 \x19[ 공성 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "　　　\x04+ \x08AAA \x04용사 펀치 \x19[ 만개 3 중첩 시 사용 가능 ] \x05", v.P_Ultimate2[playerID]);
		v.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] = 22;
	v.P_LocationID[playerID] = 181;
	v.P_UnitID[playerID] = 54;
	
	v.P_Ultimate1[playerID] = 650;
	v.P_Ultimate2[playerID] = 300;
	
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 120:
			SSSSS.main(playerID);
			break;
		case 210:
			CCC.main(playerID);
			break;
		case 220:
			CCCAA.main(playerID);
			break;
		case 230:
			CAAA.main(playerID);
			break;
		case 310:
			AAA.main(playerID);
			break;
		case 320:
			AAAAAA.main(playerID);
			break;
			
		}
	}
	
	// Commed Setting

}