import variable as v;
import func.trig as trg;

function main(playerID)
{
   if (v.P_WaitMain[playerID] == 0)
   {
      if (v.P_CountMain[playerID] == 0)
      {
         if (v.P_LoopMain[playerID] < 4)
         {                        
            trg.Shape_Square(playerID, 1, "60 + 1n Archon", 200 - v.P_LoopMain[playerID] * 50, 0);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
            
            trg.Main_Wait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            trg.Shape_Square(playerID, 4, "60 + 1n Archon", 50, 50);
            KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);

            trg.Main_Wait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 5)
         {
            trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 75);
            Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, "Anywhere");

            trg.Main_Wait(0);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }

      }
      else if (v.P_CountMain[playerID] == 1)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            v.P_Distance[playerID] = 125;
            
            trg.Table_Cos(playerID, 45 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);
            trg.Table_Sin(playerID, 45 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);

            trg.MoveLoc(v.P_UnitID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.SkillUnit(playerID, 1, "40 + 1n Wraith");
            trg.SkillUnit(playerID, 1, "40 + 1n Goliath");

            v.P_Distance[playerID] = 175;

            trg.Table_Cos(playerID, 45 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);
            trg.Table_Sin(playerID, 45 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);

            trg.MoveLoc(v.P_UnitID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.SkillUnit(playerID, 1, "40 + 1n Wraith");
            trg.SkillUnit(playerID, 1, "40 + 1n Goliath");

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Main_Wait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);

            trg.Main_Wait(0);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 2)
      {
         if (v.P_LoopMain[playerID] < 4)
         {         
            v.P_Distance[playerID] = 100;
            
            trg.Table_Cos(playerID, 0 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);
            trg.Table_Sin(playerID, 0 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);

            trg.MoveLoc(v.P_UnitID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.SkillUnit(playerID, 1, "40 + 1n Wraith");
            trg.SkillUnit(playerID, 1, "40 + 1n Goliath");

            v.P_Distance[playerID] = 150;

            trg.Table_Cos(playerID, 0 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);
            trg.Table_Sin(playerID, 0 + 90 * v.P_LoopMain[playerID], v.P_Distance[playerID]);

            trg.MoveLoc(v.P_UnitID[playerID], playerID, v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
            trg.SkillUnit(playerID, 1, "40 + 1n Wraith");
            trg.SkillUnit(playerID, 1, "40 + 1n Goliath");

            KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);

            trg.Main_Wait(50);
            v.P_LoopMain[playerID] += 1;
         }
         else if (v.P_LoopMain[playerID] == 4)
         {
            MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
            MoveUnit(All, "40 + 1n Goliath", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
            Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);

            trg.Main_Wait(50);

            v.P_CountMain[playerID] += 1;
            v.P_LoopMain[playerID] = 0;
         }
      }
      else if (v.P_CountMain[playerID] == 3)
      {
         KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
         KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
         trg.SkillEnd();
      }
   }
}