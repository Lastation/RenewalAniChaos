import PluginVariables as msqcvar;
import func.trig as trg;
import variable as v;
import system.text as text;

import character.group1.rusaruka.main 	as rusaruka;
import character.group1.chtholly.main 	as chtholly;
import character.group1.maihime.main 	as maihime;

import character.group3.kurumi.main 	as kurumi;

import character.group4.yuuna.main 		as yuuna;
import character.group4.mayuri.main 	as mayuri;
import character.group4.nanami.main 	as nanami;
import character.group4.milim.main 		as milim;

import character.group5.yume.main 		as yume;
import character.group5.magane.main 	as magane;
import character.group5.oda.main 		as oda;
import character.group5.ekidona.main 	as ekidona;
import character.group5.niwa.main 		as niwa;

import character.group6.park.main 		as park;
import character.group6.hodaka.main 	as hodaka;


function main(playerID)
{
	trg.Main_WaitLoop();
	
	text.BuildingText(playerID);
	text.ShopText(playerID);
	
	if 		(v.P_HeroID[playerID] == 1) 		rusaruka.main(playerID);
	else if 	(v.P_HeroID[playerID] == 2) 		chtholly.main(playerID);
	else if 	(v.P_HeroID[playerID] == 3) 		maihime.main(playerID);
	
	else if 	(v.P_HeroID[playerID] == 12) 	kurumi.main(playerID);
	
	else if 	(v.P_HeroID[playerID] == 22) 	yuuna.main(playerID);
	else if 	(v.P_HeroID[playerID] == 23) 	mayuri.main(playerID);
	else if 	(v.P_HeroID[playerID] == 24) 	nanami.main(playerID);
	else if 	(v.P_HeroID[playerID] == 25) 	milim.main(playerID);
	
	else if 	(v.P_HeroID[playerID] == 26) 	yume.main(playerID);
	else if 	(v.P_HeroID[playerID] == 27) 	magane.main(playerID);
	else if 	(v.P_HeroID[playerID] == 28) 	oda.main(playerID);
	else if 	(v.P_HeroID[playerID] == 29) 	ekidona.main(playerID);
	else if 	(v.P_HeroID[playerID] == 30) 	niwa.main(playerID);
	
	else if 	(v.P_HeroID[playerID] == 31) 	park.main(playerID);
	else if 	(v.P_HeroID[playerID] == 32) 	hodaka.main(playerID);
	
	SetDeaths(playerID, SetTo, v.P_Shield[playerID], " `SystemShield");
}