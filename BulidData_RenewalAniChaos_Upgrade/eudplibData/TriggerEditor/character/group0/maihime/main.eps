import PluginVariables as msqcvar;
import variable as v;

import character.group0.maihime.commend as commend;

import character.group0.maihime.common.skill1 as S;
import character.group0.maihime.common.skill2 as C;
import character.group0.maihime.common.skill3 as A;

import character.group0.maihime.combo.combo1 as SSS;
import character.group0.maihime.combo.combo2 as CC;
import character.group0.maihime.combo.combo3 as CCAA;
import character.group0.maihime.combo.combo4 as AACC;

import character.group0.maihime.ultimate.ultimate1 as AAA;
import character.group0.maihime.ultimate.ultimate2 as AAAAA;

function skillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 텐카와 \x1B마이히메　\x04[ 퀄리디아 코드 ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List");
		v.stb.printAt(3, "　　　\x18 O\x04 연설 \x19[ 자신포함 팀 + 200 gas ] \x05개인쿨 1분");
		v.stb.printAt(4, "　　　\x1F SSS\x04 강하닷! \x19[ 대인 ]");
		v.stb.printAt(5, "　　　\x1F CC + AAS\x04 힘과 파워! \x19[ 대인 / 분산딜 ]");
		v.stb.printAt(6, "　　　\x1F AACC + 300 Gas\x04 영역 \x19[ 대인 / 가두기 / 자리고정 / 쉴드 3고정 / 지속딜 / 컴퓨터 동맹 ]");
		v.stb.printAt(7, "\n");
		v.stb.printAt(8, "　　　\x08 AAA \x04 살의\x19[ 대인 / 공성 / 즉발 / 쉴드 20고정 ] \x1F[1] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(9, "　　　\x1F [1] + \x08AA \x19[ 공성 / 즉발 / 자리고정 / 쉴드 1고정 ] \x05", v.P_Ultimate2[playerID]);
		v.stb.printAt(10, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 3;
	v.P_UnitID[playerID] 			= 74;
	v.P_LocationID[playerID] 		= 121;
	
	v.P_Ultimate1[playerID] = 500;
	v.P_Ultimate2[playerID] = 250;
	
	commend.main(playerID);
	skillList(playerID);
	
	// Commed Setting
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 100:
			S.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 210:
			CC.main(playerID);
			break;
		case 220:
			CCAA.main(playerID);
			break;
		case 310:
			AACC.main(playerID);
			break;
		case 320:
			AAA.main(playerID);
			break;
		case 330:
			AAAAA.main(playerID);
			break;
		}
	}
}