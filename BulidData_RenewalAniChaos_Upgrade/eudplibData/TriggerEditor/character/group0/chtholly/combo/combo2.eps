import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

function main(playerID)
{
	MoveUnit(All, "40 + 1n Marine", playerID, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "40 + 1n Mojo", playerID, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "50 + 1n Tank", playerID, "Anywhere", "[Skill]HoldPosition");
	MoveUnit(All, "60 + 1n Danimoth", playerID, "Anywhere", "[Skill]HoldPosition");
	trg.Debuff_BanReturn();

	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] <= 6)
		{
			trg.Shape_Square(playerID, 1, "80 + 1n Tank", 16 * (v.P_LoopMain[playerID] + 1), 16 * (v.P_LoopMain[playerID] + 1));
			trg.Shape_Square(playerID, 1, "Zerg Devourer", 16 * (v.P_LoopMain[playerID] + 1), 16 * (v.P_LoopMain[playerID] + 1));
			KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 7)
		{
			trg.Shape_Square(playerID, 1, "50 + 1n Tank", 16 * (v.P_LoopMain[playerID] + 1), 16 * (v.P_LoopMain[playerID] + 1));
			trg.Shape_Square(playerID, 1, "Zerg Devourer", 16 * (v.P_LoopMain[playerID] + 1), 16 * (v.P_LoopMain[playerID] + 1));
			KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] <= 7)
		{
			trg.Shape_Square(playerID, 1, "80 + 1n Tank", 20 * (v.P_LoopMain[playerID] + 1), 0);
			trg.Shape_Square(playerID, 1, "Zerg Devourer", 20 * (v.P_LoopMain[playerID] + 1), 0);
			KillUnitAt(All, "80 + 1n Tank", "Anywhere", playerID);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] == 8)
		{
			trg.Shape_Square(playerID, 1, "50 + 1n Tank", 20 * (v.P_LoopMain[playerID] + 1), 0);
			trg.Shape_Square(playerID, 1, "Zerg Devourer",  20 * (v.P_LoopMain[playerID] + 1), 0);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			MoveUnit(All, "50 + 1n Tank", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
			Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			KillUnitAt(All, "Zerg Devourer", "Anywhere", playerID);
			trg.Main_Wait(300);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 64, 64);
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 90, 0);
			trg.Shape_Square(playerID, 8, "40 + 1n Lurker",  64, 64);
			trg.Shape_Square(playerID, 8, "40 + 1n Lurker",  90, 0);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			MoveUnit(All, "40 + 1n Marine", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			KillUnitAt(All, "40 + 1n Lurker", "Anywhere", playerID);
			trg.Main_Wait(3500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] <= 3)
		{
			trg.Shape_Square(playerID, 8, "60 + 1n High Templar", 32 * (v.P_LoopMain[playerID] + 1), 32 * (v.P_LoopMain[playerID] + 1));
			trg.Shape_Square(playerID, 8, "60 + 1n High Templar", 45 * (v.P_LoopMain[playerID] + 1), 0);
			KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
			trg.Main_Wait(100);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] == 4)
		{
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 64, 64);
			trg.Shape_Square(playerID, 8, "Torrasque", 90, 0);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			KillUnitAt(All, "Torrasque", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] == 5)
		{
			s.CharacterVoice(2);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 180, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 128, 128);
			trg.Shape_Square(playerID, 8, "Torrasque", 128, 128);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("60 + 1n Danimoth", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			KillUnitAt(All, "Torrasque", "Anywhere", playerID);
			trg.Main_Wait(2750);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 4 
		&& Bring(playerID, AtLeast, 1, "Protoss Scout", "[Skill]UseSkill")
		&& Bring(playerID, AtLeast, 1, "Protoss Arbiter", "[Skill]UseSkill"))
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			
			s.CharacterVoice(3);
			KillUnitAt(1, "Protoss Arbiter", "[Skill]UseSkill", playerID);
			KillUnitAt(1, "Protoss Scout", "[Skill]UseSkill", playerID);
			
			v.P_Step[playerID] = 220;
			v.P_CountMain[playerID] = 0;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.SkillEnd();
		}
	}
}