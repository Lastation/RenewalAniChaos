import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{			
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 64, 128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 128, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -64, -128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -128, -64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 64, 128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 128, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -64, -128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -128, -64);
				
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", 128, 192);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", 192, 128);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", -128, -192);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", -192, -128);
				KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
				
			
				
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 64, -128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 128, -64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -64, 128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -128, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 64, -128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 128, -64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -64, 128);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -128, 64);
				
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", 128, -192);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", 192, -128);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", -128, 192);
				trg.Shape_Dot(playerID, 1, "60 + 1n Archon", -192, 128);
				KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
				
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 0);
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 64, 64);
				
				trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 128, 0);
				trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 64, 64);
				KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
				
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(50);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.SkillEnd();
			}
			v.P_LoopMain[playerID] += 1;
		}
	}
}