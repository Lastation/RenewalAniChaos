import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All, "80 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
			KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
			
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 144, 0);
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 96, 48);
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 48, 96);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				trg.Main_Wait(0);
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 48, 48);
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 48, 48);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("50 + 1n Battlecruiser", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(230);
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Square(playerID, 1, "80 + 1n Artanis", 64, 32);
				trg.Shape_Square(playerID, 1, "80 + 1n Artanis", 32, 64);
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 64, 32);
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 32, 64);
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 128, 64);
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 64, 128);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("80 + 1n Artanis", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(50);
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_Square(playerID, 1, "80 + 1n Goliath", 128, 64);
				trg.Shape_Square(playerID, 1, "80 + 1n Goliath", 64, 128);
				trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 128, 64);
				trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 64, 128);
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				KillUnitAt(All, "80 + 1n Goliath", "Anywhere", playerID);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				trg.Main_Wait(100);
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.SkillEnd();
			}
			
			v.P_LoopMain[playerID] += 1;
		}
	}
}