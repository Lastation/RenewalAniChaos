import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_Square(playerID, 1, "60 + 1n Dragoon", 32, 0);
				KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_Square(playerID, 1, "60 + 1n Dragoon", 32, 32);
				KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 64, 64);
				KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 64, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", -64, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -64, -64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 64, -64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				
				trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 64, 0);
				KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", 64, 0);
				trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", -0, 64);
				trg.Shape_Dot(playerID, 1, "40 + 1n Wraith", -64, -0);
				trg.Shape_Dot(playerID, 1, "40 + 1n Mojo", 0, -64);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
				KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
				
				trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 46, 80);
				trg.Shape_Dot(playerID, 1, "60 + 1n Danimoth", 92, 0);
				trg.Shape_Dot(playerID, 1, "60 + 1n Danimoth", -92, 0);
				KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
				
				trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 46, 80);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", 92, 0);
				trg.Shape_Dot(playerID, 1, "40 + 1n Goliath", -92, 0);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(100);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
				trg.SkillEnd();
			}
		}
	}
}