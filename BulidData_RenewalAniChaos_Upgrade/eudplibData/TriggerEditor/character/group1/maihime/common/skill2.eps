import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			
			if (v.P_LoopMain[playerID] <= 4)
			{
				if (v.P_LoopMain[playerID] % 2 == 0) { trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 48, 48); 	}
				else 								{ trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 48, 0); 	}
				KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 96, 0);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 6)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 96, 0);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 7)
			{
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 96, 48);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 8)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 96, 48);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(60);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 9)
			{
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 96, 96);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 10)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 96, 96);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(60);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 11)
			{
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 48, 96);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 12)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 48, 96);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(60);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 13)
			{
				trg.Shape_Square(playerID, 1, "Protoss Dark Archon", 96, 0);
				KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 14)
			{
				trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 96, 0);
				trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 96, 0);
				trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
				Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
				trg.Main_Wait(130);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 15)
			{
				trg.SkillEnd();
			}
		}
	}
}