import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{	
	if (v.P_WaitMain[playerID] == 0)
	{
		if(v.P_CountMain[playerID] == 0 || v.P_CountMain[playerID] == 5 || v.P_CountMain[playerID] == 10)
		{
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 128, 128);
			trg.Shape_Square(playerID, 1, "40 + 1n Zergling", 64, 64);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Zergling", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 1 || v.P_CountMain[playerID] == 6 || v.P_CountMain[playerID] == 11)
		{
			trg.Shape_Square(playerID, 1, "Kakaru", 64, 0);
			trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 128, 0);
			KillUnitAt(All, "Kakaru", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 2 || v.P_CountMain[playerID] == 7 || v.P_CountMain[playerID] == 12)
		{
			trg.Shape_Square(playerID, 1, "40 + 1n Wraith", 64, 64);
			trg.Shape_Square(playerID, 1, "50 + 1n Tank", 64, 64);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(60);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 3 || v.P_CountMain[playerID] == 8 || v.P_CountMain[playerID] == 13)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 128, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Archon", 128, 0);
			KillUnitAt(All, "60 + 1n Archon", "Anywhere", playerID);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(50);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 4 || v.P_CountMain[playerID] == 9 || v.P_CountMain[playerID] == 14)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			
			
			trg.Shape_Square(playerID, 1, "50 + 1n Battlecruiser", 64, 0);
			trg.Shape_Square(playerID, 1, "Torrasque", 64, 0);
			KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
			KillUnitAt(All, "Torrasque", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 15)
		{
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			trg.SkillUnit(playerID, 1, "40 + 1n Mutalisk");
			trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 32, 0);
			trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 64, 0);
			trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 96, 0);
			trg.Shape_Square(playerID, 1, "40 + 1n Mutalisk", 128, 0);
			trg.Shape_Square(playerID, 1, "Torrasque", 32, 32);
			trg.Shape_Square(playerID, 1, "Torrasque", 64, 64);
			trg.Shape_Square(playerID, 1, "Torrasque", 96, 64);
			KillUnitAt(All, "Torrasque", "Anywhere", playerID);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(300);
			v.P_CountMain[playerID] += 1;
		}
		else if(v.P_CountMain[playerID] == 16)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
			trg.SkillEnd();
		}
	}
}