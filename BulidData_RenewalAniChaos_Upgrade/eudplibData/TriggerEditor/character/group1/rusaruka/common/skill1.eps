import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;
import func.sound as s;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (playerID < 3)
		{ MoveUnit(All, v.P_UnitID[playerID], playerID, "Anywhere", "[Unique]Position_Team1"); }
		else
		{ MoveUnit(All, v.P_UnitID[playerID], playerID, "Anywhere", "[Unique]Position_Team2"); }
	
		if (v.P_CountMain[playerID] == 0)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				SetInvincibility(Enable, v.P_UnitID[playerID], playerID, "Anywhere");
				s.CharacterVoice(7);
				trg.Main_Wait(5800);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				s.CharacterVoice(8);
				trg.Main_Wait(5600);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				s.CharacterVoice(9);
				trg.Main_Wait(6100);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				s.CharacterVoice(10);
				trg.Main_Wait(6200);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				s.CharacterVoice(11);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				v.P_LoopMain[playerID] = 0;
				v.P_CountMain[playerID] += 1;
			}
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			if (v.P_LoopMain[playerID] == 0)
			{
				trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 1, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 1)
			{
				trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 3, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 2)
			{
				trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 5, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 3)
			{
				trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 7, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 4)
			{
				trg.Shape_NxNSquare(playerID, 1, "50 + 1n Battlecruiser", 9, 64);
				KillUnitAt(All, "50 + 1n Battlecruiser", "Anywhere", playerID);
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 5)
			{
				if (playerID < 3)
				{ 
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P7);
					MoveUnit(1, v.P_UnitID[playerID], playerID, "Anywhere", "[Potal]Base7");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P7, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team1");
					MoveUnit(1, v.P_UnitID[playerID], playerID, "Anywhere", "[Unique]Position_Team1");
				}
				else
				{
					CreateUnit(1, "Csejte Ungarn Nachtzehrer", "[Skill]Unit_Wait_8", P8);
					MoveUnit(1, v.P_UnitID[playerID], playerID, "Anywhere", "[Potal]Base8");
					MoveUnit(1, "Csejte Ungarn Nachtzehrer", P8, "[Skill]Unit_Wait_ALL", "[Unique]Position_Team2");
					MoveUnit(1, v.P_UnitID[playerID], playerID, "Anywhere", "[Unique]Position_Team2");
				}
				trg.Main_Wait(0);
				v.P_LoopMain[playerID] += 1;
			}
			else if (v.P_LoopMain[playerID] == 6)
			{
				SetInvincibility(Disable, v.P_UnitID[playerID], playerID, "Anywhere");
				trg.SkillEnd();
			}
		}
	}
}