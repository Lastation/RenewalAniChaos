import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			trg.Shape_Cross(playerID, 1, "40 + 1n Wraith", 45, 3, 64);
			trg.Shape_Cross(playerID, 1,"60 + 1n Dragoon", 45, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
		
			trg.Shape_Line(playerID, 1, "40 + 1n Mojo", 45, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "40 + 1n Mojo", 225, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 45, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 225, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
		
			trg.Shape_Line(playerID, 1, "40 + 1n Mojo", 45, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "40 + 1n Mojo", 225, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 45, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 225, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
		
			trg.Shape_Line(playerID, 1, "40 + 1n Mutalisk", 135, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "40 + 1n Mutalisk", 315, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 135, 3, 64, 64);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 315, 3, 64, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
		
			trg.Shape_Line(playerID, 1, "40 + 1n Mutalisk", 135, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "40 + 1n Mutalisk", 315, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 135, 1, 64, 128);
			trg.Shape_Line(playerID, 1, "60 + 1n Dragoon", 315, 1, 64, 128);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mutalisk", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		if (v.P_CountMain[playerID] == 5)
		{
			KillUnitAt(All, "40 + 1n Mutalisk", "Anywhere", playerID);
			
			trg.Shape_Cross(playerID, 1, "40 + 1n Wraith", 0, 3, 64);
			trg.Shape_Cross(playerID, 1,"60 + 1n Dragoon", 0, 3, 64);
			KillUnitAt(All, "60 + 1n Dragoon", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Wraith", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(90);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 6)
		{
			KillUnitAt(All, "40 + 1n Wraith", "Anywhere", playerID);
			trg.SkillEnd();
		}
	}
}