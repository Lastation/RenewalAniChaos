import PluginVariables as msqcvar;
import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0)
		{
			trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 64, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 64, 0);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(200);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1)
		{
			RemoveUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
		
			trg.Shape_Square(playerID, 1, "40 + 1n Goliath", 32, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 32, 0);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(200);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			RemoveUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 64, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 64, 0);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(100);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3)
		{
			RemoveUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 32, 0);
			trg.Shape_Square(playerID, 1, "60 + 1n Danimoth", 32, 0);
			KillUnitAt(All, "60 + 1n Danimoth", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(200);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			RemoveUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 32, 32);
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 64, 64);
			trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 32, 32);
			trg.Shape_Square(playerID, 1, "60 + 1n High Templar", 64, 64);
			KillUnitAt(All, "60 + 1n High Templar", "Anywhere", playerID);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(200);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 5)
		{
			RemoveUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			trg.SkillEnd();
		}
	}
}