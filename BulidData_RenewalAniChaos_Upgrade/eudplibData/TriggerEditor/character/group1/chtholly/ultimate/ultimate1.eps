import variable as v;
import func.trig as trg;
import func.sound as s;

function main(playerID)
{
	MoveLocation("[Unique]Chtholly", v.P_UnitID[playerID], playerID, "Anywhere");
	trg.Debuff_BanReturn();
	trg.Buff_ShieldFix(10);
	
	if (v.P_CountMain[playerID] > 2)
	{
		s.CharacterVoice(14);
	}
	
	if (v.P_CountMain[playerID] < 6)
	{
		Order("80 + 1n Artanis", playerID, "Anywhere", Move, v.P_LocationID[playerID]);
	}

	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 0)
		{
			trg.Main_Wait(1000);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 1)
		{
			s.CharacterVoice(12);
			trg.Main_Wait(3500);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 0 && v.P_LoopMain[playerID] == 2)
		{
			s.CharacterVoice(13);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] <= 6)
		{
			trg.Shape_NxNSquare(playerID, 1, "40 + 1n Gantrithor", (v.P_LoopMain[playerID] + 1), 64);
			KillUnitAt(All, "40 + 1n Gantrithor", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 1 && v.P_LoopMain[playerID] == 7)
		{
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 2)
		{
			trg.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 0);
			trg.Shape_Square(playerID, 6, "50 + 1n Tank", 96, 192);
			trg.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 96);
			trg.Shape_Square(playerID, 6, "50 + 1n Tank", 192, 192);
			trg.Shape_Square(playerID, 4, "40 + 1n Goliath", 192, 0);
			trg.Shape_Square(playerID, 4, "40 + 1n Goliath", 192, 192);
			trg.Shape_Square(playerID, 4, "40 + 1n Marine", 96, 192);
			trg.Shape_Square(playerID, 4, "40 + 1n Marine", 192, 96);
			trg.Shape_Square(playerID, 4, "40 + 1n Marine", 192, 192);
			trg.Shape_Dot(playerID, 12, "40 + 1n Marine", 0, 0);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			MoveUnit(All, "(any unit)", playerID, "[Skill]Unit_Wait_ALL", v.P_LocationID[playerID]);
			Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			Order("40 + 1n Goliath", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] <= 27)
		{
			trg.Table_Cos(playerID, 12 * v.P_LoopMain[playerID], 256);
			trg.Table_Sin(playerID, 12 * v.P_LoopMain[playerID], 256);
			trg.Shape_Square(playerID, 1, "80 + 1n Artanis", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			trg.Shape_Square(playerID, 1, "Protoss Dark Archon", v.P_AngleCos[playerID], v.P_AngleSin[playerID]);
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("80 + 1n Artanis", playerID, "Anywhere", Move, v.P_LocationID[playerID]);
			KillUnitAt(All, "Protoss Dark Archon", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_LoopMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 3 && v.P_LoopMain[playerID] == 28)
		{
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
			v.P_LoopMain[playerID] = 0;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			trg.Main_Wait(1500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 5)
		{
			if (playerID < 3)	{ GiveUnits(All, "80 + 1n Artanis", playerID, "Anywhere", 6); }
			else 			{ GiveUnits(All, "80 + 1n Artanis", playerID, "Anywhere", 7); }
			SetSwitch("JunkYardDog", Set);
			trg.Main_Wait(250);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 6)
		{
			SetSwitch("JunkYardDog", Clear);
			trg.Main_Wait(1500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 7)
		{
			SetSwitch("JunkYardDog", Set);
			trg.Main_Wait(250);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 8)
		{
			SetSwitch("JunkYardDog", Clear);
			trg.Main_Wait(1000);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 9)
		{
			if (playerID < 3)	{ GiveUnits(All, "80 + 1n Artanis", 6, "Anywhere", playerID); }
			else 			{ GiveUnits(All, "80 + 1n Artanis", 7, "Anywhere", playerID); }
			
			if (v.P_LoopSub1[playerID] == 1)
			{ 
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 6);
				KillUnitAt(All, "80 + 1n Artanis", "Anywhere", 7);
			}
			
			trg.MoveLoc(v.P_UnitID[playerID], playerID, 0, 0);
			Order("80 + 1n Artanis", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(3500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 10)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			KillUnitAt(All, "40 + 1n Goliath", "Anywhere", playerID);
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			trg.Main_Wait(800);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 11)
		{
			KillUnitAt(All, "80 + 1n Artanis", "Anywhere", playerID);
			SetSwitch("UiltimateSwitch", Clear);
			SetSwitch("Recall - Chtholly", Clear);
			v.P_LoopSub1[playerID] = 0;
			trg.SkillEnd();
		}
	}
}