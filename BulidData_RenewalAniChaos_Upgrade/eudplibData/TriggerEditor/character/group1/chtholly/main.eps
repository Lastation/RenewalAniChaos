import variable as v;

import character.group1.chtholly.commend as commend;

import character.group1.chtholly.common.skill1 as S;
import character.group1.chtholly.common.skill2 as C;
import character.group1.chtholly.common.skill3 as A;

import character.group1.chtholly.combo.combo1 as SSS;
import character.group1.chtholly.combo.combo2 as CCA;
import character.group1.chtholly.combo.combo3 as CCACC;
import character.group1.chtholly.combo.combo4 as CCACCAAA;

import character.group1.chtholly.ultimate.ultimate1 as Uiltimate;

function SkillList(playerID)
{
	if(Memory(0x00596A44, Exactly, 256)) 	// Insert key Pressed
	{
		v.stb.printAt(0, "\n");
		v.stb.printAt(1, "\x1F　＃\x04- 크톨리 \x1B노타 세니오리스　\x04[ 종말에 뭐 하세요? 바쁘세요? 구해 주실 수 있나요? ]");
		v.stb.printAt(2, "　　\x1FA\x04ction List\r\n　　　\x07O \x19 전투속행 ON/OFF [ 침식 도중 사망시 5초간 전투 속행 ] \x05개인쿨 4분");
		v.stb.printAt(3, "　　　\x1FSSS \x04어쩔수 없지;; \x19[ 공성 ]");
		v.stb.printAt(4, "　　　\x1FCCA + CC + AAA \x04침식 \x19[ 공성 / AAA - 자리고정 / 귀환봉인 ] \x1F[1]");
		v.stb.printAt(5, "\n");
		v.stb.printAt(6, "　　　\x1F[1] + \x08O \x04세상에서 가장 행복한 여자 \x19[ 공성 / 선딜 4.5초 / 장판형 / 쉴드 10고정 ] \x05", v.P_Ultimate1[playerID]);
		v.stb.printAt(7, "　　　\x08AAAA \x04세상에서 가장 행복한 여자 \x19[ 공성 / 선딜 4.5초 / 장판형 / 쉴드 10고정 ] \x05", v.P_Ultimate2[playerID]);
		v.stb.printAt(8, "　　　\x04 * \x19AAAA\x04사용시 \x19O\x04 사용시보다 데미지가 많이 약해집니다.");
		v.stb.printAt(9, "\n");
		PlayWAV("sound\\Bullet\\LaserHit.wav");
	}
}

function main(playerID)
{
	v.P_HeroID[playerID] 		= 2;
	v.P_LocationID[playerID] 		= 117;
	v.P_UnitID[playerID] 			= 77;
	
	v.P_Ultimate1[playerID] = 700;
	v.P_Ultimate2[playerID] = 700;
	
	// Commed Setting
	commend.main(playerID);
	SkillList(playerID);
	
	if (v.P_SkillDelay[playerID] == 0)
	{
		switch(v.P_Step[playerID])
		{
		case 1:
			//O.main(playerID);
			break;
		case 100:
			S.main(playerID);
			break;
		case 110:
			SSS.main(playerID);
			break;
		case 200:
			C.main(playerID);
			break;
		case 210:
			CCA.main(playerID);
			break;
		case 220:
			CCACC.main(playerID);
			break;
		case 230:
			CCACCAAA.main(playerID);
			break;
		case 240:
			Uiltimate.main(playerID);
			break;
		case 300:
			A.main(playerID);
			break;
		}
	}
	
}