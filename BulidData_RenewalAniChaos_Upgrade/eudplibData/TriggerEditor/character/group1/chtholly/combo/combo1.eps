import variable as v;
import func.trig as trg;

function main(playerID)
{
	if (v.P_WaitMain[playerID] == 0)
	{
		if (v.P_CountMain[playerID] < 4)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
		
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 16 * v.P_CountMain[playerID] + 32, 16 * v.P_CountMain[playerID] + 32);
			trg.Shape_Square(playerID, 1, "40 + 1n Zealot", 16 * v.P_CountMain[playerID] + 32, 16 * v.P_CountMain[playerID] + 32);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			KillUnitAt(All, "40 + 1n Zealot", "Anywhere", playerID);
			Order("40 + 1n Mojo", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(150);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 4)
		{
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			trg.Main_Wait(0);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 5 || v.P_CountMain[playerID] == 6)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "40 + 1n Mojo", 116 - 32 * (v.P_CountMain[playerID] - 5), 0);
			trg.Shape_Square(playerID, 1, "40 + 1n Marine", 116 - 32 * (v.P_CountMain[playerID] - 5), 0);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			KillUnitAt(All, "40 + 1n Mojo", "Anywhere", playerID);
			Order("40 + 1n Marine", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(500);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 7)
		{
			KillUnitAt(All, "40 + 1n Marine", "Anywhere", playerID);
			
			trg.Shape_Square(playerID, 1, "50 + 1n Tank", 90, 90);
			trg.Shape_Square(playerID, 1, "50 + 1n Tank", 128, 0);
			MoveLocation(v.P_LocationID[playerID], v.P_UnitID[playerID], playerID, "Anywhere");
			Order("50 + 1n Tank", playerID, "Anywhere", Attack, v.P_LocationID[playerID]);
			trg.Main_Wait(740);
			v.P_CountMain[playerID] += 1;
		}
		else if (v.P_CountMain[playerID] == 8)
		{
			KillUnitAt(All, "50 + 1n Tank", "Anywhere", playerID);
			trg.SkillEnd();
		}
	}
}